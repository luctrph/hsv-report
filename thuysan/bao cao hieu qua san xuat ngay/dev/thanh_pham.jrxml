	<parameter name="AD_CLIENT_ID" class="java.lang.Integer"/>
	<parameter name="StartDate" class="java.util.Date"/>
	<parameter name="EndDate" class="java.util.Date"/>
	<queryString>
		<![CDATA[WITH params AS (
    SELECT 
        $P{StartDate}::date AS start_date,
        $P{EndDate}::date AS end_date,
        date_trunc('month', $P{EndDate}::date)::date AS month_start
),
dongia AS (
    SELECT 
        dgl.m_product_id,
        dgl.hsv_dinhmuc,
        dg.startdate,
        dg.enddate
    FROM hsv_dongia dg
    JOIN hsv_nkdongialine dgl ON dgl.hsv_dongia_id = dg.hsv_dongia_id
    WHERE dg.hsv_loaidongia_id = 1000058
      AND dg.ad_client_id = $P{AD_CLIENT_ID}
),
ducuoi AS (
    SELECT 
        m.m_product_id,
        SUM(m.movementqty) AS ducuoi
    FROM m_transaction m
    WHERE m.ad_client_id = $P{AD_CLIENT_ID}
      AND m.movementdate <= $P{EndDate}
      AND m.m_locator_id = 1000025
    GROUP BY m.m_product_id
    HAVING SUM(m.movementqty) <> 0
),
base_production AS (
    SELECT 
        ci.m_production_id,
        ci.movementdate,
        civ.m_product_id,
        civ.productionqty,
        civ.m_attributesetinstance_id,
        hh.m_attributeset_id,
        hh.value,
        k.hsv_dinhmuc
    FROM m_production ci
    JOIN m_productionplan civ ON ci.m_production_id = civ.m_production_id
    JOIN m_product hh ON hh.m_product_id = civ.m_product_id
    JOIN dongia k ON k.m_product_id = civ.m_product_id
    WHERE ci.ad_client_id = $P{AD_CLIENT_ID}
      AND ci.movementdate BETWEEN k.startdate AND k.enddate
      AND ci.c_campaign_id IN (1000018, 1000093)
      AND ci.docstatus IN ('CL', 'CO')
)
SELECT 
    id,
    noidung,
    SUM(sl) AS sl,
    SUM(lksl) AS lksl,
    SUM(tt) AS tt,
    SUM(lktt) AS lktt,
    tyle,
    SUM(sodu) AS sodu
FROM (
    -- 1: Ngao đơn hàng HCK, CN
    SELECT 
        1 AS id,
        'Ngao đơn hàng HCK,CN' AS noidung,
        SUM(CASE WHEN bp.movementdate BETWEEN p.start_date AND p.end_date THEN bp.productionqty ELSE 0 END) AS sl,
        SUM(CASE WHEN bp.movementdate BETWEEN p.month_start AND p.end_date THEN bp.productionqty ELSE 0 END) AS lksl,
        SUM(CASE WHEN bp.movementdate BETWEEN p.start_date AND p.end_date THEN bp.productionqty * bp.hsv_dinhmuc ELSE 0 END) AS tt,
        SUM(CASE WHEN bp.movementdate BETWEEN p.month_start AND p.end_date THEN bp.productionqty * bp.hsv_dinhmuc ELSE 0 END) AS lktt,
        1.035 AS tyle,
        dc.ducuoi AS sodu
    FROM base_production bp
    JOIN params p ON true
    LEFT JOIN ducuoi dc ON dc.m_product_id = bp.m_product_id
    WHERE bp.m_attributeset_id = 1000000
      AND EXISTS (
          SELECT 1
          FROM m_attributeinstance att
          WHERE att.m_attributesetinstance_id = bp.m_attributesetinstance_id
            AND att.m_attributevalue_id IN (1000012, 1000641)
      )
    GROUP BY dc.ducuoi

    UNION ALL

    -- 2: Ngao đơn hàng K.HCK: hàn túi 1 kg
    SELECT 
        2 AS id,
        'Ngao đơn hàng K.HCK: hàn túi 1 kg' AS noidung,
        SUM(CASE WHEN bp.movementdate BETWEEN p.start_date AND p.end_date THEN bp.productionqty ELSE 0 END) AS sl,
        SUM(CASE WHEN bp.movementdate BETWEEN p.month_start AND p.end_date THEN bp.productionqty ELSE 0 END) AS lksl,
        SUM(CASE WHEN bp.movementdate BETWEEN p.start_date AND p.end_date THEN bp.productionqty * bp.hsv_dinhmuc ELSE 0 END) AS tt,
        SUM(CASE WHEN bp.movementdate BETWEEN p.month_start AND p.end_date THEN bp.productionqty * bp.hsv_dinhmuc ELSE 0 END) AS lktt,
        1.035 AS tyle,
        NULL AS sodu
    FROM base_production bp
    JOIN params p ON true
    WHERE bp.m_attributeset_id = 1000000
      AND EXISTS (
          SELECT 1
          FROM m_attributeinstance att
          WHERE att.m_attributesetinstance_id = bp.m_attributesetinstance_id
            AND att.m_attributevalue_id = 1000013
      )
      AND EXISTS (
          SELECT 1
          FROM m_attributeinstance att2
          WHERE att2.m_attributesetinstance_id = bp.m_attributesetinstance_id
            AND att2.m_attributevalue_id = 1000009
      )
    GROUP BY bp.value

    UNION ALL

    -- 3: Ngao đơn hàng K.HCK: đóng bulk
    SELECT 
        3 AS id,
        'Ngao đơn hàng K.HCK: đóng bulk' AS noidung,
        SUM(CASE WHEN bp.movementdate BETWEEN p.start_date AND p.end_date THEN bp.productionqty ELSE 0 END) AS sl,
        SUM(CASE WHEN bp.movementdate BETWEEN p.month_start AND p.end_date THEN bp.productionqty ELSE 0 END) AS lksl,
        SUM(CASE WHEN bp.movementdate BETWEEN p.start_date AND p.end_date THEN bp.productionqty * bp.hsv_dinhmuc ELSE 0 END) AS tt,
        SUM(CASE WHEN bp.movementdate BETWEEN p.month_start AND p.end_date THEN bp.productionqty * bp.hsv_dinhmuc ELSE 0 END) AS lktt,
        1.09 AS tyle,
        NULL AS sodu
    FROM base_production bp
    JOIN params p ON true
    WHERE bp.m_attributeset_id = 1000000
      AND EXISTS (
          SELECT 1
          FROM m_attributeinstance ai
          WHERE ai.m_attributesetinstance_id = bp.m_attributesetinstance_id
            AND ai.m_attributevalue_id = 1000013
      )
      AND NOT EXISTS (
          SELECT 1
          FROM m_attributeinstance ai
          WHERE ai.m_attributesetinstance_id = bp.m_attributesetinstance_id
            AND ai.m_attributevalue_id = 1000009
      )
    GROUP BY bp.value

    UNION ALL

    -- 4: Ngao bulk dự trữ chưa có đơn hàng
    SELECT 
        4 AS id,
        'Ngao bulk dự trữ chưa có đơn hàng' AS noidung,
        SUM(CASE WHEN bp.movementdate BETWEEN p.start_date AND p.end_date THEN bp.productionqty ELSE 0 END) AS sl,
        SUM(CASE WHEN bp.movementdate BETWEEN p.month_start AND p.end_date THEN bp.productionqty ELSE 0 END) AS lksl,
        SUM(CASE WHEN bp.movementdate BETWEEN p.start_date AND p.end_date THEN bp.productionqty * bp.hsv_dinhmuc ELSE 0 END) AS tt,
        SUM(CASE WHEN bp.movementdate BETWEEN p.month_start AND p.end_date THEN bp.productionqty * bp.hsv_dinhmuc ELSE 0 END) AS lktt,
        1.09 AS tyle,
        dc.ducuoi AS sodu
    FROM base_production bp
    JOIN params p ON true
    LEFT JOIN ducuoi dc ON dc.m_product_id = bp.m_product_id
    WHERE bp.m_attributeset_id = 1000001
    GROUP BY dc.ducuoi

    UNION ALL

    -- 5: Ngao loại, chờ xử lý, cát, mùi ...
    SELECT 
        5 AS id,
        'Ngao loại, chờ xử lý, cát, mùi ...' AS noidung,
        SUM(CASE WHEN bp.movementdate BETWEEN p.start_date AND p.end_date THEN bp.productionqty ELSE 0 END) AS sl,
        SUM(CASE WHEN bp.movementdate BETWEEN p.month_start AND p.end_date THEN bp.productionqty ELSE 0 END) AS lksl,
        SUM(CASE WHEN bp.movementdate BETWEEN p.start_date AND p.end_date THEN bp.productionqty * bp.hsv_dinhmuc ELSE 0 END) AS tt,
        SUM(CASE WHEN bp.movementdate BETWEEN p.month_start AND p.end_date THEN bp.productionqty * bp.hsv_dinhmuc ELSE 0 END) AS lktt,
        1.09 AS tyle,
        dc.ducuoi AS sodu
    FROM base_production bp
    JOIN params p ON true
    LEFT JOIN ducuoi dc ON dc.m_product_id = bp.m_product_id
    WHERE bp.m_attributeset_id in (1000006,1000195)
      AND bp.m_product_id <> 1017801
    GROUP BY dc.ducuoi, bp.value

    UNION ALL

    -- 6: Nhập ngao chờ xử lý: do túi sì
    SELECT 
        6 AS id,
        'Nhập ngao chờ xử lý: do túi sì' AS noidung,
        SUM(CASE WHEN bp.movementdate BETWEEN p.start_date AND p.end_date THEN bp.productionqty ELSE 0 END) AS sl,
        SUM(CASE WHEN bp.movementdate BETWEEN p.month_start AND p.end_date THEN bp.productionqty ELSE 0 END) AS lksl,
        SUM(CASE WHEN bp.movementdate BETWEEN p.start_date AND p.end_date THEN bp.productionqty * bp.hsv_dinhmuc ELSE 0 END) AS tt,
        SUM(CASE WHEN bp.movementdate BETWEEN p.month_start AND p.end_date THEN bp.productionqty * bp.hsv_dinhmuc ELSE 0 END) AS lktt,
        1.09 AS tyle,
        dc.ducuoi AS sodu
    FROM base_production bp
    JOIN params p ON true
    LEFT JOIN ducuoi dc ON dc.m_product_id = bp.m_product_id
    WHERE bp.m_product_id = 1017801
    GROUP BY dc.ducuoi

    UNION ALL

    -- 7: Ngao mãnh thu được (Ngao mãnh)
    SELECT 
        7 AS id,
        'Ngao mãnh thu được (Ngao mãnh)' AS noidung,
        NULL AS sl,
        NULL AS lksl,
        SUM(CASE WHEN bp.movementdate BETWEEN p.start_date AND p.end_date THEN bp.productionqty * bp.hsv_dinhmuc ELSE 0 END) AS tt,
        SUM(CASE WHEN bp.movementdate BETWEEN p.month_start AND p.end_date THEN bp.productionqty * bp.hsv_dinhmuc ELSE 0 END) AS lktt,
        1.02 AS tyle,
        dc.ducuoi AS sodu
    FROM base_production bp
    JOIN params p ON true
    LEFT JOIN ducuoi dc ON dc.m_product_id = bp.m_product_id
    WHERE bp.m_attributeset_id IN (1000018, 1000031)
    GROUP BY dc.ducuoi, bp.value

    UNION ALL

    -- 8: Ngao thịt thu được (Ngao thịt)
    SELECT 
        8 AS id,
        'Ngao thịt thu được (Ngao thịt)' AS noidung,
        NULL AS sl,
        NULL AS lksl,
        SUM(CASE WHEN bp.movementdate BETWEEN p.start_date AND p.end_date THEN bp.productionqty * bp.hsv_dinhmuc ELSE 0 END) AS tt,
        SUM(CASE WHEN bp.movementdate BETWEEN p.month_start AND p.end_date THEN bp.productionqty * bp.hsv_dinhmuc ELSE 0 END) AS lktt,
        1.02 AS tyle,
        dc.ducuoi AS sodu
    FROM base_production bp
    JOIN params p ON true
    LEFT JOIN ducuoi dc ON dc.m_product_id = bp.m_product_id
    WHERE bp.m_attributeset_id IN (1000012, 1000062)
    GROUP BY dc.ducuoi, bp.value

    UNION ALL

    -- 9: Số lượng giảm do mạ băng (quy NET)
    SELECT 
        9 AS id,
        'Số lượng giảm do mạ băng (quy NET)' AS noidung,
        SUM(CASE WHEN bp.movementdate BETWEEN p.start_date AND p.end_date THEN 
            -1 * CASE att.m_attributevalue_id 
                WHEN 1000093 THEN 0.1
                WHEN 1000224 THEN 0.02
                WHEN 1000095 THEN 0.03
                WHEN 1000090 THEN 0.05
                ELSE 0
            END * bp.productionqty 
        END) AS sl,
        SUM(CASE WHEN bp.movementdate BETWEEN p.month_start AND p.end_date THEN 
            -1 * CASE att.m_attributevalue_id 
                WHEN 1000093 THEN 0.1
                WHEN 1000224 THEN 0.02
                WHEN 1000095 THEN 0.03
                WHEN 1000090 THEN 0.05
                ELSE 0
            END * bp.productionqty 
        END) AS lksl,
        SUM(CASE WHEN bp.movementdate BETWEEN p.start_date AND p.end_date THEN 
            -1 * CASE att.m_attributevalue_id 
                WHEN 1000093 THEN 0.1
                WHEN 1000224 THEN 0.02
                WHEN 1000095 THEN 0.03
                WHEN 1000090 THEN 0.05
                ELSE 0
            END * bp.productionqty * bp.hsv_dinhmuc 
        END) AS tt,
        SUM(CASE WHEN bp.movementdate BETWEEN p.month_start AND p.end_date THEN 
            -1 * CASE att.m_attributevalue_id 
                WHEN 1000093 THEN 0.1
                WHEN 1000224 THEN 0.02
                WHEN 1000095 THEN 0.03
                WHEN 1000090 THEN 0.05
                ELSE 0
            END * bp.productionqty * bp.hsv_dinhmuc 
        END) AS lktt,
        1.09 AS tyle,
        NULL AS sodu
    FROM base_production bp
    JOIN m_attributeinstance att ON att.m_attributesetinstance_id = bp.m_attributesetinstance_id
    JOIN params p ON true
    
) AS k
GROUP BY id, noidung, tyle
ORDER BY id;]]>
