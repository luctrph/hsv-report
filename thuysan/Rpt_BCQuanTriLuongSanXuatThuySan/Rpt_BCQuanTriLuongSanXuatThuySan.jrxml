						join m_attributevalue gtt on tt.m_attributevalue_id=gtt.m_attributevalue_id and hsv_c_thuoctinh_id=1000007
					 ) tt on pl.m_attributesetinstance_id=tt.m_attributesetinstance_id
				where  pl.ad_client_id=$P{AD_CLIENT_ID}  and p.c_campaign_id  in (1000093,1000094)			
				) as t				
				union all
				
				select -1*COALESCE(sum(io.qtyinternaluse),0)  as sl,'22' as chitieu	
		        from M_Inventoryline io 
		        join M_Inventory iol on io.M_Inventory_id=iol.M_Inventory_id and iol.docstatus in ('CO','CL') and iol.C_DocType_ID=1000167
						AND iol.movementdate>=$P{StartDate} and iol.movementdate<=$P{EndDate}
		        join m_product hh on io.m_product_id=hh.m_product_id and hh.m_product_category_id=1000017
		        join (select tt.m_attributesetinstance_id from m_attributeinstance tt
					       join m_attributevalue gtt on tt.m_attributevalue_id=gtt.m_attributevalue_id and hsv_c_thuoctinh_id=1000007
				        ) tt on io.m_attributesetinstance_id=tt.m_attributesetinstance_id
				where io.ad_client_id=$P{AD_CLIENT_ID} and iol.c_campaign_id  in (1000093,1000094)
				
		    ) as t

		union all

		select sum(t.sl) as sl,'25' as chitieu
		from(
			select COALESCE(sum(t.productionqty),0) as sl,'12' as chitieu
			from (
				select  pl.m_productionplan_id,pl.m_product_id,pl.productionqty,pl.m_attributesetinstance_id from m_productionplan pl
				join m_product hh on pl.m_product_id=hh.m_product_id and  hh.m_product_category_id=1000017 
				join m_production p on pl.m_production_id=p.m_production_id  and p.docstatus in ('CO','CL') AND P.movementdate>=$P{StartDate} and P.movementdate<=$P{EndDate}
				join (
						select tt.m_attributesetinstance_id 
						from m_attributeinstance tt
						join m_attributevalue gtt on tt.m_attributevalue_id=gtt.m_attributevalue_id and hsv_c_thuoctinh_id=1000019
				) tt on pl.m_attributesetinstance_id=tt.m_attributesetinstance_id
				where  pl.ad_client_id=$P{AD_CLIENT_ID} and p.c_campaign_id  in (1000093,1000094)
				
				) as t
				
				union all
				
				select -1*COALESCE(sum(io.qtyinternaluse),0)  as sl,'23' as chitieu	
		        from M_Inventoryline io 
		        join M_Inventory iol on io.M_Inventory_id=iol.M_Inventory_id and iol.docstatus in ('CO','CL') and iol.C_DocType_ID=1000167
						AND iol.movementdate>=$P{StartDate} and iol.movementdate<=$P{EndDate}
		        join m_product hh on io.m_product_id=hh.m_product_id and hh.m_product_category_id=1000017
		        join (select tt.m_attributesetinstance_id from m_attributeinstance tt
					       join m_attributevalue gtt on tt.m_attributevalue_id=gtt.m_attributevalue_id and hsv_c_thuoctinh_id=1000019
				        ) tt on io.m_attributesetinstance_id=tt.m_attributesetinstance_id
				where io.ad_client_id=$P{AD_CLIENT_ID}   and iol.c_campaign_id  in (1000093,1000094)

				
		    ) as t
		    
		union all
		
		select sum(t.sl1)-sum(t.sl2)-sum(t.sl3)-sum(t.sl4) as sl,'26' as chitieu
		from (		
		
					select COALESCE(sum(pl.productionqty),0) as sl1,0 as sl2,0 as sl3,0 as sl4,'2' as chitieu 
					from m_productionplan pl
					join m_product hh on pl.m_product_id=hh.m_product_id and hh.m_product_category_id=1000017
					join m_production p on pl.m_production_id=p.m_production_id  and p.docstatus in ('CO','CL')
						AND P.movementdate>=$P{StartDate} and P.movementdate<=$P{EndDate}
					join (select tt.m_attributeset_id,tt.m_attributesetinstance_id from m_attributesetinstance tt 
								join m_attributeset btt on tt.m_attributeset_id=btt.m_attributeset_id and btt.m_attributeset_id=1000000
				     		) tt on pl.m_attributesetinstance_id=tt.m_attributesetinstance_id 	
					where  pl.ad_client_id=$P{AD_CLIENT_ID}  and p.c_campaign_id = 1000018
					
					union all
					
					select -1*COALESCE(sum(io.qtyinternaluse),0)  as sl1,0 as sl2,0 as sl3,0 as sl4,'2' as chitieu	
			        from M_Inventoryline io 
			        join M_Inventory iol on io.M_Inventory_id=iol.M_Inventory_id and iol.docstatus in ('CO','CL') and iol.C_DocType_ID=1000167
       							AND iol.movementdate>=$P{StartDate} and iol.movementdate<=$P{EndDate}
			        join m_product hh on io.m_product_id=hh.m_product_id and hh.m_product_category_id=1000017
			        join (select tt.m_attributeset_id,tt.m_attributesetinstance_id from m_attributesetinstance tt 
						          join m_attributeset btt on tt.m_attributeset_id=btt.m_attributeset_id and btt.m_attributeset_id=1000000
			     	        ) tt on io.m_attributesetinstance_id=tt.m_attributesetinstance_id 
			        where io.ad_client_id=$P{AD_CLIENT_ID}  and iol.c_campaign_id  = 1000018
					
					union all
					
					select 0 as sl1,COALESCE(sum(t.productionqty),0) as sl2,0 as sl3,0 as sl4,'10' as chitieu
					from (							
							select  pl.m_productionplan_id,pl.m_product_id,pl.productionqty,pl.m_attributesetinstance_id from m_productionplan pl
							join m_product hh on pl.m_product_id=hh.m_product_id and  hh.m_product_category_id=1000017 
							join m_production p on pl.m_production_id=p.m_production_id  and p.docstatus in ('CO','CL')
								AND P.movementdate>=$P{StartDate} and P.movementdate<=$P{EndDate}
							join (select tt.m_attributesetinstance_id from m_attributeinstance tt
								join m_attributevalue gtt on tt.m_attributevalue_id=gtt.m_attributevalue_id and hsv_c_thuoctinh_id=1000005
							     ) tt on pl.m_attributesetinstance_id=tt.m_attributesetinstance_id
							where  pl.ad_client_id=$P{AD_CLIENT_ID}  and p.c_campaign_id = 1000018
			
						) as t
						
				   union all
			
				   select 0 as sl1,-1*COALESCE(sum(io.qtyinternaluse),0)  as sl2,0 as sl3,0 as sl4,'20' as chitieu	
			       from M_Inventoryline io 
			       join M_Inventory iol on io.M_Inventory_id=iol.M_Inventory_id and iol.docstatus in ('CO','CL') and iol.C_DocType_ID=1000167
							AND iol.movementdate>=$P{StartDate} and iol.movementdate<=$P{EndDate}
			       join m_product hh on io.m_product_id=hh.m_product_id and hh.m_product_category_id=1000017
			       join (select tt.m_attributesetinstance_id from m_attributeinstance tt
						       join m_attributevalue gtt on tt.m_attributevalue_id=gtt.m_attributevalue_id and hsv_c_thuoctinh_id=1000005
				            ) tt on io.m_attributesetinstance_id=tt.m_attributesetinstance_id
				    where io.ad_client_id=$P{AD_CLIENT_ID}   and iol.c_campaign_id  = 1000018
			
					union all
			
				   select 0 as sl1,-1*COALESCE(sum(io.qtyinternaluse),0)  as sl2,0 as sl3,0 as sl4,'21' as chitieu	
			       from M_Inventoryline io 
			       join M_Inventory iol on io.M_Inventory_id=iol.M_Inventory_id and iol.docstatus in ('CO','CL') and iol.C_DocType_ID=1000167
							AND iol.movementdate>=$P{StartDate} and iol.movementdate<=$P{EndDate}
			       join m_product hh on io.m_product_id=hh.m_product_id and hh.m_product_category_id=1000017
			       join (select tt.m_attributesetinstance_id from m_attributeinstance tt
						       join m_attributevalue gtt on tt.m_attributevalue_id=gtt.m_attributevalue_id and hsv_c_thuoctinh_id=1000006
				            ) tt on io.m_attributesetinstance_id=tt.m_attributesetinstance_id
				    where io.ad_client_id=$P{AD_CLIENT_ID}  and iol.c_campaign_id  = 1000018
				
					union all
			
				   select 0 as sl1,-1*COALESCE(sum(io.qtyinternaluse),0)  as sl2,0 as sl3,0 as sl4,'22' as chitieu	
			       from M_Inventoryline io 
			       join M_Inventory iol on io.M_Inventory_id=iol.M_Inventory_id and iol.docstatus in ('CO','CL') and iol.C_DocType_ID=1000167
							AND iol.movementdate>=$P{StartDate} and iol.movementdate<=$P{EndDate}
			       join m_product hh on io.m_product_id=hh.m_product_id and hh.m_product_category_id=1000017
			       join (select tt.m_attributesetinstance_id from m_attributeinstance tt
						       join m_attributevalue gtt on tt.m_attributevalue_id=gtt.m_attributevalue_id and hsv_c_thuoctinh_id=1000007
				            ) tt on io.m_attributesetinstance_id=tt.m_attributesetinstance_id
				    where io.ad_client_id=$P{AD_CLIENT_ID}   and iol.c_campaign_id  = 1000018
			
						union all
						
						select 0 as sl1,0 as sl2,COALESCE(sum(t.productionqty),0) as sl3,0 as sl4,'11' as chitieu
						from(
							
							select  pl.m_productionplan_id,pl.m_product_id,pl.productionqty,pl.m_attributesetinstance_id from m_productionplan pl
							join m_product hh on pl.m_product_id=hh.m_product_id and  hh.m_product_category_id=1000017 
							join m_production p on pl.m_production_id=p.m_production_id  and p.docstatus in ('CO','CL')
								AND P.movementdate>=$P{StartDate} and P.movementdate<=$P{EndDate}
							join (select tt.m_attributesetinstance_id from m_attributeinstance tt
								join m_attributevalue gtt on tt.m_attributevalue_id=gtt.m_attributevalue_id and hsv_c_thuoctinh_id=1000006
