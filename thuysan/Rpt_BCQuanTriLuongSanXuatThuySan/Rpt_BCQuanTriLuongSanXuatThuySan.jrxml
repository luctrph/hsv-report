							where  pl.ad_client_id=$P{AD_CLIENT_ID}  and p.c_campaign_id  = 1000018
			
					) as t
					
					union all
					
					select 0 as sl1,0 as sl2,0 as sl3,COALESCE(sum(t.productionqty),0) as sl4,'12' as chitieu
					from	(													
							select  pl.m_productionplan_id,pl.m_product_id,pl.productionqty,pl.m_attributesetinstance_id from m_productionplan pl
							join m_product hh on pl.m_product_id=hh.m_product_id and  hh.m_product_category_id=1000017 
							join m_production p on pl.m_production_id=p.m_production_id  and p.docstatus in ('CO','CL')
								AND P.movementdate>=$P{StartDate} and P.movementdate<=$P{EndDate}
							join (select tt.m_attributesetinstance_id from m_attributeinstance tt
