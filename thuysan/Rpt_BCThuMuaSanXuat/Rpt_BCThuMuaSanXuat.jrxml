	<queryString language="SQL">
		<![CDATA[select dm.hsv_stt as stt,dm.value,dm.name,sum(t.sl) as sl,sum(t.lk) as lk
		,case when dvt.c_uom_id<>0 then dvt.name else 
		case when dm.value in ('101','108','113') then '' else 
		case when dm.value in ('105') then 'Kg' else 'Đồng' end end  end as dvt
from(   --Chi tieu 101:công tác thu mua 
        select 'A' as nhom ,'101' as ct,0 as dvt,0 as sl,0 as lk from ad_client WHERE ad_client_id=1000003

        --Chi tieu 102:số lượng ngao thu mua
        union all
        --So lieu trong thang
        select 'A' as nhom,'102' as ct,hh.c_uom_id as dvt,COALESCE(sum(pl.movementqty ),0) as sl  ,0 as lk
        from m_inoutline pl
            join m_product hh on pl.m_product_id=hh.m_product_id and hh.M_Product_ID=1000017
            join m_inout p on pl.m_inout_id=p.m_inout_id  and p.docstatus in ('CO','CL') and p.C_DocType_ID=1000155  
                AND  p.movementdate<=$P{EndDate} and p.movementdate>=$P{StartDate}
        where pl.ad_client_id=1000003
        group by hh.c_uom_id
        
       
        --Chi tieu 104:Tổng  giá trị ngao mua 
        union all
        --So lieu trong ky
        select 'A' as nhom,'104' as ct,0 as dvt,round(COALESCE(sum(pl.linetotalamt),0),0) as sl,0 as lk  
        from c_invoiceline pl
            join m_product hh on pl.m_product_id=hh.m_product_id and hh.M_Product_ID=1000017
            join c_invoice p on pl.c_invoice_id=p.c_invoice_id  and p.docstatus in ('CO','CL') and p.C_DocType_ID=1000146 
                AND  p.dateacct<=$P{EndDate}and p.dateacct>=$P{StartDate}
            
        where pl.ad_client_id=1000003
        
        --Ch tieu 108:công tác sản xuất 
        union all
        
        select 'B' as nhom ,'108' as ct,0 as dvt,0 as sl,0 as lk from ad_client WHERE ad_client_id=1000003

        --Chi tieu 109:số lượng ngao xuất dùng 
        union all

        --So lieu trong ky
        SELECT 'B' as nhom,'109' as ct,hh.c_uom_id as dvt,COALESCE(sum( pl.qtyinternaluse ),0) as sl,0 as lk
        from M_Inventoryline pl
                join m_product hh on pl.m_product_id=hh.m_product_id and hh.M_Product_ID=1000017
                join M_Inventory p on pl.M_Inventory_id=p.M_Inventory_id  and p.docstatus in ('CO','CL') and p.C_DocType_ID=1000167
                    AND P.movementdate>=$P{StartDate} and p.movementdate<=$P{EndDate} and p.C_Campaign_ID in ('1000018','1000007')
        where pl.ad_client_id=1000003 
        group by hh.c_uom_id
        
       --Chi tieu 110:giá trị ngao xuất sử dụng       
        union all
        
        select 'B' as nhom,'110' as ct,0 as dvt,round(COALESCE(sum(t.sl),0)*COALESCE(sum(t.dg),0),0) as sl,round(COALESCE(sum(t.lk),0)*COALESCE(sum(t.dg1),0),0) as lk
        from(   --So luong ngao xuat dung
                --So lieu trong ky
                SELECT 0 as dg,0 as dg1,COALESCE(sum( pl.qtyinternaluse ),0) as sl,0 as lk
                from M_Inventoryline pl
                       join m_product hh on pl.m_product_id=hh.m_product_id and hh.M_Product_ID=1000017
                       join M_Inventory p on pl.M_Inventory_id=p.M_Inventory_id  and p.docstatus in ('CO','CL') and p.C_DocType_ID=1000167
                               AND P.movementdate>=$P{StartDate} and p.movementdate<=$P{EndDate} and p.C_Campaign_ID in ('1000018','1000007')
                where pl.ad_client_id=1000003 
                               
                --Don gia binh quan mua trong ky
                union all

                select case when COALESCE(sum(f.qty),0)<>0 then COALESCE(sum(f.amtacctdr),0)/COALESCE(sum(f.qty),0) end  as dg,0 as dg1,0 as sl,0 as lk  
                from c_invoiceline pl
                    join m_product hh on pl.m_product_id=hh.m_product_id and hh.M_Product_ID=1000017
                    join c_invoice p on pl.c_invoice_id=p.c_invoice_id  and p.docstatus in ('CO','CL') and p.C_DocType_ID=1000146 
                        AND  p.dateacct<=$P{EndDate}and p.dateacct>=$P{StartDate}
                    join fact_acct f on f.record_id=pl.c_invoice_id and f.line_id=pl.c_invoiceline_id and f.amtacctdr>0 and f.ad_table_id=318
                where pl.ad_client_id=1000003
                
                    
            ) as t    
                
