       COALESCE(SUM(t.amtacctdr - t.amtacctcr), 0) AS cp1
	FROM (
		    SELECT fa.amtacctdr,
		           fa.amtacctcr,
		           fa.account_id
		    FROM Fact_Acct fa
		    LEFT JOIN gl_journalline gl ON fa.record_id = gl.gl_journal_id
		                                AND fa.line_id = gl.gl_journalline_id
		                                AND fa.Account_ID = gl.Account_ID
		    LEFT JOIN gl_journal a ON a.gl_journal_id = fa.record_id
		                            AND a.docstatus IN ('CO', 'CL')
		    WHERE fa.dateacct >= $P{StartDate} 
		      AND fa.dateacct <= $P{EndDate}
		      AND fa.AD_Client_ID = $P{AD_CLIENT_ID}
		      AND fa.AD_org_id = 1000027
		      AND (
		          (fa.ad_table_id <> 224) OR
		          (fa.ad_table_id = 224 AND gl.IsGenerated = 'N') OR
		          (fa.ad_table_id = 224 AND fa.line_id IS NULL)
		      )
		) AS t
		JOIN c_elementvalue tk ON t.account_id = tk.c_elementvalue_id 
		AND tk.AD_Client_ID = $P{AD_CLIENT_ID} 
		AND LEFT(tk.value, 4) IN ('6422', '6426')


) as t
join hsv_giathanh l on t.chitieu=l.value and ad_client_id=$P{AD_CLIENT_ID}

--Lien ket de lay so luong san xuat
	left join(
		SELECT 'II' AS nhom,
		       '2' AS chitieu,
		       0 AS cp,
		       COALESCE(SUM(CASE WHEN tt.hsv_c_thuoctinh_id = 1000013 THEN pl.productionqty ELSE 0 END), 0) AS slb,
		       COALESCE(SUM(CASE WHEN tt.hsv_c_thuoctinh_id = 1000005 THEN pl.productionqty ELSE 0 END), 0) AS slb1,
		       COALESCE(SUM(CASE WHEN tt.hsv_c_thuoctinh_id IN (1000006, 1000007, 1000019) THEN pl.productionqty ELSE 0 END), 0) AS slb5
		FROM m_productionplan pl
		JOIN m_product hh ON pl.m_product_id = hh.m_product_id AND hh.M_AttributeSet_ID = 1000000
		JOIN m_production p ON pl.m_production_id = p.m_production_id
		                     AND p.docstatus IN ('CO', 'CL')
		                     AND p.AD_org_id = 1000027
		                     AND p.movementdate >= $P{StartDate} 
		                     AND p.movementdate <= $P{EndDate}
		JOIN (
		    SELECT tt.m_attributesetinstance_id, gtt.hsv_c_thuoctinh_id
		    FROM m_attributeinstance tt
		    JOIN m_attributevalue gtt ON tt.m_attributevalue_id = gtt.m_attributevalue_id
		    WHERE tt.ad_client_id = $P{AD_CLIENT_ID}
		      AND (hsv_c_thuoctinh_id = 1000013 OR hsv_c_thuoctinh_id = 1000005 OR hsv_c_thuoctinh_id IN (1000006, 1000007, 1000019))
		) tt ON pl.m_attributesetinstance_id = tt.m_attributesetinstance_id
		WHERE pl.ad_client_id = $P{AD_CLIENT_ID}
) as sx on t.nhom=sx.nhom

--Lien ket de lay so luong xuat ban
	left join (		
		select 'III' as nhom,'3' as chitieu,0 as cp,COALESCE(sum(t.slb),0) as slb,COALESCE(sum(t.slb1),0) as slb1,COALESCE(sum(t.slb5),0) as slb5
		from(
			select COALESCE(sum(io.movementqty),0) as slb,0 as slb1,0 as slb5,'3' as chitieu	from m_inoutline io
			join m_inout iol on io.m_inout_id=iol.m_inout_id and iol.docstatus in ('CO','CL') and iol.C_DocType_ID=1000152
			AND iol.movementdate>=$P{StartDate} and iol.movementdate<=$P{EndDate}and  (iol.AD_org_id=1000027)
			join m_product hh on io.m_product_id=hh.m_product_id and hh. M_AttributeSet_ID=1000000
			join (
				select tt.m_attributesetinstance_id from m_attributeinstance tt
				join m_attributevalue gtt on tt.m_attributevalue_id=gtt.m_attributevalue_id and hsv_c_thuoctinh_id=1000013 and gtt.m_attribute_id=1000001
				WHERE tt.ad_client_id=$P{AD_CLIENT_ID}
			) tt on IO.m_attributesetinstance_id=tt.m_attributesetinstance_id
		where io.ad_client_id=$P{AD_CLIENT_ID}
	
		union all
	
		select 0 as slb,COALESCE(sum(io.movementqty),0) as slb1,0 as slb5,'3' as chitieu	
		from m_inoutline io
		join m_inout iol on io.m_inout_id=iol.m_inout_id and iol.docstatus in ('CO','CL') and iol.C_DocType_ID=1000152
		AND iol.movementdate>=$P{StartDate} and iol.movementdate<=$P{EndDate}and  (iol.AD_org_id=1000027)
		join m_product hh on io.m_product_id=hh.m_product_id and hh. M_AttributeSet_ID=1000000
		join (
			select tt.m_attributesetinstance_id from m_attributeinstance tt
			join m_attributevalue gtt on tt.m_attributevalue_id=gtt.m_attributevalue_id and hsv_c_thuoctinh_id=1000005 and gtt.m_attribute_id=1000001
			WHERE tt.ad_client_id=$P{AD_CLIENT_ID}
		) tt on IO.m_attributesetinstance_id=tt.m_attributesetinstance_id
		where io.ad_client_id=$P{AD_CLIENT_ID}
		
		union all
		
		select 0 as slb,0 as slb1,COALESCE(sum(io.movementqty),0) as slb5,'3' as chitieu	
		from m_inoutline io
		join m_inout iol on io.m_inout_id=iol.m_inout_id and iol.docstatus in ('CO','CL') and iol.C_DocType_ID=1000152
		AND iol.movementdate>=$P{StartDate} and iol.movementdate<=$P{EndDate}and  (iol.AD_org_id=1000027)
		join m_product hh on io.m_product_id=hh.m_product_id and hh. M_AttributeSet_ID=1000000
		join (
			select tt.m_attributesetinstance_id from m_attributeinstance tt
			join m_attributevalue gtt on tt.m_attributevalue_id=gtt.m_attributevalue_id and hsv_c_thuoctinh_id in(1000006,1000007,1000019) and gtt.m_attribute_id=1000001
			WHERE tt.ad_client_id=$P{AD_CLIENT_ID}
		) tt on IO.m_attributesetinstance_id=tt.m_attributesetinstance_id
		where io.ad_client_id=$P{AD_CLIENT_ID}
	
	) as t
)	xb on t.nhom=xb.nhom
group by t.nhom,l.description,cast(l.value as integer),l.name
order by cast(value as integer) asc]]>
