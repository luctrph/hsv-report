	<queryString language="SQL">
		<![CDATA[select l.description,cast(l.value as integer) as value,l.name,sum(t.cp) as cp,sum(case when t.nhom <> 'I' then  t.cp1 else 0 end ) as cp1
	,sum(case when t.nhom='I' then t.slb else case when t.nhom='II' then l.hsv_dinhmuc else l.hsv_dinhmuc end end ) as slb
	,sum(case when t.nhom='I' then t.slb1 else case when t.nhom='II' then l.hsv_dinhmuc1 else l.hsv_dinhmuc1 end end ) as slb1
	,sum(case when t.nhom='I' then t.slb5 else case when t.nhom='II' then l.hsv_dinhmuc05 else l.hsv_dinhmuc05  end end ) as slb5
	--Cot de cong
	,sum( case when t.nhom in ('II','III') then l.hsv_dinhmuc  else 0 end ) as slbc
	,sum(case when t.nhom in ('II','III') then l.hsv_dinhmuc1 else 0 end ) as slbc1
	,sum( case when t.nhom in ('II','III') then l.hsv_dinhmuc05  else 0 end ) as slbc5
	
	,sum(case when t.nhom='I' then t.cp1 else case when t.nhom='II' and t.chitieu<>'17' and t.chitieu<>'18' then l.hsv_dinhmuc*sx.slb+l.hsv_dinhmuc1*sx.slb1+l.hsv_dinhmuc05*sx.slb5 else l.hsv_dinhmuc*xb.slb+l.hsv_dinhmuc1*xb.slb1+l.hsv_dinhmuc05*xb.slb5 end end ) as tdm
	, sum(case when l.value = '2' then t.slb end) as a1
	,sum(case when l.value = '2' then t.slb1 end) as a2
	,sum(case when l.value ='2' then t.slb5 end) as a3
from (
	select 'I' as nhom,'1' as chitieu,-1* sum(fa.qty) as cp , 0 as slb,0 as slb1,0 as slb5, -1*sum(fa.amtacctdr-fa.amtacctcr)  as cp1
	from m_inventory mi
	join m_inventoryline miv on mi.m_inventory_id  = miv.m_inventory_id
	join fact_acct fa on fa.record_id = miv.m_inventory_id and fa.line_id = miv.m_inventoryline_id and fa.ad_table_id = 321
	join c_elementvalue ev on fa.account_id=ev.c_elementvalue_id and ev.ad_client_id=$P{AD_CLIENT_ID} and left(ev.value,2) in ('15')
	where  mi.ad_client_id = $P{AD_CLIENT_ID} and mi.movementdate  >= $P{StartDate} and mi.movementdate <= $P{EndDate}
	and fa.m_product_id = 1000017 and fa.ad_org_id =1000027
	
	union all
	--Chi tieu so 2 so luong san xuat
	select 'I' as nhom,'2' as chitieu,0 as cp,COALESCE(sum(t.slb),0) as slb,COALESCE(sum(t.slb1),0) as slb1,COALESCE(sum(t.slb5),0) as slb5,0 as cp1
	from (
			select distinct pl.m_productionplan_id as m_productionline_id,pl.m_product_id,pl.productionqty as slb,0 as slb1,0 as slb5,pl.m_attributesetinstance_id
			from m_productionplan pl
			join m_product hh on pl.m_product_id=hh.m_product_id and  hh.M_AttributeSet_ID=1000000
			join m_production p on pl.m_production_id=p.m_production_id  and p.docstatus in ('CO','CL') and  (p.AD_org_id=1000027)
			AND p.movementdate>=$P{StartDate} and p.movementdate<=$P{EndDate}
			join (
				select tt.m_attributesetinstance_id 
				from m_attributeinstance tt
				join m_attributevalue gtt on tt.m_attributevalue_id=gtt.m_attributevalue_id and hsv_c_thuoctinh_id=1000013 and gtt.m_attribute_id=1000001
				WHERE tt.ad_client_id=$P{AD_CLIENT_ID}
			) tt on pl.m_attributesetinstance_id=tt.m_attributesetinstance_id
		where  pl.ad_client_id=$P{AD_CLIENT_ID}
		
		union all
		
		select distinct pl.m_productionplan_id as m_productionline_id,pl.m_product_id,0 as slb,pl.productionqty as slb1,0 as slb5,pl.m_attributesetinstance_id
		from m_productionplan pl
		join m_product hh on pl.m_product_id=hh.m_product_id and  hh.M_AttributeSet_ID=1000000
		join m_production p on pl.m_production_id=p.m_production_id  and p.docstatus in ('CO','CL') and  (p.AD_org_id=1000027)
		AND p.movementdate>=$P{StartDate} and p.movementdate<=$P{EndDate}
		join (
			select tt.m_attributesetinstance_id 
			from m_attributeinstance tt
			join m_attributevalue gtt on tt.m_attributevalue_id=gtt.m_attributevalue_id and hsv_c_thuoctinh_id=1000005 and gtt.m_attribute_id=1000001
			WHERE tt.ad_client_id=$P{AD_CLIENT_ID}
		) tt on pl.m_attributesetinstance_id=tt.m_attributesetinstance_id
		where  pl.ad_client_id=$P{AD_CLIENT_ID}
		
		union all
		
		select distinct pl.m_productionplan_id as m_productionline_id,pl.m_product_id,0 as slb,0 as slb1,pl.productionqty as slb5,pl.m_attributesetinstance_id
		from m_productionplan pl
		join m_product hh on pl.m_product_id=hh.m_product_id and  hh.M_AttributeSet_ID=1000000
		join m_production p on pl.m_production_id=p.m_production_id  and p.docstatus in ('CO','CL') and  (p.AD_org_id=1000027)
		AND p.movementdate>=$P{StartDate} and p.movementdate<=$P{EndDate}
		join (
			select tt.m_attributesetinstance_id 
			from m_attributeinstance tt
			join m_attributevalue gtt on tt.m_attributevalue_id=gtt.m_attributevalue_id and hsv_c_thuoctinh_id in(1000006,1000007,1000019) and gtt.m_attribute_id=1000001
			WHERE tt.ad_client_id=$P{AD_CLIENT_ID}
		) tt on pl.m_attributesetinstance_id=tt.m_attributesetinstance_id
		where  pl.ad_client_id=$P{AD_CLIENT_ID}
	
	) as t
	
	union all
	--Chi tieu so 3 so luong xuat ban
	SELECT 'I' AS nhom,'3' AS chitieu,  0 AS cp,   COALESCE(SUM(CASE WHEN ta.hsv_c_thuoctinh_id = 1000013 THEN io.movementqty ELSE 0 END), 0) AS slb,
    COALESCE(SUM(CASE WHEN ta.hsv_c_thuoctinh_id = 1000005 THEN io.movementqty ELSE 0 END), 0) AS slb1,
    COALESCE(SUM(CASE WHEN ta.hsv_c_thuoctinh_id IN (1000006, 1000007, 1000019) THEN io.movementqty ELSE 0 END), 0) AS slb5, 0 AS cp1
	FROM 
	    m_inoutline io
	JOIN  m_inout iol ON io.m_inout_id = iol.m_inout_id  AND iol.docstatus IN ('CO', 'CL')   AND iol.C_DocType_ID = 1000152  AND iol.movementdate >= $P{StartDate}    AND iol.movementdate <= $P{EndDate}
	    AND iol.AD_org_id = 1000027
	JOIN m_product hh ON io.m_product_id = hh.m_product_id  AND hh.M_AttributeSet_ID = 1000000
	JOIN (
		SELECT tt.m_attributesetinstance_id,  gtt.hsv_c_thuoctinh_id
	    FROM  m_attributeinstance tt
	    JOIN m_attributevalue gtt ON tt.m_attributevalue_id = gtt.m_attributevalue_id 
	    WHERE tt.ad_client_id = $P{AD_CLIENT_ID}
	) ta ON io.m_attributesetinstance_id = ta.m_attributesetinstance_id
	WHERE io.ad_client_id = $P{AD_CLIENT_ID}


	
	union all
	--Chi tieu so 4
	select 'I' as nhom,'4' as chitieu,sum(miv.productionqty) as cp,0 as slb,0 as slb1,0 as slb5,0 as cp1
	from m_production mi
	join m_productionplan miv on mi.m_production_id = miv.m_production_id and mi.docstatus in ('CL','CO')
	join m_product hh on miv.m_product_id = hh.m_product_id and hh.M_Product_Category_ID=1000017
	where mi.ad_client_id=$P{AD_CLIENT_ID} and mi.c_campaign_id in (1000018,1000093)  and mi.movementdate  >= $P{StartDate} and mi.movementdate <= $P{EndDate}
	and mi.ad_org_id = 1000027
	
	union all
	--Chi tieu so 5: chi phi san xuat
	select 'I' as nhom,'5' as chitieu,0 as cp,0 as slb,0 as slb1,0 as slb5,0 as cp1  from ad_client where ad_client_id=$P{AD_CLIENT_ID}
	
	--Chi tieu so 6: chi phi thu mua tai khoan 6276
	union all
	SELECT 
	    'II' AS nhom,
	    '6' AS chitieu,
	    COALESCE(SUM(t.amtacctdr - t.amtacctcr), 0) AS cp,
	    0 AS slb,
	    0 AS slb1,
	    0 AS slb5,
	    COALESCE(SUM(t.amtacctdr - t.amtacctcr), 0) AS cp1
	FROM (
	    SELECT fa.amtacctdr,
	        fa.amtacctcr,
	        fa.account_id
	    FROM 
	        Fact_Acct fa
	    LEFT JOIN 
	        gl_journalline gl ON fa.record_id = gl.gl_journal_id
	        AND fa.line_id = gl.gl_journalline_id
	        AND fa.Account_ID = gl.Account_ID
	    LEFT JOIN 
	        gl_journal a ON a.gl_journal_id = fa.record_id
	        AND a.docstatus IN ('CO', 'CL')
	    WHERE 
	        fa.dateacct <= $P{EndDate}
	        AND fa.dateacct >= $P{StartDate}
	        AND fa.AD_Client_ID = $P{AD_CLIENT_ID}
	        AND fa.AD_org_id = 1000027
	        AND (
	            fa.ad_table_id <> 224
	            OR (fa.ad_table_id = 224 AND gl.IsGenerated = 'N')
	            OR (fa.ad_table_id = 224 AND fa.line_id IS NULL)
	        )
	) AS t
	JOIN 
	    c_elementvalue tk 
	    ON t.account_id = tk.c_elementvalue_id
	    AND tk.AD_Client_ID = $P{AD_CLIENT_ID} 
	    AND LEFT(tk.value, 4) IN ('6276')

	--Chi tieu so 7: Chi phi tien dien tai khoan 6275
	
	union all
	
	select 	'II' as nhom,'7' as chitieu, COALESCE(sum( t.amtacctdr-t.amtacctcr ),0) as cp,0 as slb,0 as slb1,0 as slb5,COALESCE( sum( t.amtacctdr-t.amtacctcr ),0) as cp1
	from
	( 
		SELECT fa.dateacct,fa.amtacctdr,fa.amtacctcr,fa.account_id,fa.description ,fa.AD_Client_ID
		FROM Fact_Acct fa
		WHERE
		fa.dateacct<=$P{EndDate} and fa.dateacct>=$P{StartDate}
		and  fa.AD_Client_ID = $P{AD_CLIENT_ID}and  (fa.AD_org_id=1000027)
		and fa.ad_table_id=318 and fa.C_BPartner_ID in (1000147,1000741)
	
	) as t
	join c_elementvalue tk 	on t.account_id=tk.c_elementvalue_id and tk.AD_Client_ID = $P{AD_CLIENT_ID} and left(tk.value,4) in ('6275')
	
	--Chi tieu so 8: chi phi tien nuoc tai khoan 6275 voi khach hang la C_BPartner_ID=1000247
	
	union all
	
	select 	'II' as nhom,'8' as chitieu, COALESCE(sum( t.amtacctdr-t.amtacctcr ),0) as cp,0 as slb,0 as slb1,0 as slb5,COALESCE( sum( t.amtacctdr-t.amtacctcr ),0) as cp1
	from( 	
		SELECT fa.dateacct,fa.amtacctdr,fa.amtacctcr,fa.account_id,fa.description ,fa.AD_Client_ID
		FROM Fact_Acct fa
		where fa.dateacct<=$P{EndDate} and fa.dateacct>=$P{StartDate}
		and  fa.AD_Client_ID = $P{AD_CLIENT_ID} and  (fa.AD_org_id=1000027)
		and fa.ad_table_id=318 and fa.C_BPartner_ID in (1000247)
	) as t
	join c_elementvalue tk 	on t.account_id=tk.c_elementvalue_id and tk.AD_Client_ID = $P{AD_CLIENT_ID} and left(tk.value,4) in ('6275')
	
	--chi tieu so 9:Chi phi than dot lo bang phat sinh tai khoan 6275-chi phi dien, nuoc
	union all
	
	select 	'II' as nhom,'9' as chitieu, COALESCE(sum( t.amtacctdr-t.amtacctcr ),0) as cp,0 as slb,0 as slb1,0 as slb5,COALESCE( sum( t.amtacctdr-t.amtacctcr ),0) as cp1
	from ( 
		SELECT fa.dateacct,fa.amtacctdr,fa.amtacctcr,fa.account_id,fa.description ,fa.AD_Client_ID
		FROM Fact_Acct fa
		WHERE
		fa.dateacct<=$P{EndDate} and fa.dateacct>=$P{StartDate}
		and  fa.AD_Client_ID = $P{AD_CLIENT_ID} and  (fa.AD_org_id=1000027)
		and fa.ad_table_id<>224
		
		union all
		-- tru di chi phi dien,nuoc
		SELECT fa.dateacct,-1*fa.amtacctdr,-1*fa.amtacctcr,fa.account_id,fa.description ,fa.AD_Client_ID
		FROM Fact_Acct fa
		WHERE
		fa.dateacct<=$P{EndDate} and fa.dateacct>=$P{StartDate}
		and  fa.AD_Client_ID = $P{AD_CLIENT_ID} and  (fa.AD_org_id=1000027)
		and fa.ad_table_id=318 and fa.C_BPartner_ID in (1000147,1000741,1000247)
		
		
		
		union all
		SELECT fa.dateacct,fa.amtacctdr,fa.amtacctcr,fa.account_id ,fa.description ,fa.AD_Client_ID
		FROM Fact_Acct fa
		join gl_journalline gl on  fa.record_id=gl.gl_journal_id
		and fa.line_id=gl.gl_journalline_id and fa.Account_ID=gl.Account_ID  and gl.IsGenerated='N'
		join gl_journal a on a.gl_journal_id =fa.record_id and a.docstatus in ('CO','CL')
		WHERE
		fa.dateacct<=$P{EndDate} and fa.dateacct>=$P{StartDate}
		and  fa.AD_Client_ID = $P{AD_CLIENT_ID} and  (fa.AD_org_id=1000027)
		and fa.ad_table_id=224
		
		union all
		
		SELECT fa.dateacct,fa.amtacctdr,fa.amtacctcr,fa.account_id ,fa.description ,fa.AD_Client_ID
		FROM Fact_Acct fa
		join gl_journal a on a.gl_journal_id =fa.record_id and a.docstatus in ('CO','CL')
		WHERE
		fa.dateacct<=$P{EndDate} and fa.dateacct>=$P{StartDate}
		and  fa.AD_Client_ID = $P{AD_CLIENT_ID} and  (fa.AD_org_id=1000027)
		and fa.line_id is null
	) as t
	join c_elementvalue tk 	on t.account_id=tk.c_elementvalue_id and tk.AD_Client_ID = $P{AD_CLIENT_ID} and left(tk.value,4) in ('6275')
	
	--Chi tieu so 10: bao bi PA(gom ca truc in) xuat kho su dung voi M_PartType_ID=1000076
	
	union all
	
	select 	'II' as nhom,'10' as chitieu, COALESCE(sum( t.amtacctdr -t.amtacctcr ),0) as cp,0 as slb,0 as slb1,0 as slb5,COALESCE( sum( t.amtacctdr-t.amtacctcr  ),0) as cp1
	from ( 
		SELECT fa.dateacct,fa.amtacctdr,fa.amtacctcr,fa.m_product_id,fa.account_id,fa.description ,fa.AD_Client_ID
		FROM Fact_Acct fa
		join  M_Inventory m on fa.record_id=m.M_Inventory_id and m.docstatus in ('CO','CL') and m.C_DocType_ID in (1000167,1000164)
		join m_product hh on fa.m_product_id=hh.m_product_id and hh.M_PartType_ID=1000076
		WHERE
		fa.dateacct<=$P{EndDate} and fa.dateacct>=$P{StartDate}
		and  fa.AD_Client_ID = $P{AD_CLIENT_ID} and  (fa.AD_org_id=1000027)
		and fa.ad_table_id=321 
	
	) as t
	join c_elementvalue tk 	on t.account_id=tk.c_elementvalue_id and tk.AD_Client_ID = $P{AD_CLIENT_ID} and left(tk.value,4) in ('6278')
	
	
	--Chi tieu so 11: Bao bi PE voi xuat kho su dung voi M_PartType_ID=1000077
	union all
	
	select 	'II' as nhom,'11' as chitieu, COALESCE(sum( t.amtacctdr -t.amtacctcr ),0) as cp,0 as slb,0 as slb1,0 as slb5,COALESCE( sum( t.amtacctdr -t.amtacctcr ),0) as cp1
	from ( 
		SELECT fa.dateacct,fa.amtacctdr,fa.amtacctcr,fa.m_product_id,fa.account_id,fa.description ,fa.AD_Client_ID
		FROM Fact_Acct fa
		join  M_Inventory m on fa.record_id=m.M_Inventory_id and m.docstatus in ('CO','CL') and m.C_DocType_ID in (1000167,1000164)
		join m_product hh on fa.m_product_id=hh.m_product_id and hh.M_PartType_ID=1000077
		WHERE
		fa.dateacct<=$P{EndDate} and fa.dateacct>=$P{StartDate}
		and  fa.AD_Client_ID = $P{AD_CLIENT_ID} and  (fa.AD_org_id=1000027)
		and fa.ad_table_id=321 
	
	) as t
	join c_elementvalue tk 	on t.account_id=tk.c_elementvalue_id and tk.AD_Client_ID = $P{AD_CLIENT_ID} and left(tk.value,4) in ('6278')
	
	--Chi tieu so 12: Bao bi tem nhan voi xuat kho su dung voi M_PartType_ID=1000078
	union all
	
	select 	'II' as nhom,'12' as chitieu, COALESCE(sum(   t.amtacctdr -t.amtacctcr),0) as cp,0 as slb,0 as slb1,0 as slb5,COALESCE( sum(  t.amtacctdr -t.amtacctcr),0) as cp1
	from ( 
		SELECT fa.dateacct,fa.amtacctdr,fa.amtacctcr,fa.m_product_id,fa.account_id,fa.description ,fa.AD_Client_ID
		FROM Fact_Acct fa
		join  M_Inventory m on fa.record_id=m.M_Inventory_id and m.docstatus in ('CO','CL') and m.C_DocType_ID in (1000167,1000164)
		join m_product hh on fa.m_product_id=hh.m_product_id and hh.M_PartType_ID=1000078
		where fa.dateacct<=$P{EndDate} and fa.dateacct>=$P{StartDate}
		and  fa.AD_Client_ID = $P{AD_CLIENT_ID} and  (fa.AD_org_id=1000027)
		and fa.ad_table_id=321
	
	) as t
	join c_elementvalue tk 	on t.account_id=tk.c_elementvalue_id and tk.AD_Client_ID = $P{AD_CLIENT_ID} and left(tk.value,4) in ('6278')
	
	--Chi tieu so 13: Bao bi carton voi xuat kho su dung voi M_PartType_ID=1000079
	union all
	
	select 	'II' as nhom,'13' as chitieu, COALESCE(sum( t.amtacctdr -t.amtacctcr ),0) as cp,0 as slb,0 as slb1,0 as slb5,COALESCE( sum( t.amtacctdr -t.amtacctcr ),0) as cp1
	from (
		SELECT fa.dateacct,fa.amtacctdr,fa.amtacctcr,fa.m_product_id,fa.account_id,fa.description ,fa.AD_Client_ID
		FROM Fact_Acct fa
		join  M_Inventory m on fa.record_id=m.M_Inventory_id and m.docstatus in ('CO','CL') and m.C_DocType_ID in (1000167,1000164)
		join m_product hh on fa.m_product_id=hh.m_product_id and hh.M_PartType_ID=1000079
