join( select 	'I' as ct,-1*sum(case when t.ct='2' then t.qty else 0 end )*case when sum(case when t.ct='1' then t.qty else 0 end )<>0 then sum(case when t.ct='1'then t.amtacctdr else 0 end)/sum(case when t.ct='1' then t.qty else 0 end ) else 0 end as nltt
