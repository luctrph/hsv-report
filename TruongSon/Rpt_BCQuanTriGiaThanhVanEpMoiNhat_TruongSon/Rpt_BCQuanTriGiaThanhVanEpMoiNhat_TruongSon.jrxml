			                            ,sum(case when m.movementdate<=$P{EndDate} then m.movementqty*hh.ShelfDepth*hh.ShelfWidth*hh.ShelfHeight end )/1000000000 
			                            -sum(case when m.movementdate<$P{StartDate} then m.movementqty*hh.ShelfDepth*hh.ShelfWidth*hh.ShelfHeight end )/1000000000 as sl
