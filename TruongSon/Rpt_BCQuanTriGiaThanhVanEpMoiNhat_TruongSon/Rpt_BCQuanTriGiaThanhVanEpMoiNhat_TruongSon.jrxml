<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="Rpt_BCQuanTriGiaThanhVanEpMoiNhat_TruongSon" pageWidth="842" pageHeight="595" orientation="Landscape" columnWidth="842" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0" uuid="4023df51-1b73-46d9-adf6-650e3c9f2034">
	<property name="com.jaspersoft.studio.data.defaultdataadapter" value="adempiere_7.1"/>
	<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
	<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w1" value="169"/>
	<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w2" value="824"/>
	<parameter name="StartDate" class="java.util.Date">
		<defaultValueExpression><![CDATA[new Date(100,0,1)]]></defaultValueExpression>
	</parameter>
	<parameter name="EndDate" class="java.util.Date">
		<defaultValueExpression><![CDATA[new Date(100,0,1)]]></defaultValueExpression>
	</parameter>
	<parameter name="AD_CLIENT_ID" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[1000000]]></defaultValueExpression>
	</parameter>
	<parameter name="SUBREPORT_DIR" class="java.lang.String">
		<defaultValueExpression><![CDATA["dev/"]]></defaultValueExpression>
	</parameter>
	<parameter name="EXPORT_TYPE" class="java.lang.String"/>
	<parameter name="AD_Org_ID" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[select t.stt,t.value,t.name,t.b,t.c,t.d,t.e,case when t.value in ('1000','999','1001','1002','1003','1004','998','10031','998') then 0 else t.g end as g 
,case when t.value in ('1000','999','1001','1002','1003','1004','998','10031','998') then 0 else t.b end as chiphi
,case when t.value in ('1000','999','1001','1002','1003','1004','998','10031','998') then 0 else t.e end as binhquan
,case when t.value in ('1000') then t.e else 0  end as bqban
from(SELECT row_number() over() as stt,gt.value
			,case when t.chitieu in ('1001') then concat(gt.name,gt.hsv_dinhmuc1,'%') else 
			case when t.chitieu in ('1002') then concat(gt.name,gt.hsv_dinhmuc2,'%') else 
			case when t.chitieu in ('1003') then concat(gt.name,gt.hsv_dinhmuc3,'%') else
			case when t.chitieu in ('10031') then concat(gt.name,gt.hsv_dinhmuc4,'%') else
			gt.name end end end end as name
			,case when $P{AD_Org_ID}=1000119 then gt.hsv_dinhmuc else
			case when $P{AD_Org_ID}=1000047 then gt.hsv_dinhmuc05 else gt.hsv_dinhmuc end end AS b
			,case when t.chitieu in ('1000','999','1001','1002','1003','1004','998','10031') then t.c 
			else case when t.chitieu in ('1018','1019') then sl1.c 
			else case when t.chitieu in ('1006') then (sl3.c+sl3.c1*gt.hsv_dinhmuc1/100+sl3.c2*gt.hsv_dinhmuc2/100)--sl3.c
			else case when t.chitieu in ('1005') then t.c else  (sl2.c+sl2.c1*gt.hsv_dinhmuc1/100+sl2.c2*gt.hsv_dinhmuc2/100+sl2.c3*gt.hsv_dinhmuc3/100+sl2.c4*gt.hsv_dinhmuc4/100) end end end end as c,t.d
		    ,case when t.chitieu in ('1000','999','1001','1002','1003','1004','1005','998','10031')then t.e 
	         else case when  sl1.c<>0 and t.chitieu in ('1018','1019') then t.d/sl1.c else  case when (sl3.c+sl3.c1*gt.hsv_dinhmuc1/100+sl3.c2*gt.hsv_dinhmuc2/100)<>0 and t.chitieu in ('1006') then t.d/(sl3.c+sl3.c1*gt.hsv_dinhmuc1/100+sl3.c2*gt.hsv_dinhmuc1/100) else t.d/(sl2.c+sl2.c1*gt.hsv_dinhmuc1/100+sl2.c2*gt.hsv_dinhmuc2/100+sl2.c3*gt.hsv_dinhmuc3/100+sl2.c4*gt.hsv_dinhmuc4/100) end  end end as e
	         ,case when t.chitieu in ('1000','999','1001','1002','1003','1004','998','10031')then t.e 
	         else case when t.chitieu in ('1005')then t.e
	         else case when  sl1.c<>0 and t.chitieu in ('1018','1019') then t.d/sl1.c 
	         else  case when (sl3.c+sl3.c1*gt.hsv_dinhmuc1/100+sl3.c2*gt.hsv_dinhmuc2/100)<>0 and t.chitieu in ('1006') then t.d/(sl3.c+sl3.c1*gt.hsv_dinhmuc1/100+sl3.c2*gt.hsv_dinhmuc1/100) else t.d/(sl2.c+sl2.c1*gt.hsv_dinhmuc1/100+sl2.c2*gt.hsv_dinhmuc2/100+sl2.c3*gt.hsv_dinhmuc3/100+sl2.c4*gt.hsv_dinhmuc4/100) end  end end END 
	        -case when $P{AD_Org_ID}=1000119  then gt.hsv_dinhmuc else
			case when $P{AD_Org_ID}=1000047  then gt.hsv_dinhmuc05 else gt.hsv_dinhmuc end end as g
	         
	FROM(
		      SELECT 'III' as nhom,'998' AS chitieu,0 AS b,0 AS c  ,0 AS d, 0 AS e FROM ad_client WHERE ad_client_id=$P{AD_CLIENT_ID}
		      union all
	      --Chi tieu 999:Ván lỗi xuất bán 
	      SELECT 'I' as nhom,'999' AS chitieu,0 AS b,sl as c,tt AS d,CASE WHEN sl<>0 THEN tt/sl ELSE 0 END AS e
	      FROM (
	      		select sum(tt) as tt, sum(sl)/1000000000 as sl
	      		from(
		      		SELECT COALESCE(sum(fc.amtacctcr),0)-COALESCE(sum(fc.amtacctdr),0) AS tt
		                    ,-1*COALESCE(sum(fc.qty*hh.ShelfWidth *hh.ShelfDepth *hh.ShelfHeight),0) AS sl 
		            FROM fact_acct fc 
		            JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,2)='51'
		            JOIN M_product hh ON fc.m_product_id=hh.m_product_id AND hh.M_AttributeSet_ID IN ('1000130','1000131') 
		            WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id<>224 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} AND (fc.AD_Org_ID=$P{AD_Org_ID} OR $P{AD_Org_ID} IS NULL) AND fc.C_Activity_ID=1000072
		            	 
						
		             UNION ALL
						
		             SELECT COALESCE(sum(fc.amtacctcr),0)-COALESCE(sum(fc.amtacctdr),0) AS tt
		                    ,-1*COALESCE(sum(fc.qty*hh.ShelfWidth *hh.ShelfDepth *hh.ShelfHeight),0) AS sl  
		             FROM fact_acct fc 
		             JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,2)='51'
		             JOIN gl_journalline gl ON  Fc.record_id=gl.gl_journal_id AND Fc.line_id=gl.gl_journalline_id AND Fc.Account_ID=gl.Account_ID
		                                 AND gl.IsGenerated='N'
		             JOIN M_product hh ON fc.m_product_id=hh.m_product_id AND hh.M_AttributeSet_ID IN ('1000130','1000131') 
		             WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id=224 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} AND (fc.AD_Org_ID=$P{AD_Org_ID} OR $P{AD_Org_ID} IS NULL) AND fc.C_Activity_ID=1000072
	            
	  			) as t
	            
	           ) AS t        
	
	      UNION ALL  
	      --xuát bán ván xuất khẩu : chi tieu 1000
	      SELECT 'I' as nhom,'1000' AS chitieu,0 AS b,COALESCE(sum(t.sl)) AS c,COALESCE(sum(t.tt)) AS d,sum(CASE WHEN t.sl<>0 THEN t.tt/t.sl ELSE 0 END) AS e
	      FROM (
			      select sum(sl) as sl, sum(tt) as tt 
			      from
			      (
	      			select 0 as sl, sum(tt) as tt
	      			from(
		      			SELECT fc.account_id,COALESCE(sum(fc.amtacctcr),0)-COALESCE(sum(fc.amtacctdr),0) AS tt,0 AS sl 
		      			FROM fact_acct fc 
		            	    JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,2)='51'
		            	    JOIN M_product hh ON fc.m_product_id=hh.m_product_id AND hh.M_AttributeSet_ID IN ('1000064','1000136') 
		               		WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id<>224 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} AND (fc.AD_Org_ID=$P{AD_Org_ID} OR $P{AD_Org_ID} IS NULL) AND fc.C_Activity_ID=1000072
		            	    GROUP BY fc.account_id
						
		                  UNION ALL
						
		                  SELECT fc.account_id,COALESCE(sum(fc.amtacctcr),0)-COALESCE(sum(fc.amtacctdr),0) AS tt,0 AS sl  
		                  FROM fact_acct fc 
		               		JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,2)='51'
		               		JOIN gl_journalline gl ON  Fc.record_id=gl.gl_journal_id AND Fc.line_id=gl.gl_journalline_id AND Fc.Account_ID=gl.Account_ID
		                                   AND gl.IsGenerated='N'
		             	    JOIN M_product hh ON fc.m_product_id=hh.m_product_id AND hh.M_AttributeSet_ID IN ('1000064','1000136') 
		               		WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id=224 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} AND (fc.AD_Org_ID=$P{AD_Org_ID} OR $P{AD_Org_ID} IS NULL) AND fc.C_Activity_ID=1000072
		                  GROUP BY fc.account_id
	  					) as t
	  				union all 
	  				select vout.qtyentered as sl ,0 as tt from c_invoice ci 
	  				join c_invoiceline civ on ci.c_invoice_id = civ.c_invoice_id and ci.docstatus in ('CO','CL')
	  				join m_inoutline vout on civ.m_inoutline_id = vout.m_inoutline_id
					join m_inout cout on cout.m_inout_id = vout.m_inout_id 
					join m_product hh on hh.m_product_id = civ.m_product_id and hh.M_AttributeSet_ID IN ('1000064','1000136') 
					where ci.ad_client_id=$P{AD_CLIENT_ID} and ci.dateacct>=$P{StartDate} AND ci.dateacct<=$P{EndDate} AND (cout.AD_Org_ID=$P{AD_Org_ID} OR $P{AD_Org_ID} IS NULL) AND ci.C_Activity_ID=1000072
	           ) AS t        
			) as t
	      UNION ALL
	      --ván sản xuất  kiêm lần 1 (tấm): chi tieu 1001  
	      SELECT 'II' as nhom,'1001' AS chitieu,0 AS b
	            ,sum(case when t.movementdate<=$P{EndDate} then t.sl else 0 end)-sum(case when t.movementdate<$P{StartDate} then t.sl else 0 end) AS c  ,0 AS d, 0 AS e
	      FROM( select m_transaction.AD_Org_ID,hh.value ,hh.name ,m_transaction.m_product_id,m_transaction.movementdate,m_transaction.movementtype,m_transaction.movementqty as sl
	            from m_transaction 
	            join m_product hh on m_transaction.m_product_id=hh.m_product_id and hh.M_AttributeSet_ID IN ('1000064','1000136') 
			    where m_transaction.movementdate<=$P{EndDate} and m_transaction.ad_client_id=$P{AD_CLIENT_ID} 
	                AND (m_transaction.m_locator_id in (select m_locator_id from m_locator where AD_Org_ID=$P{AD_Org_ID} ) or $P{AD_Org_ID} is null )
	          ) AS t
	
	
	      UNION ALL
	
	      --Ván sản xuất  kiêm lần 1 (m3) duoc quy doi: chi tieu 1002  sp.ShelfDepth*sp.ShelfWidth*sp.ShelfHeight
	      SELECT 'III' as nhom,'1002' AS chitieu,0 AS b
	            ,sum(case when t.movementdate<=$P{EndDate} then t.sl else 0 end)-sum(case when t.movementdate<$P{StartDate} then t.sl else 0 end) AS c  ,0 AS d, 0 AS e
	      FROM( select m_transaction.AD_Org_ID,hh.value ,hh.name ,m_transaction.m_product_id,m_transaction.movementdate,m_transaction.movementtype
	                    ,m_transaction.movementqty*hh.ShelfDepth*hh.ShelfWidth*hh.ShelfHeight/1000000000 as sl
	            from m_transaction 
	            join m_product hh on m_transaction.m_product_id=hh.m_product_id and hh.M_AttributeSet_ID IN ('1000134','1000135','1000131') 
			    where m_transaction.movementdate<=$P{EndDate} and m_transaction.ad_client_id=$P{AD_CLIENT_ID}
	                    AND (m_transaction.m_locator_id in (select m_locator_id from m_locator where AD_Org_ID=$P{AD_Org_ID} ) or $P{AD_Org_ID} is null )
	      		) AS t
	        
	      UNION ALL
	      --Chi tieu 1003
	      SELECT 'III' as nhom,'1003' AS chitieu,0 AS b
	            ,sum(case when t.movementdate<=$P{EndDate} then t.sl else 0 end)-sum(case when t.movementdate<$P{StartDate} then t.sl else 0 end) AS c  ,0 AS d, 0 AS e
	      FROM( select m_transaction.AD_Org_ID,hh.value ,hh.name ,m_transaction.m_product_id,m_transaction.movementdate,m_transaction.movementtype
	                    ,m_transaction.movementqty*hh.ShelfDepth*hh.ShelfWidth*hh.ShelfHeight/1000000000 as sl
	            from m_transaction 
	            join m_product hh on m_transaction.m_product_id=hh.m_product_id and hh.M_AttributeSet_ID IN ('1000130','1000133') 
			    where m_transaction.movementdate<=$P{EndDate} and m_transaction.ad_client_id=$P{AD_CLIENT_ID} 
	                      AND (m_transaction.m_locator_id in (select m_locator_id from m_locator where AD_Org_ID=$P{AD_Org_ID} ) or $P{AD_Org_ID} is null )
	      		) AS t
	
	      --Chi tieu 10031: cot thieu
	      union all
	      
	      SELECT 'III' as nhom,'10031' AS chitieu,0 AS b
	            ,sum(case when t.movementdate<=$P{EndDate} then t.sl else 0 end)-sum(case when t.movementdate<$P{StartDate} then t.sl else 0 end) AS c  ,0 AS d, 0 AS e
	      FROM( select m_transaction.AD_Org_ID,hh.value ,hh.name ,m_transaction.m_product_id,m_transaction.movementdate,m_transaction.movementtype
	                    ,m_transaction.movementqty*hh.ShelfDepth*hh.ShelfWidth*hh.ShelfHeight/1000000000 as sl
	            from m_transaction 
	            join m_product hh on m_transaction.m_product_id=hh.m_product_id and hh.M_AttributeSet_ID IN ('1000132') 
			    where m_transaction.movementdate<=$P{EndDate} and m_transaction.ad_client_id=$P{AD_CLIENT_ID} 
	                      AND (m_transaction.m_locator_id in (select m_locator_id from m_locator where AD_Org_ID=$P{AD_Org_ID} ) or $P{AD_Org_ID} is null )
	      		) AS t
	      		
	      UNION ALL
	
	      --Chi phi san xuat 
	      SELECT 'III' as nhom,'1004' AS chitieu,0 AS b,0 AS c  ,0 AS d, 0 AS e FROM ad_client WHERE ad_client_id=$P{AD_CLIENT_ID}
	
	      UNION ALL
	
	      --nguyên liệu   (2 600 x 1.17) 
	      SELECT 'II' as nhom,'1005' AS chitieu,0 AS b,sum(hsv_dinhmuc1) AS c  ,sum(Amountdg1)  AS d, case when sum(hsv_dinhmuc1)<> 0 then  sum(Amountdg1)/sum(hsv_dinhmuc1) else 0 end AS e 
	      FROM hsv_nhatky_qtri  
	      WHERE ad_client_id=$P{AD_CLIENT_ID}  and dateacct>=$P{StartDate} AND dateacct<=$P{EndDate} and  HSV_solieu_ID=1000013 AND (AD_Org_ID=$P{AD_Org_ID} OR $P{AD_Org_ID} IS NULL)
	      
	      UNION ALL
	      
	      --Ván lót mặt  10051: Xuat su dung voi  M_PartType_ID=1000104
	      SELECT 'II' as nhom,'10051' AS chitieu,0 AS b,0 AS c  ,sum(t.tt) AS d, 0 AS e
	      FROM(         SELECT 0 AS sl,COALESCE(sum(fc.amtacctdr),0) AS tt,'10051' AS chitieu FROM fact_acct fc 
		           		JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id --and left(acct.value,4) in ('6211')
		           		JOIN m_inventory m ON  Fc.record_id=m.m_inventory_id AND m.c_doctype_id IN ('1000326') AND m.docstatus IN ('CO','CL')
		           		JOIN m_product hh ON fc.m_product_id=hh.m_product_id AND hh.M_PartType_ID=1000104
		           		WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id=321 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} 
		           					AND (fc.AD_Org_ID=$P{AD_Org_ID} OR $P{AD_Org_ID} IS NULL) AND fc.C_Activity_ID=1000072
	           			
	      		) AS t
	      
	      UNION all

	      
	      --ván mặt chi tieu 1006: Xuat su dung voi M_PartType_ID=1000008
	      SELECT 'III' as nhom,'1006' AS chitieu,0 AS b,0 AS c  ,sum(t.tt) AS d, 0 AS e
	      FROM(         SELECT hh.value,hh.name,fc.qty AS sl, case when fc.qty >0 then fc.amtacctdr else -1*fc.amtacctcr end as tt,'1006' AS chitieu 
	      				FROM fact_acct fc 
		           		JOIN m_inventory m ON  Fc.record_id=m.m_inventory_id AND m.c_doctype_id = 1000326 AND m.docstatus IN ('CO','CL')
		           		join m_inventoryline mi on  mi.m_inventoryline_id = fc.line_id and fc.qty = mi.qtyinternaluse 
		           		JOIN m_product hh ON fc.m_product_id=hh.m_product_id AND hh.M_PartType_ID=1000008
		           		WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id=321 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} 
		           					AND (fc.AD_Org_ID=$P{AD_Org_ID} OR $P{AD_Org_ID} IS NULL) AND fc.C_Activity_ID=1000072 
	      		) AS t
	      UNION all
	      
	      
	      --keo (115 kg * 8 000) chi tieu 1007:  Xuat su dung voi M_PartType_ID=1000009
	      SELECT 'II' as nhom,'1007' AS chitieu,0 AS b,0 AS c  ,sum(t.tt) AS d, 0 AS e
	      FROM(         SELECT 0 AS sl,COALESCE(sum(fc.amtacctdr),0) AS tt,'1006' AS chitieu FROM fact_acct fc 
		           		JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id --and left(acct.value,4) in ('6211')
		           		JOIN m_inventory m ON  Fc.record_id=m.m_inventory_id AND m.c_doctype_id IN ('1000326') AND m.docstatus IN ('CO','CL')
		           		JOIN m_product hh ON fc.m_product_id=hh.m_product_id AND hh.M_PartType_ID=1000009
		           		WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id=321 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} 
		           					AND (fc.AD_Org_ID=$P{AD_Org_ID} OR $P{AD_Org_ID} IS NULL) AND fc.C_Activity_ID=1000072
	           		
	      		) AS t
			
	      UNION ALL
	
	      --nhuom chi tieu 1008:  Xuat su dung voi M_PartType_ID=1000022
	      SELECT 'II' as nhom,'1008' AS chitieu,0 AS b,0 AS c  ,sum(t.tt) AS d, 0 AS e
	      FROM(         SELECT 0 AS sl,COALESCE(sum(fc.amtacctdr),0) AS tt,'1008' AS chitieu FROM fact_acct fc 
		           		JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id --and left(acct.value,4) in ('6211')
		           		JOIN m_inventory m ON  Fc.record_id=m.m_inventory_id AND m.c_doctype_id IN ('1000326') AND m.docstatus IN ('CO','CL')
		           		JOIN m_product hh ON fc.m_product_id=hh.m_product_id AND hh.M_PartType_ID=1000022
		           		WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id=321 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} 
		           					AND (fc.AD_Org_ID=$P{AD_Org_ID} OR $P{AD_Org_ID} IS NULL) AND fc.C_Activity_ID=1000072
	           		
	      		) AS t
			
	      UNION ALL
	
	       --Ba chi tieu 1009:  Xuat su dung voi M_PartType_ID=1000023
	      SELECT 'II' as nhom,'1009' AS chitieu,0 AS b,0 AS c  ,sum(t.tt) AS d, 0 AS e
	      FROM(         SELECT 0 AS sl,COALESCE(sum(fc.amtacctdr),0) AS tt,'1009' AS chitieu FROM fact_acct fc 
		           		JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id --and left(acct.value,4) in ('6211')
		           		JOIN m_inventory m ON  Fc.record_id=m.m_inventory_id AND m.c_doctype_id IN ('1000326') AND m.docstatus IN ('CO','CL')
		           		JOIN m_product hh ON fc.m_product_id=hh.m_product_id AND hh.M_PartType_ID=1000023
		           		WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id=321 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} 
		           					AND (fc.AD_Org_ID=$P{AD_Org_ID} OR $P{AD_Org_ID} IS NULL) AND fc.C_Activity_ID=1000072
	           		
	           		
	      		) AS t
			
	      UNION ALL
	
	      --Chà nhám chi tieu 1010:  Xuat su dung voi M_PartType_ID=1000087
	      SELECT 'II' as nhom,'1010' AS chitieu,0 AS b,0 AS c  ,sum(t.tt) AS d, 0 AS e
	      FROM(         SELECT 0 AS sl,COALESCE(sum(fc.amtacctdr),0) AS tt,'1009' AS chitieu FROM fact_acct fc 
		           		JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id --and left(acct.value,4) in ('6211')
		           		JOIN m_inventory m ON  Fc.record_id=m.m_inventory_id AND m.c_doctype_id IN ('1000326') AND m.docstatus IN ('CO','CL')
		           		JOIN m_product hh ON fc.m_product_id=hh.m_product_id AND hh.M_PartType_ID=1000087
		           		WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id=321 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} 
		           					AND (fc.AD_Org_ID=$P{AD_Org_ID} OR $P{AD_Org_ID} IS NULL) AND fc.C_Activity_ID=1000072
	           		
	           			
	      		) AS t
			
	      UNION ALL
	
	      --vật tư khác , bao tay ..chi tieu 1011:  Xuat su dung voi M_PartType_ID=1000026
