				) AS t		
	
	
	      UNION ALL
	
	      --Chi nhân công P xưởng: Tai khoan 62711
	      SELECT 'II' as nhom,23 as stt,'1017' AS chitieu  ,sum(t.tt) AS d
	      FROM(        
	              SELECT 0 AS sl,COALESCE(sum(fc.amtacctdr),0)-COALESCE(sum(fc.amtacctcr),0) AS tt,'13' AS chitieu FROM fact_acct fc 
	           		JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,5) IN ('62711') 
	           		WHERE  fc.ad_client_id=$P{AD_CLIENT_ID}  AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} AND fc.C_Activity_ID=1000072
						
	           ) AS t 
	           
	      UNION ALL
	      
	      --Chi phi van chuyen  chi tieu 1018: tai khoan 6412 -day-
	      SELECT 'I' as nhom,24 as stt,'1018' AS chitieu  ,sum(t.tt) AS d
	      FROM(        
	              SELECT 0 AS sl,COALESCE(sum(fc.amtacctdr),0) AS tt,'13' AS chitieu FROM fact_acct fc 
	           		JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,4) IN ('6412') 
	           		WHERE  fc.ad_client_id=$P{AD_CLIENT_ID}  AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} AND fc.C_Activity_ID=1000072
						
	           ) AS t   
	      
	      
	      UNION ALL    
	          
	      --Chi phi ban hang  chi tieu 1019: tai khoan 6411 phân bổ theo doanh thu ban hang van xuat khau cua tung bo phan
	      		--Tong so luong ban toan cong ty
