	<queryString language="SQL">
		<![CDATA[select   stt,id_dm,dm1.name as name_dm, chitieu, name_chitieu, qty,dg,tt
from(
select stt,id_dm , chitieu, dm.name as name_chitieu, qty,dg,tt
from(
---- Chi phí Nguyên  liệu Gỗ, ván cót -----

select '883' as id_dm,* 
from(
select 1 as stt, '884' as chitieu , sum(qty) as qty,case when sum(qty)<>0 then  sum(tt)/sum(qty) else 0 end as dg, sum(tt)  as tt
from(

		select fc.qty as qty,case when fc.qty >0 then fc.amtacctdr else -1*fc.amtacctcr end as tt 
		from fact_acct fc 
		join m_inventory ci on fc.record_id=ci.m_inventory_id  and ci.docstatus in ('CO','CL') and ci.c_doctype_id = 1000326
		join m_inventoryline civ on civ.m_inventory_id = ci.m_inventory_id and civ.qtyinternaluse = fc.qty and civ.m_product_id = fc.m_product_id 
		JOIN m_attributesetinstance att ON civ.m_attributesetinstance_id = att.m_attributesetinstance_id  AND att.M_AttributeSet_ID in (1000065,0) 
		join m_product hh on fc.m_product_id=hh.m_product_id and hh.M_Product_Category_ID=1000084 
		where fc.ad_table_id=321  and ci.ad_client_id=$P{AD_CLIENT_ID}  
		and ci.movementdate<=$P{EndDate} and ci.movementdate>=$P{StartDate}		
) as k

union all 

select 2 as stt,'885' as chitieu,sum(miv.qtyentered) as qty, case when sum(miv.movementqty) <> 0 then sum(civ.linenetamt)/sum(miv.movementqty) end as dg , sum(civ.linenetamt) as tt
from m_inout mi 
join m_inoutline miv on mi.m_inout_id = miv.m_inout_id and mi.docstatus in ('CL','CO') and mi.issotrx ='N'
join c_orderline civ on civ.c_orderline_id = miv.c_orderline_id 
join m_product hh on civ.m_product_id = hh.m_product_id and hh.m_product_category_id = 1000085 and hh.M_AttributeSet_ID in (1000066,1000065)
where mi.ad_client_id = $P{AD_CLIENT_ID} and  mi.dateacct >=$P{StartDate} and mi.dateacct <= $P{EndDate} 


union all 

select 3 as stt,'886' as chitieu,sum(miv.qtyentered) as qty, case when sum(miv.movementqty) <> 0 then sum(civ.linenetamt)/sum(miv.movementqty) end as dg , sum(civ.linenetamt) as tt
from m_inout mi 
join m_inoutline miv on mi.m_inout_id = miv.m_inout_id and mi.docstatus in ('CL','CO') and mi.issotrx ='N'
join c_orderline civ on civ.c_orderline_id = miv.c_orderline_id 
join m_product hh on hh.m_product_id = miv.m_product_id and hh.m_product_category_id = 1000530
where mi.ad_client_id = $P{AD_CLIENT_ID} and  mi.dateacct >=$P{StartDate} and mi.dateacct <= $P{EndDate} 

union all 
select 4 as stt,'887' as chitieu,sum(miv.qtyentered) as qty, case when sum(miv.movementqty) <> 0 then sum(civ.linenetamt)/sum(miv.movementqty) end as dg , sum(civ.linenetamt) as tt
from m_inout mi 
join m_inoutline miv on mi.m_inout_id = miv.m_inout_id and mi.docstatus in ('CL','CO') and mi.issotrx ='N'
join c_orderline civ on civ.c_orderline_id = miv.c_orderline_id 
join m_product hh on hh.m_product_id = miv.m_product_id and hh.M_Product_ID=1013879
where mi.ad_client_id = $P{AD_CLIENT_ID} and  mi.dateacct >=$P{StartDate} and mi.dateacct <= $P{EndDate} 

) as k
union all 

-----Chi phí PX bóc sấy -----------------------
select '889' as dm_id,stt,chitieu, null::int4 as sl, null::int4 as dg, tt
from(
	select 5 as stt,'990' AS chitieu,COALESCE(sum(fc.amtacctdr-fc.amtacctcr),0) AS tt
	FROM fact_acct fc 
	JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,4) ='6275' 
	WHERE  fc.ad_client_id=$P{AD_CLIENT_ID}  AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} AND fc.C_Activity_ID=1000073
	
	union all
	
	select 6 as stt,'991' AS chitieu,COALESCE(sum(fc.amtacctdr-fc.amtacctcr),0) AS tt
	FROM fact_acct fc 
	JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,4) = '6221'
	WHERE  fc.ad_client_id=$P{AD_CLIENT_ID}  AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} AND fc.C_Activity_ID=1000073
	
	union all
	
	select 7 as stt,'992' AS chitieu,COALESCE(sum(fc.amtacctdr-fc.amtacctcr),0) AS tt
	FROM fact_acct fc 
	JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,4) = '6271'
	WHERE  fc.ad_client_id=$P{AD_CLIENT_ID}  AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} AND fc.C_Activity_ID=1000073
	
	union all
	
	select 8 as stt,'993' AS chitieu,COALESCE(sum(fc.amtacctdr-fc.amtacctcr),0) AS tt
	FROM fact_acct fc 
	JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,4) in ('6272','6273')
	WHERE  fc.ad_client_id=$P{AD_CLIENT_ID}  AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} AND fc.C_Activity_ID=1000073
	
	union all
	select 9 as stt,'994' AS chitieu,COALESCE(sum(fc.amtacctdr-fc.amtacctcr),0) AS tt
	FROM fact_acct fc 
	JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,4) = '6276'
	WHERE  fc.ad_client_id=$P{AD_CLIENT_ID}  AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} AND fc.C_Activity_ID=1000073
	union all
	select 10 as stt,'995' AS chitieu,COALESCE(sum(fc.amtacctdr-fc.amtacctcr),0) AS tt
	FROM fact_acct fc 
	JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,4) in ('6277','6278','6412')
	WHERE  fc.ad_client_id=$P{AD_CLIENT_ID}  AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} AND fc.C_Activity_ID=1000073
) as t
union all 
--------------Chi phí vât liệu sản xuất  PX ván ------------------------------------
select  '997' as id_dm,stt,chitieu,sl as qty,dg,tt
	from(
		select stt,t.chitieu,case when t.chitieu = '1006' then  (sl3.c+sl3.c1*gt.hsv_dinhmuc1/100+sl3.c2*gt.hsv_dinhmuc2/100) else 
			case when t.chitieu ='10051' then (sl2.c+sl2.c1*gt.hsv_dinhmuc1/100+sl2.c2*gt.hsv_dinhmuc2/100+sl2.c3*gt.hsv_dinhmuc3/100+sl2.c4*gt.hsv_dinhmuc4/100) else null::int4 end end as sl
		,
		case when (sl3.c+sl3.c1*gt.hsv_dinhmuc1/100+sl3.c2*gt.hsv_dinhmuc2/100)<>0 and t.chitieu = '1006' then t.d/(sl3.c+sl3.c1*gt.hsv_dinhmuc1/100+sl3.c2*gt.hsv_dinhmuc1/100) 
		else case when  (sl2.c+sl2.c1*gt.hsv_dinhmuc1/100+sl2.c2*gt.hsv_dinhmuc2/100+sl2.c3*gt.hsv_dinhmuc3/100+sl2.c4*gt.hsv_dinhmuc4/100)<>0 and t.chitieu='10051' then  t.d/(sl2.c+sl2.c1*gt.hsv_dinhmuc1/100+sl2.c2*gt.hsv_dinhmuc2/100+sl2.c3*gt.hsv_dinhmuc3/100+sl2.c4*gt.hsv_dinhmuc4/100) else null::int4  end end as dg, 
		t.d as tt
		from 
		(

	      SELECT 'II' as nhom, 11 as stt,'10051' AS chitieu  ,sum(t.tt) AS d
	      FROM(         SELECT 0 AS sl,COALESCE(sum(fc.amtacctdr),0) AS tt,'10051' AS chitieu FROM fact_acct fc 
		           		JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id 
		           		JOIN m_inventory m ON  Fc.record_id=m.m_inventory_id AND m.c_doctype_id IN ('1000326') AND m.docstatus IN ('CO','CL')
		           		JOIN m_product hh ON fc.m_product_id=hh.m_product_id AND hh.M_PartType_ID=1000104
		           		WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id=321 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} 
		           					 AND fc.C_Activity_ID=1000072
	           			
	      		) AS t
	      
	      UNION all

	      
	      --ván mặt chi tieu 1006: Xuat su dung voi M_PartType_ID=1000008
