	      		) AS t
			
	      UNION ALL
	
	       --Nhan cong chi tieu 1016: tai khoan 62211  
	      SELECT 'II' as nhom,22 as stt,'1016' AS chitieu  ,sum(t.tt) AS d
	      FROM(        
	              SELECT 0 AS sl,COALESCE(sum(fc.amtacctdr),0)-COALESCE(sum(fc.amtacctcr),0) AS tt,'13' AS chitieu FROM fact_acct fc 
	           		JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,5) IN ('62211') 
