	      		) AS t
			
	      UNION ALL
	
	      --vật tư khác , bao tay ..chi tieu 1011:  Xuat su dung voi M_PartType_ID=1000026
	      SELECT 'II' as nhom,17 as stt,'1011' AS chitieu  ,sum(t.tt) AS d
	      FROM(         SELECT 0 AS sl,COALESCE(sum(fc.amtacctdr),0) AS tt,'1009' AS chitieu FROM fact_acct fc 
		           		JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id --and left(acct.value,4) in ('6211')
		           		JOIN m_inventory m ON  Fc.record_id=m.m_inventory_id AND m.c_doctype_id IN ('1000326') AND m.docstatus IN ('CO','CL')
		           		JOIN m_product hh ON fc.m_product_id=hh.m_product_id AND hh.M_PartType_ID=1000026
		           		WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id=321 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} 
		           					 AND fc.C_Activity_ID=1000072
	           		
	           		) AS t
			
	      UNION ALL
	
	      --xang dầu chi tieu 1012:  Xuat su dung voi M_PartType_ID=1000025
	      SELECT 'II' as nhom,18 as stt,'1012' AS chitieu  ,sum(t.tt) AS d
	      FROM(         SELECT 0 AS sl,COALESCE(sum(fc.amtacctdr),0) AS tt,'1009' AS chitieu FROM fact_acct fc 
		           		JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id --and left(acct.value,4) in ('6211')
		           		JOIN m_inventory m ON  Fc.record_id=m.m_inventory_id AND m.c_doctype_id IN ('1000326') AND m.docstatus IN ('CO','CL')
		           		JOIN m_product hh ON fc.m_product_id=hh.m_product_id AND hh.M_PartType_ID=1000025
		           		WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id=321 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} 
		           					 AND fc.C_Activity_ID=1000072	           			           		
	      		) AS t
			
	      UNION ALL
	      
	      --Tien dien chi tieu 1013: tai khoan 6275  
	      SELECT 'II' as nhom,19 as stt,'1013' AS chitieu  ,sum(t.tt) AS d
	      FROM(        
	              SELECT 0 AS sl,COALESCE(sum(fc.amtacctdr),0)-COALESCE(sum(fc.amtacctcr),0) AS tt,'13' AS chitieu FROM fact_acct fc 
	           		JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,4) IN ('6275')  
	           		WHERE  fc.ad_client_id=$P{AD_CLIENT_ID}  AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} AND fc.C_Activity_ID=1000072						
	      		) AS t
	
	      UNION ALL
	
	      --Bao bi chi tieu 1014:  Xuat su dung voi M_PartType_ID=1000024
	      SELECT 'II' as nhom,20 as stt,'1014' AS chitieu  ,sum(t.tt) AS d
