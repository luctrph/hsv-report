        ) as t 
        join (
			select 1 as id,sum(qty) as qty1 --sl mua-ban van cot quy tam
			from(
				select -1*sum(civ.qtyentered)* hh.shelfdepth *hh.shelfheight *hh.shelfwidth /1000000000 as qty
				from m_inout ci
				join m_inoutline civ on ci.m_inout_id= civ.m_inout_id and ci.docstatus in('CL','CO') and ci.issotrx = 'Y'
				join m_product hh on civ.m_product_id = hh.m_product_id and hh.m_product_category_id = 1000530
