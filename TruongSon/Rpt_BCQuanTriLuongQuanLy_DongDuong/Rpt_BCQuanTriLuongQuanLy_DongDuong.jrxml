              --Chi tieu 131: 13. Lương  kỹ thuật bóc (ván lót mặt)
             union all
             
              select 1 as id, '131' as chitieu,t.AD_Org_ID,l.hsv_luongql_id,t.movementdate as ngay,sum(t.sl) as sl0 
