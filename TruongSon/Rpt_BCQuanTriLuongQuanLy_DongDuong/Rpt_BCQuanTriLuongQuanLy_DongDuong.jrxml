	<queryString language="SQL">
		<![CDATA[select row_number() over() as stt,t.hsv_stt,t.description,t.value,t.name,case when value =2 then sl - qty1 else (sl -qty1) +qty1*0.3 end as sl ,t.hsv_dongialuong ,hsv_dongialuong*case when value =2 then    sl - qty1 else (sl -qty1) +qty1*0.3 end  as luongsx, t.lcd,t.lgv,t.lgt,hsv_dongialuong*case when value =2 then    sl - qty1 else (sl -qty1) +qty1*0.3 end +t.lgv+t.lcd-t.lgt as lcl
from(   
select description , hsv_stt, value,name,sum(sl) as sl,sum(qty1) as qty1,sum(hsv_dongialuong) as hsv_dongialuong,sum(lcd) as lcd,sum(lgv) as lgv,sum(lgt) as lgt
from(

		select description, hsv_stt, value,t1.name,sl ,qty1,hsv_dongialuong,lcd,lgv,lgt
		from(
		
		select dm.description,dm.hsv_stt,cast(dm.value as integer) as value,dm.name,sum(t.sl0) as sl, k.qty1  ,l.hsv_dongialuong, l.hsv_luongcd as lcd,0 as lgv,0 as lgt
