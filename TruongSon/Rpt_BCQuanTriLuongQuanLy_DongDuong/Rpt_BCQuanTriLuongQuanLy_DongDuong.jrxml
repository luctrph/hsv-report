                  where civ.ad_client_id=$P{AD_CLIENT_ID} 
                  group by ci.movementdate,ci.AD_Org_ID
                  ) as t
             join hsv_luongql l on t.chitieu=l.value and ad_client_id=$P{AD_CLIENT_ID}	
             group by  l.hsv_luongql_id,t.movementdate,t.AD_Org_ID	
         
              
             union all 
              
             select 1 as id, '3' as chitieu,t.AD_Org_ID,l.hsv_luongql_id,t.movementdate as ngay,sum(t.sl) as sl0 
