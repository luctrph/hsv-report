        join hsv_luongql dm on t.hsv_luongql_ID=dm.hsv_luongql_ID and ad_client_id=$P{AD_CLIENT_ID} 
        
        --Luong giam tru
        union all

 		select dm.description,dm.hsv_stt,cast(dm.value as integer) as value,dm.name,0 as sl,0 as qty1,0 as hsv_dongialuong, 0 as lcd,0,t.lgt as lgt
		from(	select gv.hsv_luongql_ID,sum(Amount) as lgt
		        from hsv_giamtruLine gvl
		 		join hsv_giamtru gv on gv.hsv_giamtru_id  =gvl.hsv_giamtru_id 
	 			where gv.dateacct>=$P{StartDate} and gv.dateacct<=$P{EndDate} and gv.ad_client_id =$P{AD_CLIENT_ID} and (gv.AD_Org_ID=$P{AD_Org_ID} or $P{AD_Org_ID} is null) 
	 			group by gv.hsv_luongql_ID
 		) as t	
        join hsv_luongql dm on t.hsv_luongql_ID=dm.hsv_luongql_ID and ad_client_id=$P{AD_CLIENT_ID} 

        ) as t
        group by t.hsv_stt,t.description,t.value,t.name 
    ) as t

order by t.hsv_stt asc]]>
