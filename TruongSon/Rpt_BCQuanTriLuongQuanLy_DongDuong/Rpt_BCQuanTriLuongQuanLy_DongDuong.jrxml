                  ) as t
             join hsv_luongql l on t.chitieu=l.value and ad_client_id=$P{AD_CLIENT_ID}	
             group by  l.hsv_luongql_id,t.movementdate	,t.AD_Org_ID	           
             
             --Chi tieu 13:Luong ky thuat boc
             union all
             
              select 1 as id, '13' as chitieu,t.AD_Org_ID,l.hsv_luongql_id,t.movementdate as ngay,sum(t.sl) as sl0 
              from( select P.movementdate,pl.AD_Org_ID,COALESCE(sum(pl.productionqty),0) as sl,'13' as chitieu from m_productionplan pl
                            join m_product hh on pl.m_product_id=hh.m_product_id and hh.M_Product_Category_ID=1000085 and hh.M_AttributeSet_ID in ('1000065')
