	<property name="com.jaspersoft.studio.data.defaultdataadapter" value="adempiere_7.1"/>
	<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
	<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w1" value="70"/>
	<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w2" value="924"/>
	<parameter name="StartDate" class="java.util.Date">
		<defaultValueExpression><![CDATA[new Date(120,0,1)]]></defaultValueExpression>
	</parameter>
	<parameter name="EndDate" class="java.util.Date">
		<defaultValueExpression><![CDATA[new Date(120,0,31)]]></defaultValueExpression>
	</parameter>
	<parameter name="M_Lot_ID" class="java.lang.Integer"/>
	<parameter name="C_Activity_ID" class="java.lang.Integer"/>
	<parameter name="SUBREPORT_DIR" class="java.lang.String">
		<defaultValueExpression><![CDATA["dev/"]]></defaultValueExpression>
	</parameter>
	<parameter name="EXPORT_TYPE" class="java.lang.String"/>
	<parameter name="AD_CLIENT_ID" class="java.lang.Integer"/>
	<parameter name="AD_Org_ID" class="java.lang.Integer"/>
	<parameter name="M_LotCtl_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT  'go' as nhom,c1,c2 as c2,c3 as c3,c4 as c4,c5 as c5,c6 as c6,c7 as c7,c8 as c8,c9 as c9,c10 as c10
,case when $P{M_Lot_ID} is null then  null::int4  else c11 end as  c11,
case when $P{M_Lot_ID} is null then  null::int4  else c12 end as  c12,
case when $P{M_Lot_ID} is null then  null::int4  else c13 end as c13,
case when $P{M_Lot_ID} is null then  null::int4  else c14 end as c14,
 c15,
 c16 as c16,
case when $P{M_Lot_ID} is null then  null::int4  else c17 end as c17
FROM(
SELECT k.c1, sum(c2) AS c2, sum(c3) AS c3, sum(c4) AS c4,
sum(c5) AS c5,
CASE WHEN sum(c5) <> 0 THEN sum(c6) / sum(c5) * 100 ELSE 0 END AS c6,
CASE WHEN sum(c5) <> 0 THEN sum(c7) / sum(c5) * 100 ELSE 0 END AS c7,
CASE WHEN sum(c5) <> 0 THEN sum(c8) / sum(c5) * 100 ELSE 0 END AS c8,
CASE WHEN sum(c5) <> 0 THEN (sum(c2)-sum(c15)) / sum(c5) ELSE 0 END AS c9,
sum(k.c10) - sum(k.c3) AS c10,
sum(c11) as c11,
sum(c12) as c12,
sum(c13) as c13,
sum(c14) as c14,
sum(c15) as c15,
case when sum(c5) <> 0 then sum(c16)/sum(c5) * 100 else 0 end as c16,
sum(c17) as c17
FROM (
	SELECT k.lot AS c1, k.sl AS c2, k.tt AS c3, k.tt / k.sl AS c4, 0 AS c5, 0 AS c6, 0 AS c7, 0 AS c8, 0 AS c9, 0 AS c10,0 as c11,0 as c12,0 as c13, 0 as c14, 0 as c15, 0 as c16,0 as c17
	FROM (
		SELECT t.lot, sum(t.sl) AS sl, sum(t.tt) AS tt
		FROM (
			SELECT att.lot, civ.qtyinvoiced AS sl, civ.priceactual * civ.qtyinvoiced AS tt
			FROM c_invoiceline civ
			JOIN c_invoice ci ON civ.c_invoice_id = ci.c_invoice_id AND ci.issotrx = 'N' AND ci.docstatus IN ('CO', 'CL')
			JOIN m_product hh ON hh.m_product_id = civ.m_product_id AND hh.m_product_category_id = 1000084  AND  hh.M_Product_ID NOT IN (1013026)
			JOIN m_attributesetinstance att ON civ.m_attributesetinstance_id = att.m_attributesetinstance_id AND att.lot IS NOT NULL
			WHERE civ.ad_client_id = $P{AD_CLIENT_ID} 
			) AS t
		JOIN m_lot lo ON lo.name = t.lot AND lo.dateto >= $P{StartDate} AND lo.dateto <= $P{EndDate} AND (lo.M_Lot_ID=$P{M_Lot_ID} or $P{M_Lot_ID} is null)
		JOIN m_lotctl ctl ON ctl.m_lotctl_id = lo.m_lotctl_id AND (lo.m_lotctl_id=$P{M_LotCtl_ID} or $P{M_LotCtl_ID} is null)
		where lo.ad_client_id = $P{AD_CLIENT_ID} and (lo.ad_org_id = $P{AD_Org_ID} or $P{AD_Org_ID} is NULL)
		GROUP BY t.lot
		) AS k
	
	UNION ALL
	

	--- Tổng SL SX C5
	SELECT k.lot AS c1, 0 c2, 0 AS c3, 0 AS c4, sum(k.sl) AS c5, 0 AS c6, 0 AS c7, 0 AS c8, 0 AS c9, 0 AS c10, 0 as c11,0 as c12,0 as c13, 0 as c14, 0 as c15, 0 as c16,0 as c17
	FROM (
		SELECT att.m_attributesetinstance_id, att.lot, civ.productionqty AS sl
		FROM m_productionplan civ
		JOIN m_production pro ON civ.m_production_id = pro.m_production_id AND pro.docstatus IN ('CO', 'CL')
		JOIN m_product hh ON hh.m_product_id = civ.m_product_id AND hh.M_Product_Category_ID = 1000085 
		JOIN m_attributesetinstance att ON civ.m_attributesetinstance_id = att.m_attributesetinstance_id  AND att.M_AttributeSet_ID = 1000065 AND att.lot IS NOT NULL
		WHERE civ.ad_client_id = $P{AD_CLIENT_ID} 
		union all
		SELECT att.m_attributesetinstance_id, att.lot, (civ.productionqty * hh.shelfwidth * hh.shelfdepth * hh.shelfheight)/1000000000 AS sl
		FROM m_productionplan civ
		JOIN m_production pro ON civ.m_production_id = pro.m_production_id AND pro.docstatus IN ('CO', 'CL')
		JOIN m_product hh ON hh.m_product_id = civ.m_product_id AND  hh.M_Product_ID in (1017248,1017259)
		JOIN m_attributesetinstance att ON civ.m_attributesetinstance_id = att.m_attributesetinstance_id  AND att.M_AttributeSet_ID = 1000065 AND att.lot IS NOT NULL
		WHERE civ.ad_client_id = $P{AD_CLIENT_ID} 
		) AS k
	JOIN m_lot lo ON lo.name = k.lot AND lo.dateto >= $P{StartDate} AND lo.dateto <= $P{EndDate} AND (lo.M_Lot_ID=$P{M_Lot_ID} or $P{M_Lot_ID} is null)
	JOIN m_lotctl ctl ON ctl.m_lotctl_id = lo.m_lotctl_id AND (lo.m_lotctl_id=$P{M_LotCtl_ID} or $P{M_LotCtl_ID} is null)
	where lo.ad_client_id = $P{AD_CLIENT_ID} and (lo.ad_org_id = $P{AD_Org_ID} or $P{AD_Org_ID} is NULL)
	group by k.lot
	
	UNION ALL
	 
	--số lượng ván A ,column C6
	SELECT t.lot AS c1, 0 c2, 0 AS c3, 0 AS c4, 0 AS c5, sum(t.sl) AS c6, 0 AS c7, 0 AS c8, 0 AS c9, 0 AS c10,0 as c11,0 as c12,0 as c13, 0 as c14, 0 as c15, 0 as c16,0 as c17
	FROM (
		SELECT att.lot, civ.productionqty AS sl
		FROM m_productionplan civ
		JOIN m_production ci ON civ.m_production_id = ci.m_production_id AND ci.docstatus IN ('CO', 'CL')
		JOIN m_product hh ON civ.m_product_id = hh.m_product_id AND hh.m_product_id = 1004077 
		JOIN m_attributesetinstance att ON civ.m_attributesetinstance_id = att.m_attributesetinstance_id AND att.lot IS NOT NULL
		WHERE civ.ad_client_id = $P{AD_CLIENT_ID} 
		) AS t
	JOIN m_lot lo ON lo.name = t.lot
	JOIN m_lotctl ctl ON ctl.m_lotctl_id = lo.m_lotctl_id AND (lo.m_lotctl_id=$P{M_LotCtl_ID} or $P{M_LotCtl_ID} is null)
	where lo.ad_client_id = $P{AD_CLIENT_ID} and lo.dateto >= $P{StartDate} AND lo.dateto <= $P{EndDate} AND (lo.M_Lot_ID=$P{M_Lot_ID} or $P{M_Lot_ID} is null)
	and (lo.ad_org_id = $P{AD_Org_ID} or $P{AD_Org_ID} is NULL)
	GROUP BY t.lot
	--số lượng van B(C7)
	
	UNION ALL
	
	SELECT t.lot AS c1, 0 c2, 0 AS c3, 0 AS c4, 0 AS c5, 0 AS c6, sum(t.sl) AS c7, 0 AS c8, 0 AS c9, 0 AS c10,0 as c11,0 as c12,0 as c13, 0 as c14, 0 as c15,0 as c16,0 as c17
	FROM (
		SELECT att.lot, civ.productionqty AS sl
		FROM m_productionplan civ
		JOIN m_production ci ON civ.m_production_id = ci.m_production_id AND ci.docstatus IN ('CO', 'CL')
		JOIN m_product hh ON civ.m_product_id = hh.m_product_id AND hh.m_product_id = 1004076
		JOIN m_attributesetinstance att ON civ.m_attributesetinstance_id = att.m_attributesetinstance_id AND att.lot IS NOT NULL
		WHERE civ.ad_client_id = $P{AD_CLIENT_ID} 
		) AS t
	JOIN m_lot lo ON lo.name = t.lot
	JOIN m_lotctl ctl ON ctl.m_lotctl_id = lo.m_lotctl_id AND (lo.m_lotctl_id=$P{M_LotCtl_ID} or $P{M_LotCtl_ID} is null)
	where lo.ad_client_id = $P{AD_CLIENT_ID} and lo.dateto >= $P{StartDate} AND lo.dateto <= $P{EndDate} AND (lo.M_Lot_ID=$P{M_Lot_ID} or $P{M_Lot_ID} is null)
	and (lo.ad_org_id = $P{AD_Org_ID} or $P{AD_Org_ID} is NULL)
	GROUP BY t.lot
	
	
	UNION ALL
	-- số lượng van C(c8)
	SELECT t.lot AS c1, 0 c2, 0 AS c3, 0 AS c4, 0 AS c5, 0 AS c6, 0 AS c7, sum(t.sl) AS c8, 0 AS c9, 0 AS c10,0 as c11,0 as c12,0 as c13, 0 as c14, 0 as c15,0 as c16,0 as c17
	FROM (
		SELECT att.lot, civ.productionqty AS sl
		FROM m_productionplan civ
		JOIN m_production ci ON civ.m_production_id = ci.m_production_id AND ci.docstatus IN ('CO', 'CL')
		JOIN m_product hh ON civ.m_product_id = hh.m_product_id AND hh.m_product_id = 1004079 
		JOIN m_attributesetinstance att ON civ.m_attributesetinstance_id = att.m_attributesetinstance_id AND att.lot IS NOT NULL
		WHERE civ.ad_client_id = $P{AD_CLIENT_ID} 
		) AS t
	JOIN m_lot lo ON lo.name = t.lot
	JOIN m_lotctl ctl ON ctl.m_lotctl_id = lo.m_lotctl_id AND (lo.m_lotctl_id=$P{M_LotCtl_ID} or $P{M_LotCtl_ID} is null)
	where lo.ad_client_id = $P{AD_CLIENT_ID} and lo.dateto >= $P{StartDate} AND lo.dateto <= $P{EndDate} AND (lo.M_Lot_ID=$P{M_Lot_ID} or $P{M_Lot_ID} is null)
	and (lo.ad_org_id = $P{AD_Org_ID} or $P{AD_Org_ID} is NULL)
	GROUP BY t.lot
	union ALL
	--- SL ván lot C16 ---
	SELECT t.lot AS c1, 0 c2, 0 AS c3, 0 AS c4, 0 AS c5, 0 AS c6, 0 AS c7, 0 AS c8, 0 AS c9, 0 AS c10,0 as c11,0 as c12,0 as c13, 0 as c14, 0 as c15,sum(t.sl) AS c16,0 as c17
	FROM (
		SELECT att.lot, (civ.productionqty * hh.shelfwidth * hh.shelfdepth * hh.shelfheight)/1000000000  AS sl
		FROM m_productionplan civ
		JOIN m_production ci ON civ.m_production_id = ci.m_production_id AND ci.docstatus IN ('CO', 'CL')
		JOIN m_product hh ON civ.m_product_id = hh.m_product_id AND hh.M_Product_ID in (1017248,1017259) 
		JOIN m_attributesetinstance att ON civ.m_attributesetinstance_id = att.m_attributesetinstance_id AND att.lot IS NOT NULL
		WHERE civ.ad_client_id = $P{AD_CLIENT_ID} 
		) AS t
	JOIN m_lot lo ON lo.name = t.lot
	JOIN m_lotctl ctl ON ctl.m_lotctl_id = lo.m_lotctl_id AND (lo.m_lotctl_id=$P{M_LotCtl_ID} or $P{M_LotCtl_ID} is null)
	where lo.ad_client_id = $P{AD_CLIENT_ID} and lo.dateto >= $P{StartDate} AND lo.dateto <= $P{EndDate} AND (lo.M_Lot_ID=$P{M_Lot_ID} or $P{M_Lot_ID} is null)
	and (lo.ad_org_id = $P{AD_Org_ID} or $P{AD_Org_ID} is NULL)
	GROUP BY t.lot
	
	UNION all
--Hiệu quả thu mua--
	SELECT t.lot AS c1, 0 c2, 0 AS c3, 0 AS c4, 0 AS c5, 0 AS c6, 0 AS c7, 0 AS c8, 0 AS c9, sum(c10) AS c10,sum(c11) as c11,sum(c12) as c12,sum(c13) as c13, sum(c14) as c14, sum(c15) as c15, 0 as c16, sum(c17) as c17
	FROM (
	--- kết quả van loại khac
		SELECT t.lot, sum(t.hq) AS c10, t.dg as c11, 0 as c12, 0 as c13,0 as c14, sum(t.sl) as c15,0 as c17
		FROM (
				SELECT att.lot, civ.qtyinternaluse as sl,civ.qtyinternaluse * dgl.hsv_dinhmuc AS hq,hsv_dg.enddate,hsv_dg.startdate, 
				case when $P{M_Lot_ID} is null then  null::int4  else dgl.hsv_dinhmuc end as dg
				FROM  m_inventoryline civ
				JOIN m_inventory ci ON civ.m_inventory_id = ci.m_inventory_id AND ci.docstatus IN ('CO', 'CL')
				JOIN m_product hh ON civ.m_product_id = hh.m_product_id AND hh.m_product_id in (1004081,1004083,1004084,1004080)
				join hsv_nkdongialine dgl on hh.m_product_id = dgl.m_product_id 
				join hsv_dongia hsv_dg on hsv_dg.hsv_dongia_id = dgl.hsv_dongia_id and hsv_dg.hsv_loaidongia_id = 1000000
				JOIN m_attributesetinstance att ON civ.m_attributesetinstance_id = att.m_attributesetinstance_id AND att.lot IS NOT NULL
				WHERE civ.ad_client_id = $P{AD_CLIENT_ID} and civ.hsv_xuatkhac='Y'
			) AS t
		JOIN m_lot lo ON lo.name = t.lot
		JOIN m_lotctl ctl ON ctl.m_lotctl_id = lo.m_lotctl_id AND (lo.m_lotctl_id=$P{M_LotCtl_ID} or $P{M_LotCtl_ID} is null)
		WHERE lo.ad_client_id = $P{AD_CLIENT_ID} and lo.dateto >= $P{StartDate} AND lo.dateto <= $P{EndDate} AND (lo.M_Lot_ID=$P{M_Lot_ID} or $P{M_Lot_ID} is null)
		and (lo.ad_org_id = $P{AD_Org_ID} or $P{AD_Org_ID} is NULL) and lo.dateto <= t.enddate and lo.dateto >= t.startdate 
		GROUP BY t.lot,t.dg,t.enddate, lo.dateto,t.startdate 
		union all 
		--- kết quả van loại A
		SELECT t.lot, sum(t.hq) AS c10, 0 as c11, t.dg as c12, 0 as c13,0 as c14,0 as c15,0 as c17
		FROM (
			SELECT att.lot, civ.productionqty * dgl.hsv_dinhmuc AS hq,hsv_dg.enddate,hsv_dg.startdate, 
			case when $P{M_Lot_ID} is null then  null::int4  else dgl.hsv_dinhmuc end as dg
			FROM  m_productionplan civ
			JOIN m_production ci ON civ.m_production_id = ci.m_production_id AND ci.docstatus IN ('CO', 'CL')
			JOIN m_product hh ON civ.m_product_id = hh.m_product_id AND hh.m_product_id = 1004077
			join hsv_nkdongialine dgl on hh.m_product_id = dgl.m_product_id 
			join hsv_dongia hsv_dg on hsv_dg.hsv_dongia_id = dgl.hsv_dongia_id and hsv_dg.hsv_loaidongia_id = 1000000
			JOIN m_attributesetinstance att ON civ.m_attributesetinstance_id = att.m_attributesetinstance_id AND att.lot IS NOT NULL
			WHERE civ.ad_client_id = $P{AD_CLIENT_ID} 
			) AS t
		JOIN m_lot lo ON lo.name = t.lot
		JOIN m_lotctl ctl ON ctl.m_lotctl_id = lo.m_lotctl_id AND (lo.m_lotctl_id=$P{M_LotCtl_ID} or $P{M_LotCtl_ID} is null)
		WHERE  lo.ad_client_id = $P{AD_CLIENT_ID} and lo.dateto >= $P{StartDate} AND lo.dateto <= $P{EndDate} AND (lo.M_Lot_ID=$P{M_Lot_ID} or $P{M_Lot_ID} is null)
		and (lo.ad_org_id = $P{AD_Org_ID} or $P{AD_Org_ID} is NULL) and lo.dateto <= t.enddate and lo.dateto >= t.startdate 
		GROUP BY t.lot,t.dg,t.enddate, lo.dateto,t.startdate 
		-- van B----
		UNION ALL
		
		SELECT t.lot, sum(t.hq) AS c10,0 as c11, 0 as c12, t.dg as c13, 0 as c14, 0 as c15,0 as c17
		FROM (
			SELECT att.lot, civ.productionqty * dgl.hsv_dinhmuc AS hq,hsv_dg.enddate,hsv_dg.startdate, 
			case when $P{M_Lot_ID} is null then  null::int4  else dgl.hsv_dinhmuc end as dg
			FROM  m_productionplan civ
			JOIN m_production ci ON civ.m_production_id = ci.m_production_id AND ci.docstatus IN ('CO', 'CL')
			JOIN m_product hh ON civ.m_product_id = hh.m_product_id AND hh.m_product_id = 1004076
			join hsv_nkdongialine dgl on hh.m_product_id = dgl.m_product_id 
			join hsv_dongia hsv_dg on hsv_dg.hsv_dongia_id = dgl.hsv_dongia_id and hsv_dg.hsv_loaidongia_id = 1000000
			JOIN m_attributesetinstance att ON civ.m_attributesetinstance_id = att.m_attributesetinstance_id AND att.lot IS NOT NULL
			WHERE civ.ad_client_id = $P{AD_CLIENT_ID} 
			) AS t
		JOIN m_lot lo ON lo.name = t.lot
		JOIN m_lotctl ctl ON ctl.m_lotctl_id = lo.m_lotctl_id AND (lo.m_lotctl_id=$P{M_LotCtl_ID} or $P{M_LotCtl_ID} is null)
		WHERE  lo.ad_client_id = $P{AD_CLIENT_ID} and lo.dateto >= $P{StartDate} AND lo.dateto <= $P{EndDate} AND (lo.M_Lot_ID=$P{M_Lot_ID} or $P{M_Lot_ID} is null)
		and (lo.ad_org_id = $P{AD_Org_ID} or $P{AD_Org_ID} is NULL) and lo.dateto <= t.enddate and lo.dateto >= t.startdate 
		GROUP BY t.lot,t.dg,t.enddate, lo.dateto,t.startdate
		--van C---
		
		UNION ALL
		
		SELECT t.lot, sum(t.hq) AS c10,0 as c11, 0 as c12, 0 as c13,t.dg as c14, 0 as c15, 0 as c17
		FROM (
			SELECT att.lot, civ.productionqty * dgl.hsv_dinhmuc AS hq,hsv_dg.enddate,hsv_dg.startdate, 
			case when $P{M_Lot_ID} is null then  null::int4  else dgl.hsv_dinhmuc end as dg
			FROM  m_productionplan civ
			JOIN m_production ci ON civ.m_production_id = ci.m_production_id AND ci.docstatus IN ('CO', 'CL')
			JOIN m_product hh ON civ.m_product_id = hh.m_product_id AND hh.m_product_id = 1004079
			join hsv_nkdongialine dgl on hh.m_product_id = dgl.m_product_id 
			join hsv_dongia hsv_dg on hsv_dg.hsv_dongia_id = dgl.hsv_dongia_id and hsv_dg.hsv_loaidongia_id = 1000000
			JOIN m_attributesetinstance att ON civ.m_attributesetinstance_id = att.m_attributesetinstance_id AND att.lot IS NOT NULL
			WHERE civ.ad_client_id = $P{AD_CLIENT_ID} 
			) AS t
		JOIN m_lot lo ON lo.name = t.lot
		JOIN m_lotctl ctl ON ctl.m_lotctl_id = lo.m_lotctl_id AND (lo.m_lotctl_id=$P{M_LotCtl_ID} or $P{M_LotCtl_ID} is null)
		WHERE  lo.dateto >= $P{StartDate} AND lo.dateto <= $P{EndDate} AND (lo.M_Lot_ID=$P{M_Lot_ID} or $P{M_Lot_ID} is null)
		and lo.ad_client_id = $P{AD_CLIENT_ID} and  (lo.ad_org_id = $P{AD_Org_ID} or $P{AD_Org_ID} is NULL) and lo.dateto <= t.enddate and lo.dateto >= t.startdate 
		GROUP BY t.lot,t.dg,t.enddate, lo.dateto,t.startdate
		union all 
		----VAN LOT ----
		SELECT t.lot, sum(t.hq) AS c10,0 as c11, 0 as c12, 0 as c13,0 as c14, 0 as c15, t.dg as c17
		FROM (
			SELECT att.lot,civ.productionqty  * dgl.hsv_dinhmuc AS hq,hsv_dg.enddate,hsv_dg.startdate, 
			case when $P{M_Lot_ID} is null then  null::int4  else dgl.hsv_dinhmuc end as dg
			FROM  m_productionplan civ
			JOIN m_production ci ON civ.m_production_id = ci.m_production_id AND ci.docstatus IN ('CO', 'CL')
			JOIN m_product hh ON civ.m_product_id = hh.m_product_id AND hh.M_Product_ID in (1017248,1017259)
			join hsv_nkdongialine dgl on hh.m_product_id = dgl.m_product_id 
			join hsv_dongia hsv_dg on hsv_dg.hsv_dongia_id = dgl.hsv_dongia_id and hsv_dg.hsv_loaidongia_id = 1000000
			JOIN m_attributesetinstance att ON civ.m_attributesetinstance_id = att.m_attributesetinstance_id AND att.lot IS NOT NULL
			WHERE civ.ad_client_id = $P{AD_CLIENT_ID} 
			) AS t
		JOIN m_lot lo ON lo.name = t.lot
		JOIN m_lotctl ctl ON ctl.m_lotctl_id = lo.m_lotctl_id AND (lo.m_lotctl_id=$P{M_LotCtl_ID} or $P{M_LotCtl_ID} is null)
		WHERE  lo.dateto >= $P{StartDate} AND lo.dateto <= $P{EndDate} AND (lo.M_Lot_ID=$P{M_Lot_ID} or $P{M_Lot_ID} is null)
		and lo.ad_client_id = $P{AD_CLIENT_ID} and  (lo.ad_org_id = $P{AD_Org_ID} or $P{AD_Org_ID} is NULL) and lo.dateto <= t.enddate and lo.dateto >= t.startdate 
		GROUP BY t.lot,t.dg,t.enddate, lo.dateto,t.startdate
		) AS t
	GROUP BY t.lot
	) AS k
GROUP BY k.c1
)as k]]>
	</queryString>
	<field name="nhom" class="java.lang.String">
		<property name="com.jaspersoft.studio.field.name" value="nhom"/>
		<property name="com.jaspersoft.studio.field.label" value="nhom"/>
