	<queryString language="SQL">
		<![CDATA[select  ROw_number() over()  as stt,sp.name,sum(t.slcv) as slcv,sum(t.tt) as tt
from(   select gv.DocumentNo,gv.Description,t.name as tosx,bp.name as ng,gv.DateAcct,gvc.hsv_sanpham_ID,gvc.qty as slcv,gvc.Amount as tt
        from hsv_giaoviec gv
        join hsv_tosx t on t.hsv_tosx_ID=gv.hsv_tosx_ID
        left join C_BPartner bp on bp.C_BPartner_ID=gv.C_BPartner_ID
        join hsv_pgvLine gvc on gv.hsv_giaoviec_id=gvc.hsv_giaoviec_id
        where gv.ad_client_id=$P{AD_CLIENT_ID} and (gv.AD_Org_ID=$P{AD_Org_ID} or $P{AD_Org_ID} is null) 
                and gv.DateAcct>=$P{StartDate} and gv.DateAcct<=$P{EndDate}
    ) as t
join  hsv_sanpham sp on t.hsv_sanpham_ID=sp.hsv_sanpham_ID
group by sp.name]]>
	</queryString>
