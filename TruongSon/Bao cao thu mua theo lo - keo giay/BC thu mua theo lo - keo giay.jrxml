	<queryString language="SQL">
		<![CDATA[select row_number() over() as stt, name , sum(slm) as slm, sum(ttm) as ttm, sum(slb) as slb, sum(ttb) as ttb
from(
	select ml.name, sum(civ.qtyordered) as slm, sum(civ.linenetamt)  as ttm, 0 as slb, 0 as ttb
	from c_order co 
	join c_orderline civ on co.c_order_id = civ.c_order_id and co.docstatus in ('CO','CL') 
	join m_attributesetinstance att on  att.m_attributesetinstance_id = civ.m_attributesetinstance_id 
	join m_lot ml on ml.m_lot_id = att.m_lot_id and (ml.m_lot_id =$P{M_Lot_ID} or $P{M_Lot_ID} is null)
	where civ.m_product_id =1018217 and civ.ad_client_id = $P{AD_CLIENT_ID} and co.issotrx = 'N' and co.dateacct >= $P{StartDate} and co.dateacct <= $P{EndDate} 
	group by ml.name
	union all 
	select ml.name, 0 as slm, 0 as ttm, sum(civ.qtyordered) as slb, sum(civ.linenetamt)  as ttb
	from c_order co 
	join c_orderline civ on co.c_order_id = civ.c_order_id and co.docstatus in ('CO','CL') and co.issotrx ='Y'
	join m_attributesetinstance att on att.m_attributesetinstance_id = civ.m_attributesetinstance_id 
	join m_lot ml on ml.m_lot_id =att.m_lot_id and (ml.m_lot_id =$P{M_Lot_ID} or $P{M_Lot_ID} is null)
	where civ.m_product_id =1018219 and civ.ad_client_id = $P{AD_CLIENT_ID} and co.dateacct >= $P{StartDate} and co.dateacct <= $P{EndDate}
	group by ml.name 
) as k group by k.name]]>
	</queryString>
