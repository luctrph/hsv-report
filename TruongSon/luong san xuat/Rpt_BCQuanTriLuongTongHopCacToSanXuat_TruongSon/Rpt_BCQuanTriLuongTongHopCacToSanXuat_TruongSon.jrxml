	<parameter name="EXPORT_TYPE" class="java.lang.String"/>
	<parameter name="AD_Org_ID" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[select ROW_NUMBER()  over() as stt,t.tosx,sum(t.sl) as sl,sum(t.luong) as luong,sum(t.luonggv) as luonggv,sum(t.luonggt) as luonggt,sum(t.luong)+sum(t.luonggv)-sum(t.luonggt)  as tc
from(select t.hsv_stt,t.tosx,sum(t.sl) as sl,t.hsv_dongialuong,sum(t.luong) as luong,0 as luonggv,0 as luonggt
     from(   select  tsx.hsv_stt,tsx.name as tosx ,sp.name AS sp,t.DateAcct
                    ,  sum(t.sl) as sl
                    ,l.hsv_dongialuong 
                    ,case when tsx.hsv_loaimat_ID in (1000000,1000064) then sum(t.sl) * sp.ShelfDepth*sp.ShelfWidth*sp.ShelfHeight else sum(t.sl) end*l.hsv_dongialuong as luong
                     
            from(  
            	select nk.AD_Org_ID ,nk.hsv_tosx_ID ,nkl.hsv_sanpham_ID,nk.DateAcct ,sum(nkl.QtyOther +nkl.Qty_tam) as sl                                       
                from hsv_nhatkysx nk
                join hsv_nhatkysxline nkl on nk.hsv_nhatkysx_id=nkl.hsv_nhatkysx_id
                where nk.ad_client_id=$P{AD_CLIENT_ID}  
                      and (nk.AD_Org_ID=$P{AD_Org_ID} or $P{AD_Org_ID} is null)   
                      and nk.DateAcct>=$P{StartDate} and nk.DateAcct<=$P{EndDate}  and nk.hsv_tosx_ID not in (1000006, 1000024,1000246,1000195)
                group by nk.AD_Org_ID ,nk.hsv_tosx_ID, nkl.hsv_sanpham_ID   ,nk.DateAcct
                   
                --To xe nang lay so lieu tu to hsv_tosx_ID=1000021
                union all
                
                select nk.AD_Org_ID ,case when nk.ad_client_id = 1000006 then 1000067  else  1000217 end as hsv_tosx_ID,nkl.hsv_sanpham_ID,nk.DateAcct ,sum(nkl.QtyOther +nkl.Qty_tam) as sl                                        
                from hsv_nhatkysx nk
                join hsv_nhatkysxline nkl on nk.hsv_nhatkysx_id=nkl.hsv_nhatkysx_id
                join hsv_tosx tsx on nk.hsv_tosx_id =tsx.hsv_tosx_id and tsx.hsv_loaimat_ID in  (1000003,1000067)
                where nk.ad_client_id=$P{AD_CLIENT_ID}  
                      and (nk.AD_Org_ID=$P{AD_Org_ID} or $P{AD_Org_ID} is null)   
                      and nk.DateAcct>=$P{StartDate} and nk.DateAcct<=$P{EndDate} 
                group by nk.AD_Org_ID ,nk.hsv_tosx_ID, nkl.hsv_sanpham_ID,nk.DateAcct, nk.ad_client_id
                
                --To lo dau lay so lieu tu to hsv_tosx_ID=1000009,1000038
                union all
                
                select nk.AD_Org_ID,case when nk.ad_client_id = 1000006 then 1000034 else   1000205 end as hsv_tosx_ID  ,nkl.hsv_sanpham_ID,nk.DateAcct,sum(nkl.QtyOther +nkl.Qty_tam) as sl                                        
                from hsv_nhatkysx nk
                join hsv_nhatkysxline nkl on nk.hsv_nhatkysx_id=nkl.hsv_nhatkysx_id
                join hsv_tosx tsx on nk.hsv_tosx_id =tsx.hsv_tosx_id and tsx.hsv_loaimat_ID in  (1000003,1000067)          
                where nk.ad_client_id=$P{AD_CLIENT_ID}  
                      and (nk.AD_Org_ID=$P{AD_Org_ID} or $P{AD_Org_ID} is null)   
                      and nk.DateAcct>=$P{StartDate} and nk.DateAcct<=$P{EndDate} 
                group by nk.AD_Org_ID ,nk.hsv_tosx_ID, nkl.hsv_sanpham_ID      ,nk.DateAcct, nk.ad_client_id
                
                --To pha keo tang lop lay so lieu tu to hsv_tosx_ID=1000013,1000016
                union all
                
                select nk.AD_Org_ID ,1000024 as hsv_tosx_ID ,nkl.hsv_sanpham_ID ,nk.DateAcct,sum(nkl.QtyOther +nkl.Qty_tam) as sl                                        
                from hsv_nhatkysx nk
                join hsv_nhatkysxline nkl on nk.hsv_nhatkysx_id=nkl.hsv_nhatkysx_id
                join hsv_tosx tsx on nk.hsv_tosx_id =tsx.hsv_tosx_id and tsx.hsv_loaimat_ID in (1000001,1000011) 
                where nk.ad_client_id=$P{AD_CLIENT_ID}  
                      and (nk.AD_Org_ID=$P{AD_Org_ID} or $P{AD_Org_ID} is null)   
                      and nk.DateAcct>=$P{StartDate} and nk.DateAcct<=$P{EndDate} 
                group by nk.AD_Org_ID,nk.hsv_tosx_ID, nkl.hsv_sanpham_ID      ,nk.DateAcct
                
                --To pha keo xep cot lay so lieu tu to hsv_tosx_ID=1000005
                union all
                
                select nk.AD_Org_ID,1000006 as hsv_tosx_ID ,nkl.hsv_sanpham_ID ,nk.DateAcct,sum(nkl.QtyOther +nkl.Qty_tam) as sl                                        
                from hsv_nhatkysx nk
                join hsv_nhatkysxline nkl on nk.hsv_nhatkysx_id=nkl.hsv_nhatkysx_id
                join hsv_tosx tsx on nk.hsv_tosx_id =tsx.hsv_tosx_id and tsx.hsv_loaimat_ID in (1000000)                 
                where nk.ad_client_id=$P{AD_CLIENT_ID}  
                      and (nk.AD_Org_ID=$P{AD_Org_ID} or $P{AD_Org_ID} is null)   
                      and nk.DateAcct>=$P{StartDate} and nk.DateAcct<=$P{EndDate}
                group by nk.AD_Org_ID ,nk.hsv_tosx_ID, nkl.hsv_sanpham_ID      ,nk.DateAcct
                             union all
                --- to lan keo
             select nk.AD_Org_ID,case when nk.ad_client_id = 1000006  then 1000252 end  as hsv_tosx_ID ,nkl.hsv_sanpham_ID ,nk.DateAcct,(nkl.QtyOther +nkl.Qty_tam)* sp.ShelfDepth*sp.ShelfWidth*sp.ShelfHeight as sl                                        
                from hsv_nhatkysx nk
                join hsv_nhatkysxline nkl on nk.hsv_nhatkysx_id=nkl.hsv_nhatkysx_id
                join hsv_sanpham sp on nkl.hsv_sanpham_ID=sp.hsv_sanpham_ID 
                join hsv_tosx tsx on nk.hsv_tosx_id =tsx.hsv_tosx_id and tsx.hsv_loaimat_ID in (1000064,1000000)                 
                where nk.ad_client_id=$P{AD_CLIENT_ID}  
                      and (nk.AD_Org_ID=$P{AD_Org_ID} or $P{AD_Org_ID} is null)   
                      and nk.DateAcct>=$P{StartDate} and nk.DateAcct<=$P{EndDate}
                             
                             
                
                --Tổ pha bôt bả cốt   (hsv_tosx_ID=1000023)
                union all
                  select nk.AD_Org_ID,case when nk.ad_client_id = 1000006  then 1000247 else 1000246 end  as hsv_tosx_ID ,nkl.hsv_sanpham_ID ,nk.DateAcct,(nkl.QtyOther +nkl.Qty_tam)* sp.ShelfDepth*sp.ShelfWidth*sp.ShelfHeight as sl                                        
                from hsv_nhatkysx nk
                join hsv_nhatkysxline nkl on nk.hsv_nhatkysx_id=nkl.hsv_nhatkysx_id
                join hsv_sanpham sp on nkl.hsv_sanpham_ID=sp.hsv_sanpham_ID 
                join hsv_tosx tsx on nk.hsv_tosx_id =tsx.hsv_tosx_id and tsx.hsv_loaimat_ID in (1000064,1000000)                 
                where nk.ad_client_id=$P{AD_CLIENT_ID}  
                      and (nk.AD_Org_ID=$P{AD_Org_ID} or $P{AD_Org_ID} is null)   
                      and nk.DateAcct>=$P{StartDate} and nk.DateAcct<=$P{EndDate}

                union all
                select nk.AD_Org_ID,case when nk.ad_client_id = 1000006 then  1000023  else 1000194 end as hsv_tosx_ID ,nkl.hsv_sanpham_ID ,nk.DateAcct,sum(nkl.QtyOther +nkl.Qty_tam) as sl                                       
                from hsv_nhatkysx nk
                join hsv_nhatkysxline nkl on nk.hsv_nhatkysx_id=nkl.hsv_nhatkysx_id
                join hsv_tosx tsx on nk.hsv_tosx_id =tsx.hsv_tosx_id and tsx.hsv_loaimat_ID in (1000004,1000068)           
                where nk.ad_client_id=$P{AD_CLIENT_ID}  and (nk.AD_Org_ID=$P{AD_Org_ID} or $P{AD_Org_ID} is null)   
                      and nk.DateAcct>=$P{StartDate} and nk.DateAcct<=$P{EndDate} 
                group by nk.AD_Org_ID ,nk.hsv_tosx_ID,tsx.hsv_loaimat_ID, nkl.hsv_sanpham_ID,nk.DateAcct, nk.ad_client_id
                
                --To tổ xe gắp xe nâng nguyên liệu   (SP bóc)   hsv_tosx_ID=1000099 them ngay 3/11/2020
                union all
                
                select nk.AD_Org_ID,case when nk.ad_client_id  = 1000006 then 1000099 else 1000238 end  as hsv_tosx_ID ,nkl.hsv_sanpham_ID ,nk.DateAcct,sum(nkl.QtyOther +nkl.Qty_tam) as sl                                       
                from hsv_nhatkysx nk
                join hsv_nhatkysxline nkl on nk.hsv_nhatkysx_id=nkl.hsv_nhatkysx_id
                join hsv_tosx tsx on nk.hsv_tosx_id =tsx.hsv_tosx_id and tsx.hsv_loaimat_ID in (1000051, 1000080)                
                where nk.ad_client_id=$P{AD_CLIENT_ID}  and (nk.AD_Org_ID=$P{AD_Org_ID} or $P{AD_Org_ID} is null)   
                      and nk.DateAcct>=$P{StartDate} and nk.DateAcct<=$P{EndDate} 
                group by nk.AD_Org_ID ,nk.hsv_tosx_ID,tsx.hsv_loaimat_ID, nkl.hsv_sanpham_ID,nk.DateAcct, nk.ad_client_id
                
                --tổ xe gắp xe nâng nguyên liệu   (SP sấy     hsv_tosx_ID=1000098 them ngay 3/11/2020
                union all
                
                select nk.AD_Org_ID,case when nk.ad_client_id = 1000006 then 1000098 else 1000237 end as hsv_tosx_ID ,nkl.hsv_sanpham_ID,nk.DateAcct,sum(nkl.QtyOther +nkl.Qty_tam) as sl                                       
                from hsv_nhatkysx nk
                join hsv_nhatkysxline nkl on nk.hsv_nhatkysx_id=nkl.hsv_nhatkysx_id
                join hsv_tosx tsx on nk.hsv_tosx_id =tsx.hsv_tosx_id and tsx.hsv_loaimat_ID in (1000052,1000081)                 
                where nk.ad_client_id=$P{AD_CLIENT_ID}  and (nk.AD_Org_ID=$P{AD_Org_ID} or $P{AD_Org_ID} is null)   
                      and nk.DateAcct>=$P{StartDate} and nk.DateAcct<=$P{EndDate} 
                group by nk.AD_Org_ID ,nk.hsv_tosx_ID,tsx.hsv_loaimat_ID, nkl.hsv_sanpham_ID,nk.DateAcct, nk.ad_client_id
                
                --Tổ:  "6.9 Tổ đốt lò sấy (đẩy kéo xe)"  có  hsv_tosx_ID=1000113
                 union all
                
                select nk.AD_Org_ID,case when nk.ad_client_id = 1000006 then 1000113 else 1000241 end as hsv_tosx_ID ,nkl.hsv_sanpham_ID ,nk.DateAcct,sum(nkl.QtyOther +nkl.Qty_tam) as sl                                       
                from hsv_nhatkysx nk
                join hsv_nhatkysxline nkl on nk.hsv_nhatkysx_id=nkl.hsv_nhatkysx_id
                join hsv_tosx tsx on nk.hsv_tosx_id =tsx.hsv_tosx_id and tsx.hsv_loaimat_ID in (1000052,1000081)              
                where nk.ad_client_id=$P{AD_CLIENT_ID}  and (nk.AD_Org_ID=$P{AD_Org_ID} or $P{AD_Org_ID} is null)   
                      and nk.DateAcct>=$P{StartDate} and nk.DateAcct<=$P{EndDate} 
                group by nk.AD_Org_ID ,nk.hsv_tosx_ID,tsx.hsv_loaimat_ID, nkl.hsv_sanpham_ID,nk.DateAcct, nk.ad_client_id
                
                    
                 ) as t
            join hsv_sanpham sp on t.hsv_sanpham_ID=sp.hsv_sanpham_ID              
            join (select l.EndDate,l.StartDate,ln.hsv_tosx_ID ,ln.hsv_sanpham_ID,ln.hsv_dongialuong,ln.AD_Org_ID   
                  from  hsv_dgluongLine ln
                  join hsv_dgluong l on l.hsv_loaidongia_ID in(1000001,1000002,1000008,1000022,1000043) and l.hsv_dgluong_ID=ln.hsv_dgluong_ID and l.ad_client_id=$P{AD_CLIENT_ID}
                 ) as l on t.hsv_sanpham_ID=l.hsv_sanpham_ID and t.hsv_tosx_ID=l.hsv_tosx_ID  and t.DateAcct>=l.StartDate and t.DateAcct<=l.EndDate  and t.AD_Org_ID=l.AD_Org_ID   
            join hsv_tosx tsx on t.hsv_tosx_ID=tsx.hsv_tosx_ID 
            group by tsx.hsv_stt,tsx.name,tsx.hsv_loaimat_ID ,sp.name,t.DateAcct,sp.ShelfDepth,sp.ShelfWidth,sp.ShelfHeight,l.hsv_dongialuong 
        ) as t   
    group by  t.hsv_stt,t.tosx,t.hsv_dongialuong
	
    --Luong tu giao viec
	union all
                
    select tsx.hsv_stt,tsx.name as tosx,0 as sl,0 as hsv_dongialuong,0 as luong,coalesce(sum(nkl.Amount),0) as luonggv,0 as luonggt                                       
    from hsv_giaoviec nk
    join hsv_pgvLine nkl on nk.hsv_giaoviec_id=nkl.hsv_giaoviec_id
    join hsv_tosx tsx on nk.hsv_tosx_id =tsx.hsv_tosx_id                  
    where nk.ad_client_id=$P{AD_CLIENT_ID}  and (nk.AD_Org_ID=$P{AD_Org_ID} or $P{AD_Org_ID} is null)   
                 and nk.DateAcct>=$P{StartDate} and nk.DateAcct<=$P{EndDate} 
    group by tsx.hsv_stt,tsx.name
    
     --Luong giam tru
    
   union all
               
   select tsx.hsv_stt ,tsx.name as tosx,0 as sl,0 as hsv_dongialuong,0 as luong,0 as luonggv,coalesce(sum(nkl.Amount),0) as luonggt                                       
   from hsv_giamtru nk
   join hsv_giamtruLine nkl on nk.hsv_giamtru_id=nkl.hsv_giamtru_id
   join hsv_tosx tsx on nk.hsv_tosx_id =tsx.hsv_tosx_id                  
   where nk.ad_client_id=$P{AD_CLIENT_ID}  and (nk.AD_Org_ID=$P{AD_Org_ID} or $P{AD_Org_ID} is null)   
           and nk.DateAcct>=$P{StartDate} and nk.DateAcct<=$P{EndDate} 
   group by tsx.hsv_stt,tsx.name
   
    ) as t
group by   t.hsv_stt,  t.tosx
order by t.hsv_stt
]]>
	</queryString>
