	            SELECT 'I' as nhom,'1000' AS chitieu,0 AS b,COALESCE(sum(t.sl)) AS c,COALESCE(sum(t.tt)) AS d,sum(CASE WHEN t.sl<>0 THEN t.tt/t.sl ELSE 0 END) AS e
	                  FROM (SELECT fc.account_id,COALESCE(sum(fc.amtacctcr),0)-COALESCE(sum(fc.amtacctdr),0) AS tt,-1*COALESCE(sum(fc.qty),0) AS sl FROM fact_acct fc 
	                                JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,2)='51'
	                                JOIN M_product hh ON fc.m_product_id=hh.m_product_id AND hh.M_AttributeSet_ID IN ('1000064','1000136') 
	                                WHERE  fc.ad_client_id=1000006 AND fc.ad_table_id<>224 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} AND (fc.AD_Org_ID=$P{AD_Org_ID} OR $P{AD_Org_ID} IS NULL) AND fc.C_Activity_ID=1000072
	                                GROUP BY fc.account_id
	                            
	                              UNION ALL
	                            
	                              SELECT fc.account_id,COALESCE(sum(fc.amtacctcr),0)-COALESCE(sum(fc.amtacctdr),0) AS tt,-1*COALESCE(sum(fc.qty),0) AS sl  FROM fact_acct fc 
	                                JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,2)='51'
	                                JOIN gl_journalline gl ON  Fc.record_id=gl.gl_journal_id AND Fc.line_id=gl.gl_journalline_id AND Fc.Account_ID=gl.Account_ID
	                                               AND gl.IsGenerated='N'
	                                JOIN M_product hh ON fc.m_product_id=hh.m_product_id AND hh.M_AttributeSet_ID IN ('1000064','1000136') 
	                                WHERE  fc.ad_client_id=1000006 AND fc.ad_table_id=224 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} AND (fc.AD_Org_ID=$P{AD_Org_ID} OR $P{AD_Org_ID} IS NULL) AND fc.C_Activity_ID=1000072
	                              GROUP BY fc.account_id
	  
	            
	           ) AS t        
	
	) as sl1 on t.nhom=sl1.nhom
	--So luong san xuat kiem lan 1(M3)
	 left join (     
	            SELECT 'II' as nhom,'1002' AS chitieu,0 AS b,sum(t.sl) AS c  ,sum(t.sl1) as c1 , sum(t.sl2) as c2, sum(t.sl3) as c3, sum(t.sl4) as c4
		     	FROM(   --Chi tieu 999
				      	SELECT fc.account_id,-1*COALESCE(sum(fc.qty*hh.ShelfWidth *hh.ShelfDepth *hh.ShelfHeight),0)/1000000000 AS sl,0 as sl1,0 as sl2,0 as sl3,0 as sl4 
	                    FROM fact_acct fc 
	            	    JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,2)='51'
	            	    JOIN M_product hh ON fc.m_product_id=hh.m_product_id AND hh.M_AttributeSet_ID IN ('1000130','1000131') 
	               		WHERE  fc.ad_client_id=1000006 AND fc.ad_table_id<>224 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} 
	                                AND (fc.AD_Org_ID=$P{AD_Org_ID} OR $P{AD_Org_ID} IS NULL) AND fc.C_Activity_ID=1000072
	            	    GROUP BY fc.account_id
					
	                    UNION ALL
					
	                    SELECT fc.account_id,-1*COALESCE(sum(fc.qty*hh.ShelfWidth *hh.ShelfDepth *hh.ShelfHeight),0)/1000000000 AS sl,0 as sl1,0 as sl2,0 as sl3,0 as sl4  
	                    FROM fact_acct fc 
	               		JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,2)='51'
	               		JOIN gl_journalline gl ON  Fc.record_id=gl.gl_journal_id AND Fc.line_id=gl.gl_journalline_id AND Fc.Account_ID=gl.Account_ID
	                                   AND gl.IsGenerated='N'
	             	    JOIN M_product hh ON fc.m_product_id=hh.m_product_id AND hh.M_AttributeSet_ID IN ('1000130','1000131') 
	               		WHERE  fc.ad_client_id=1000006 AND fc.ad_table_id=224 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} 
	                            AND (fc.AD_Org_ID=$P{AD_Org_ID} OR $P{AD_Org_ID} IS NULL) AND fc.C_Activity_ID=1000072
	                    GROUP BY fc.account_id
	                    
	                    --Chi tieu 1000
	                    union all
	                    
	                    SELECT fc.account_id,-1*COALESCE(sum(fc.qty),0) AS sl,0 as sl1,0 as sl2,0 as sl3,0 as sl4
	                    FROM fact_acct fc 
	            	    JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,2)='51'
	            	    JOIN M_product hh ON fc.m_product_id=hh.m_product_id AND hh.M_AttributeSet_ID IN ('1000064','1000136') 
	               		WHERE  fc.ad_client_id=1000006 AND fc.ad_table_id<>224 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} 
	                            AND (fc.AD_Org_ID=$P{AD_Org_ID} OR $P{AD_Org_ID} IS NULL) AND fc.C_Activity_ID=1000072
	            	    GROUP BY fc.account_id
					
	                   UNION ALL
					
	                    SELECT fc.account_id,-1*COALESCE(sum(fc.qty),0) AS sl,0 as sl1,0 as sl2,0 as sl3,0 as sl4
	                    FROM fact_acct fc 
	               		JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,2)='51'
	               		JOIN gl_journalline gl ON  Fc.record_id=gl.gl_journal_id AND Fc.line_id=gl.gl_journalline_id AND Fc.Account_ID=gl.Account_ID
	                                   AND gl.IsGenerated='N'
	             	    JOIN M_product hh ON fc.m_product_id=hh.m_product_id AND hh.M_AttributeSet_ID IN ('1000064','1000136') 
	               		WHERE  fc.ad_client_id=1000006 AND fc.ad_table_id=224 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} 
	                            AND (fc.AD_Org_ID=$P{AD_Org_ID} OR $P{AD_Org_ID} IS NULL) AND fc.C_Activity_ID=1000072
	                    GROUP BY fc.account_id
	                   
	                    --Chi tieu 1001: Tinh theo % phan bo hsv_dinhmuc1
	                    
	                    union all
	                    
	                   	--select t.AD_Org_ID,t.sl*gt.hsv_dinhmuc1/100 as sl
	                   	select t.AD_Org_ID,0 as sl,t.sl as sl1,0 as sl2,0 as sl3,0 as sl4
					    from(	select '1001' as ct,m.AD_Org_ID
		                            ,sum(case when m.movementdate<=$P{EndDate} then m.movementqty else 0 end)-sum(case when m.movementdate<$P{StartDate} then m.movementqty else 0 end) AS sl
			                    from m_transaction m
			                    join m_product hh on m.m_product_id=hh.m_product_id and hh.M_AttributeSet_ID IN ('1000064','1000136') 
			                    where m.movementdate<=$P{EndDate} and m.ad_client_id=$P{AD_CLIENT_ID} 
						                AND (m.m_locator_id in (select m_locator_id from m_locator where AD_Org_ID=$P{AD_Org_ID} ) or $P{AD_Org_ID} is null )
			                    group by m.AD_Org_ID
			                 ) as t   
	                    JOIN hsv_giathanh gt ON t.ct=gt.value AND gt.AD_CLIENT_ID=$P{AD_CLIENT_ID}
	                    
	                     --Chi tieu 1002: tinh theo % phan bo        
				      	union all
				      	
	--					select t.AD_Org_ID,t.sl*gt.hsv_dinhmuc2/100 as sl
						select t.AD_Org_ID,0 as sl,0 as sl1,t.sl as sl2,0 as sl3,0 as sl4
					    from(  select '1002' as ct,m.AD_Org_ID
			                            ,sum(case when m.movementdate<=$P{EndDate} then m.movementqty*hh.ShelfDepth*hh.ShelfWidth*hh.ShelfHeight end )/1000000000 
			                            -sum(case when m.movementdate<$P{StartDate} then m.movementqty*hh.ShelfDepth*hh.ShelfWidth*hh.ShelfHeight end )/1000000000 as sl
			                    from m_transaction m
			                    join m_product hh on m.m_product_id=hh.m_product_id and hh.M_AttributeSet_ID IN ('1000134','1000135','1000131') 
			                    where m.movementdate<=$P{EndDate} and m.ad_client_id=$P{AD_CLIENT_ID} 
			                            AND (m.m_locator_id in (select m_locator_id from m_locator where AD_Org_ID=$P{AD_Org_ID} ) or $P{AD_Org_ID} is null )
			                    group by m.AD_Org_ID
		                    ) as t
	                    JOIN hsv_giathanh gt ON t.ct=gt.value AND gt.AD_CLIENT_ID=$P{AD_CLIENT_ID}
	                                                
	                    --Chi tieu 1003
	                    union all
	                    
						--select 0 as b,t1.sl*gt.hsv_dinhmuc3/100  as sl
						select 0 as b,0 as sl,0 as sl1,0 as sl2,t1.sl as sl3,0 as sl4
	                    from(	SELECT '1003' as ct,0 AS b,sum(case when t.movementdate<=$P{EndDate} then t.sl else 0 end)-sum(case when t.movementdate<$P{StartDate} then t.sl else 0 end) AS sl
			      				FROM( select m_transaction.AD_Org_ID,hh.value ,hh.name ,m_transaction.m_product_id,m_transaction.movementdate,m_transaction.movementtype
			                    						,m_transaction.movementqty*hh.ShelfDepth*hh.ShelfWidth*hh.ShelfHeight/1000000000 as sl
			            			  from m_transaction 
			  						  join m_product hh on m_transaction.m_product_id=hh.m_product_id and hh.M_AttributeSet_ID IN ('1000130','1000133') 
									  where m_transaction.movementdate<=$P{EndDate} and m_transaction.ad_client_id=$P{AD_CLIENT_ID} 
							                      AND (m_transaction.m_locator_id in (select m_locator_id from m_locator where AD_Org_ID=$P{AD_Org_ID} ) or $P{AD_Org_ID} is null )
						      		) AS t
						      ) as t1		
	                    JOIN hsv_giathanh gt ON t1.ct=gt.value AND gt.AD_CLIENT_ID=$P{AD_CLIENT_ID}
	                    
	                    --Chi tieu 10031:
	                    union all
	                    
						--select 0 as b,t1.sl*gt.hsv_dinhmuc4/100  as sl
						select 0 as b,0 as sl,0 as sl1,0 as sl2,0 as sl3,t1.sl as sl4
	                    from(	SELECT '10031' as ct,0 AS b,sum(case when t.movementdate<=$P{EndDate} then t.sl else 0 end)-sum(case when t.movementdate<$P{StartDate} then t.sl else 0 end) AS sl
			      				FROM( select m_transaction.AD_Org_ID,hh.value ,hh.name ,m_transaction.m_product_id,m_transaction.movementdate,m_transaction.movementtype
			                    						,m_transaction.movementqty*hh.ShelfDepth*hh.ShelfWidth*hh.ShelfHeight/1000000000 as sl
			            			  from m_transaction 
			  						  join m_product hh on m_transaction.m_product_id=hh.m_product_id and hh.M_AttributeSet_ID IN ('1000132') 
									  where m_transaction.movementdate<=$P{EndDate} and m_transaction.ad_client_id=$P{AD_CLIENT_ID} 
