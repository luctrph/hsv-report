) as sl1 on t.nhom=sl1.nhom
--So luong dung de tinh chi phi san xuat
 left join (     
            SELECT 'II' as nhom,'1002' AS chitieu,0 AS b,sum(t.sl) AS c  ,sum(t.sl1) as c1 , sum(t.sl2) as c2, sum(t.sl3) as c3, sum(t.sl4) as c4
