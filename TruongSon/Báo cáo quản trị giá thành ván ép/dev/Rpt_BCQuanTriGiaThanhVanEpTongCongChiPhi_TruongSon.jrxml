		                    from m_transaction m
		                    join m_product hh on m.m_product_id=hh.m_product_id and hh.M_AttributeSet_ID IN ('1000134','1000135','1000131') 
		                    where m.movementdate<=$P{EndDate} and m.ad_client_id=$P{AD_CLIENT_ID} 
