                            AND (fc.AD_Org_ID=$P{AD_Org_ID} OR $P{AD_Org_ID} IS NULL) AND fc.C_Activity_ID=1000072
                    GROUP BY fc.account_id
                   
                    --Chi tieu 1001: Tinh theo % phan bo hsv_dinhmuc1
                    
                    union all
                    
                   	--select t.AD_Org_ID,t.sl*gt.hsv_dinhmuc1/100 as sl
                   	select t.AD_Org_ID,0 as sl,t.sl as sl1,0 as sl2,0 as sl3,0 as sl4
				    from(	select '1001' as ct,m.AD_Org_ID
	                            ,sum(case when m.movementdate<=$P{EndDate} then m.movementqty else 0 end)-sum(case when m.movementdate<$P{StartDate} then m.movementqty else 0 end) AS sl
		                    from m_transaction m
		                    join m_product hh on m.m_product_id=hh.m_product_id and hh.M_AttributeSet_ID IN ('1000064','1000136') 
		                    where m.movementdate<=$P{EndDate} and m.ad_client_id=$P{AD_CLIENT_ID} 
