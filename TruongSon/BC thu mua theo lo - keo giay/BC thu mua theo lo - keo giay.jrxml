	<queryString language="SQL">
		<![CDATA[select row_number() over() as stt, lot,sum(slm) as slm, sum(ttm) as ttm, sum(c5) as cp_dm, sum(c6) as nhap_cl, sum(c7) as xuat_cl, sum(c8) as tp_sx,sum(c9) as slb, sum(c10) as ttb, sum(c11)*0.17*500 as tien_ppdm, case when sum(c8)<>0 then sum(c11)/sum(c8) else 0 end  as tyle
,sum(c12)+sum(c11)*0.17*500 as hq
from(
				---2---
		select  att.lot, civ.qtyentered  as slm, civ.linenetamt  as ttm, 0 as c5, 0 as c6,0 as c7, 0 as c8,0 as c9, 0 as c10 ,civ.qtyentered as c11	,-1*civ.linenetamt as c12		
		from c_invoice ci  
		join c_invoiceline civ  on ci.c_invoice_id  = civ.c_invoice_id  and ci.issotrx = 'N'
		join M_AttributeSetInstance att on att.m_attributesetinstance_id = civ.m_attributesetinstance_id 
		join m_lot lot on lot.m_lot_id = att.m_lot_id
		join m_product hh on hh.m_product_id = civ.m_product_id 
		where ci.ad_client_id = $P{AD_CLIENT_ID} and ci.docstatus in ('CO','CL')
		


	union all
		----CPDM.VC,SX--3-
			select att.lot,0 as slm, 0 as ttm,civ.productionqty *170 as c5 , 0 as c6,0 as c7 , 0 as c8, 0 as c9,0 as c10,0 as c11,-1*civ.productionqty *170 as c12
			from m_production ci 
			join m_productionplan civ on ci.m_production_id = civ.m_production_id 
			join m_product hh on hh.m_product_id = civ.m_product_id  and (hh.m_product_category_id in (1000569,1000085) or hh.m_product_id in (1018826,1018827))
			join M_AttributeSetInstance att on att.m_attributesetinstance_id = civ.m_attributesetinstance_id 
			where ci.ad_client_id= $P{AD_CLIENT_ID} and ci.docstatus in ('CO','CL')		
			union all 
		----SL nhap chuyen lo---1--
			select   att.lot,0 as slm, 0 as ttm, 0 as c5, fa.qty as c6, 0 as c7, 0 as c8, 0 as c9,0 as c10 , fa.qty  as c11, -1*fa.amtsourcedr as c12
			from fact_acct fa 
			join m_product hh on hh.m_product_id = fa.m_product_id and (hh.m_product_category_id in (1000562,1000084) or hh.m_product_id in (1008358,1018344))
			join m_productionline  civ on fa.line_id = civ.m_productionline_id 
			join m_attributesetinstance att on att.m_attributesetinstance_id = civ.m_attributesetinstance_id 
			where fa.ad_table_id = 325 and fa.ad_client_id = $P{AD_CLIENT_ID}  and fa.amtacctdr >0 
			and fa.qty >0	
		-- xuat chuyen lo--5,6--
			union all	
			
			select  att.lot ,0 as slm, 0 as ttm,0 as c5, 0 as c6, fa.qty as c7, 0 as c8 , 0 as c9,0 as c10  ,-1*fa.qty  as c11,fa.amtacctdr as c12
			from m_inventory mi
			join m_inventoryline miv on mi.m_inventory_id  = miv.m_inventory_id  
			join m_attributesetinstance att on att.m_attributesetinstance_id = miv.m_attributesetinstance_id 
			join m_product hh on miv.m_product_id = hh.m_product_id and hh.m_product_category_id in (1000562,1000084)
			join fact_acct fa on fa.record_id = miv.m_inventory_id and fa.line_id = miv.m_inventoryline_id and fa.ad_table_id = 321
			where  mi.ad_client_id = $P{AD_CLIENT_ID}  and fa.c_campaign_id in (1000036,1000091,1000088,1000090)
			and fa.qty >0 
	
		union all
		---sl tp sx ra-4--
		select 	lot,0 as slm, 0 as ttm, 0 as c5, 0 as c6, 0 as c7,c8 as c8, 0 as c9,0 as c10,0  as c11, tt as c12
		from(
			select   att.lot, civ.productionqty*coalesce(k.amountdg1,0) as c8,coalesce(k.hsv_dinhmuc,0) *civ.productionqty as tt
			from m_production ci 	 
			join m_productionplan  civ on ci.m_production_id  = civ.m_production_id and ci.docstatus in ('CL','CO')	
			join m_attributesetinstance att on att.m_attributesetinstance_id = civ.m_attributesetinstance_id 
			join m_product hh on hh.m_product_id = civ.m_product_id and hh.m_product_category_id in (1000569,1000085,1000565,1000470,1000558,1000570)
			left join (
				select hsv_dg.startdate , hsv_dg.enddate, dgl.m_product_id,coalesce(hsv_dinhmuc,0) as hsv_dinhmuc,coalesce(amountdg1,0) as amountdg1
				from hsv_nkdongialine dgl  
				join hsv_dongia hsv_dg on hsv_dg.hsv_dongia_id = dgl.hsv_dongia_id and hsv_dg.hsv_loaidongia_id  in (1000000,1000042)
				where hsv_dg.ad_client_id = $P{AD_CLIENT_ID}
			) as k on k.m_product_id = civ.m_product_id 
			where ci.ad_client_id =$P{AD_CLIENT_ID}  and ci.movementdate >= k.startdate and ci.movementdate <= k.enddate 			
		
		) as k 
		
		union all
	------7----		
		select  lot,0 as slm, 0 as ttm,0 as c5, 0 as c6, 0 as c7,0 as c8,  c9,  c10,-1*tong_van as c11, c10 as c12
		from(		
			select  att.lot, civ.qtyinvoiced as c9, civ.linenetamt as c10,0 as tong_van 
			from c_invoice ci 
			join c_invoiceline civ on ci.c_invoice_id = civ.c_invoice_id 
			join m_attributesetinstance att on att.m_attributesetinstance_id = civ.m_attributesetinstance_id 
			join m_product hh on civ.m_product_id = hh.m_product_id and (hh.M_Product_Category_ID in (1000562,1000084) or hh.m_product_id in (1018826,1018827,1018340,1018219))
			where ci.issotrx = 'Y' and ci.ad_client_id = $P{AD_CLIENT_ID} and ci.docstatus in ('CO','CL')
	
			union all
			select att.lot, 0 as c9, 0 as c10,civ.qtyinvoiced as tong_van 
			from c_invoice ci 
			join c_invoiceline civ on ci.c_invoice_id = civ.c_invoice_id 
			join m_attributesetinstance att on att.m_attributesetinstance_id = civ.m_attributesetinstance_id 
			join m_product hh on civ.m_product_id = hh.m_product_id and hh.M_Product_Category_ID in (1000562,1000084) 
			where ci.issotrx = 'Y' and ci.ad_client_id = $P{AD_CLIENT_ID} and ci.docstatus in ('CO','CL')
		) as k 
	) as k
	join m_lot ml on ml.name = k.lot and ml.isactive ='Y'
	where ml.m_lotctl_id = $P{M_LotCtl_ID} and   ml.dateto >= $P{StartDate} AND ml.dateto <= $P{EndDate} 
	group by lot]]>
	</queryString>
