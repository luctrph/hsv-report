               union all
              select '1022' as chitieu,t.movementdate as ngay,l.hsv_luongsxts_id,sum(t.sl) as sl 
              from(select P.movementdate,COALESCE(sum(pl.productionqty),0) as sl,'1022' as chitieu from m_productionplan pl
                            join m_product hh on pl.m_product_id=hh.m_product_id and hh.M_Product_Category_ID=1000085 and hh.M_AttributeSet_ID=1000065
                            join m_production p on pl.m_production_id=p.m_production_id  and p.docstatus in ('CO','CL')
                                AND P.movementdate>=$P{StartDate} and p.movementdate<=$P{EndDate}
                            where pl.ad_client_id=$P{AD_CLIENT_ID} and pl.AD_Org_ID=1000047 
                    group by P.movementdate
                        ) as t
              join hsv_luongsxts l on t.chitieu =l.value and l.ad_client_id=$P{AD_CLIENT_ID} and l.AD_Org_ID=1000047  
              group by t.movementdate,l.hsv_luongsxts_id        
              
              --Chi tieu 1023:xe gắp xe nâng (tờ lot  mặt bóc)  , tính theo sản lượng ván lót mặt bóc nhé
               union all
			  
			  select '1023' as chitieu,t.movementdate as ngay,l.hsv_luongsxts_id,sum(t.sl) as sl 
              from(select P.movementdate,COALESCE(sum(pl.productionqty),0) as sl,'1023' as chitieu from m_productionplan pl
                        join m_product hh on pl.m_product_id=hh.m_product_id and hh.M_Product_ID in ('1017259','1017248')
                        join m_production p on pl.m_production_id=p.m_production_id  and p.docstatus in ('CO','CL')
                                AND P.movementdate>=$P{StartDate} and p.movementdate<=$P{EndDate}
                        where pl.ad_client_id=$P{AD_CLIENT_ID} and  pl.AD_Org_ID=1000047 
                        group by P.movementdate
                            
                        ) as t
              join hsv_luongsxts l on t.chitieu =l.value and l.ad_client_id=$P{AD_CLIENT_ID} and l.AD_Org_ID=1000047  
              group by t.movementdate,l.hsv_luongsxts_id
              
              
        ) as t			
        join hsv_luongsxts dm on t.chitieu =dm.value and dm.ad_client_id=$P{AD_CLIENT_ID} and dm.AD_Org_ID=1000047
        join (select l.EndDate,l.StartDate,ln.hsv_dongialuong,ln.AD_Org_ID,  ln.hsv_luongsxts_id 
            from  hsv_dgluongLine ln
            join hsv_dgluong l on l.hsv_loaidongia_ID in('1000015') and l.hsv_dgluong_ID=ln.hsv_dgluong_ID and l.ad_client_id=$P{AD_CLIENT_ID}
            where ln.AD_Org_ID=1000047 and ln.ad_client_id=1000006
          ) as l on t.hsv_luongsxts_id=l.hsv_luongsxts_id  and t.ngay>=l.StartDate and t.ngay<=l.EndDate and l.AD_Org_ID=1000047
        order by dm.hsv_stt asc
    ) as t
group by t.hsv_stt,t.description,t.name,t.hsv_dongialuong]]>
	</queryString>
