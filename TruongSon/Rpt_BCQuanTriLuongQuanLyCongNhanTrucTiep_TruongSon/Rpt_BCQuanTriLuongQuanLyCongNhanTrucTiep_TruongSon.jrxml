                        ) as t
              join hsv_luongsxts l on t.chitieu =l.value and l.ad_client_id=$P{AD_CLIENT_ID} and l.AD_Org_ID=1000047  
              group by t.movementdate,l.hsv_luongsxts_id        
                        
