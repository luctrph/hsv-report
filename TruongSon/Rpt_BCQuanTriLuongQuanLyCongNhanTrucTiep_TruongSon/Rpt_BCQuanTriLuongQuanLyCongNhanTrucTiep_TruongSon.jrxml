	<queryString language="SQL">
		<![CDATA[select row_number() over() as value,t.hsv_stt,t.description,t.name,sum(t.sl) as sl,t.hsv_dongialuong,sum(t.luong) as luong
from(   select dm.hsv_stt,dm.description,dm.name,t.sl,l.hsv_dongialuong,l.hsv_dongialuong*t.sl as luong
        from( select '1' as chitieu,$P{StartDate} as ngay,hsv_luongsxts_id,0 as sl from hsv_luongsxts where value ='1' and ad_client_id=$P{AD_CLIENT_ID} and AD_Org_ID=1000047
              
              union all
              --Chi tieu 51: 1. Công Nhân   Bóc ván 
              select '51' as chitieu,t.movementdate as ngay,l.hsv_luongsxts_id,sum(t.sl) as sl 
              from(select P.movementdate,COALESCE(sum(pl.productionqty),0) as sl,'51' as chitieu from m_productionplan pl
                            join m_product hh on pl.m_product_id=hh.m_product_id and hh.M_Product_Category_ID=1000085 and hh.M_AttributeSet_ID=1000065
                            join m_production p on pl.m_production_id=p.m_production_id  and p.docstatus in ('CO','CL')
                                AND P.movementdate>=$P{StartDate} and p.movementdate<=$P{EndDate}
                            where pl.ad_client_id=$P{AD_CLIENT_ID} and pl.HR_Job_ID=1000020 and pl.AD_Org_ID=1000047 
                    group by P.movementdate
                        ) as t
              join hsv_luongsxts l on t.chitieu =l.value and l.ad_client_id=$P{AD_CLIENT_ID} and l.AD_Org_ID=1000047  
              group by t.movementdate,l.hsv_luongsxts_id        
              
              union all
              --Chi tieu 3: 2.  CN sấy  Ván  ca 1 (ngày)

              select '3' as chitieu,t.movementdate as ngay,l.hsv_luongsxts_id,sum(t.sl) as sl 
              from( select P.movementdate,COALESCE(sum(pl.productionqty),0) as sl,'3' as chitieu from m_productionplan pl
                        join m_product hh on pl.m_product_id=hh.m_product_id and hh.M_Product_Category_ID=1000085 and (hh.M_AttributeSet_ID=1000066 or hh.M_AttributeSet_ID=1000069)
                        join m_production p on pl.m_production_id=p.m_production_id  and p.docstatus in ('CO','CL')
                                AND P.movementdate>=$P{StartDate} and p.movementdate<=$P{EndDate}
                        where pl.ad_client_id=$P{AD_CLIENT_ID} and pl.HR_Job_ID=1000021 and pl.AD_Org_ID=1000047 
