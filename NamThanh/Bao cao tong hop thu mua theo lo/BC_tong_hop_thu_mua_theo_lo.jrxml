	<parameter name="AD_CLIENT_ID" class="java.lang.Integer"/>
	<parameter name="M_Lot_ID" class="java.lang.Integer"/>
	<parameter name="StartDate" class="java.util.Date"/>
	<parameter name="EndDate" class="java.util.Date"/>
	<parameter name="SUBREPORT_DIR" class="java.lang.String">
		<defaultValueExpression><![CDATA["dev/"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select  name as a1,sum(a2) as a2,sum(a3) as a3,sum(a4) as a4,sum(a5) as a5,sum(a6) as a6,sum(a7) as a7, (sum(a2)+ sum(a3))-(sum(a4)+sum(a5)+sum(a6)+sum(a7)) as a8
from(
select  lo.name , sum(civ.linenetamt) as a2,0 as a3, 0 as a4, 0 as a5, 0 as a6, 0 as a7
from c_invoice ci 
join c_invoiceline civ on ci.c_invoice_id = civ.c_invoice_id and ci.issotrx = 'Y' and ci.docstatus in ('CO','CL')
join M_AttributeSetInstance att on civ.m_attributesetinstance_id = att.m_attributesetinstance_id 
join m_lot lo on lo.m_lot_id = att.m_lot_id   and lo.isactive = 'Y' 
where ci.ad_client_id = $P{AD_CLIENT_ID} and ci.c_doctype_id <> 1001103 and ci.dateacct >= lo.datefrom and lo.datefrom >= $P{StartDate} and lo.datefrom <= $P{EndDate} 
and (lo.m_lot_id = $P{M_Lot_ID} or $P{M_Lot_ID} is null)
group by lo.name

union all 

select  k.name  , 0 as a2, sum(tt) as a3, 0 as a4, 0 as a5, 0 as a6, 0 as a7
from(
	select  lo.name ,sum(civ.productionqty) as qty,t.hsv_dinhmuc * sum(civ.productionqty) as tt
	from  m_production ci
	join m_productionplan civ on civ.m_production_id = ci.m_production_id and ci.docstatus in  ('CL','CO') 
	join m_attributeinstance tt on civ.m_attributesetinstance_id = tt.m_attributesetinstance_id 
	join m_lot lo on lo.m_lot_id = civ.m_lot_id 
	join (
		select dgl.m_product_id,dgl.m_attribute_id, dgl.m_attributevalue_id, dgl.hsv_dinhmuc,dg.enddate, dg.startdate 
		from m_attributevalue atv      
		join hsv_nkdongialine dgl on atv.m_attributevalue_id = dgl.m_attributevalue_id 
		join hsv_dongia dg on dgl.hsv_dongia_id =  dg.hsv_dongia_id 
		where dg.hsv_loaidongia_id = 1000009 	 
	) as t on t.m_attribute_id = tt.m_attribute_id and t.m_attributevalue_id = tt.m_attributevalue_id 
	where civ.ad_client_id = $P{AD_CLIENT_ID}  AND (civ.M_Lot_ID = $P{M_Lot_ID} OR $P{M_Lot_ID} IS NULL)  
		and  lo.datefrom >= t.startdate and lo.datefrom <= t.enddate AND lo.datefrom >= $P{StartDate} AND lo.datefrom <= $P{EndDate}
		and (lo.m_lot_id = $P{M_Lot_ID} or $P{M_Lot_ID} is null)
	group by  t.hsv_dinhmuc, lo.name 
) as k group by  k.name 

union all
select lo.name  , 0 as a2, 0 as a3,  sum(civ.linenetamt) as a4, 0 as a5 , 0 as a6, 0 as a7
from c_invoice ci 
join c_invoiceline civ on ci.c_invoice_id = civ.c_invoice_id and ci.issotrx = 'N' and ci.docstatus in ('CO','CL')
join M_AttributeSetInstance att on civ.m_attributesetinstance_id = att.m_attributesetinstance_id 
join m_lot lo on lo.m_lot_id = att.m_lot_id   and lo.isactive = 'Y' 
where ci.ad_client_id = $P{AD_CLIENT_ID} and ci.c_doctype_id <> 1001105 and ci.dateacct >= lo.datefrom and lo.datefrom >= $P{StartDate} and lo.datefrom <= $P{EndDate} 
and (lo.m_lot_id = $P{M_Lot_ID} or $P{M_Lot_ID} is null)
group by  lo.name 

union all 		
select lo.name,0 as a2, 0 as a3, 0 as a4,
	sum(case when hh.m_parttype_id = 1000101 then fc.amtacctcr - fc.amtacctdr else 0 end) as a5,
	sum(case when hh.m_parttype_id = 1000102 then fc.amtacctcr -fc.amtacctdr else 0 end) as a6, 0 as a7
	
from fact_acct fc
join m_inventory  ci on fc.record_id = ci.m_inventory_id  and ci.docstatus  in ('CL','CO')  
join m_inventoryline civ on civ.m_inventoryline_id = fc.line_id 
join m_lot lo on lo.m_lot_id = civ.m_lot_id 
join m_product hh on hh.m_product_id = fc.m_product_id
join c_elementvalue ev on fc.account_id=ev.c_elementvalue_id and ev.ad_client_id=$P{AD_CLIENT_ID} and left(ev.value,2) in ('15')
where fc.ad_client_id = $P{AD_CLIENT_ID}  and fc.ad_table_id = 321
and fc.dateacct >= $P{StartDate} and fc.dateacct <= $P{EndDate} and (civ.M_Lot_ID = $P{M_Lot_ID} or $P{M_Lot_ID} is null)
group by lo.name
union all
select lo.name,0 as a2, 0 as a3, 0 as a4,0 as a5,0 as a6, sum(civ.linenetamt) as a7
from c_invoice ci
join c_invoiceline civ on ci.c_invoice_id = civ.c_invoice_id and ci.issotrx = 'N' and ci.isprinted  = 'Y' and ci.docstatus in ('CL','CO')
join m_lot lo on lo.m_lot_id = civ.m_lot_id 	
where ci.ad_client_id = $P{AD_CLIENT_ID} and ci.C_DocType_ID<> 1001105 and (civ.m_lot_id = $P{M_Lot_ID} or $P{M_Lot_ID} is null) and ci.dateacct >= $P{StartDate} and ci.dateacct <= $P{EndDate}	
group by lo.name
) as k
group by name]]>
	</queryString>
