	<queryString language="SQL">
		<![CDATA[select m_attributesetinstance.description as t,a.c_order_id as id ,b.value as mh,b.name as th,c.name as dvt,a.qtyordered as sl,a.priceactual as gia,a.priceactual*a.qtyordered as tt
