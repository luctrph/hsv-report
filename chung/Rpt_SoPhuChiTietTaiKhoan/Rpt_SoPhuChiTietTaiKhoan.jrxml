	<queryString language="SQL">
		<![CDATA[WITH fa_filtered AS (
    SELECT fa.dateacct, fa.amtacctdr, fa.amtacctcr, fa.account_id,
           fa.description,   bp.value AS bp_value,fa.ad_table_id,fa.record_id  
    FROM Fact_Acct fa
    LEFT JOIN C_BPartner bp ON bp.c_bpartner_id = fa.c_bpartner_id
    WHERE fa.AD_Client_ID = $P{AD_CLIENT_ID}
      AND fa.Account_ID = $P{Account_ID}
      AND (fa.AD_Org_ID = $P{AD_Org_ID} OR $P{AD_Org_ID} IS NULL)
      AND (fa.C_BPartner_ID = $P{C_BPartner_ID} OR $P{C_BPartner_ID} IS NULL)
      AND (fa.C_Activity_ID = $P{C_Activity_ID} OR $P{C_Activity_ID} IS NULL)
      AND (fa.C_Campaign_ID = $P{C_Campaign_ID} OR $P{C_Campaign_ID} IS NULL)
     
),
base AS (
    -- 1. A_Asset_Addition
    SELECT fa.dateacct, fa.amtacctdr, fa.amtacctcr, fa.account_id,
           fa.description, fa.bp_value AS value, a.documentno
    FROM fa_filtered fa
    JOIN A_Asset_Addition a ON a.a_asset_addition_id = fa.record_id
    WHERE fa.ad_table_id = 53137 AND a.docstatus IN ('CO','CL')

    UNION ALL
    -- 2. A_Asset_Disposed
    SELECT fa.dateacct, fa.amtacctdr, fa.amtacctcr, fa.account_id,
           fa.description, fa.bp_value, a1.documentno
    FROM fa_filtered fa
    JOIN A_Asset_Disposed a1 ON a1.a_asset_disposed_id = fa.record_id
    WHERE fa.ad_table_id = 53127 AND a1.docstatus IN ('CO','CL')

    UNION ALL
    -- 3. A_Depreciation_Entry
    SELECT fa.dateacct, fa.amtacctdr, fa.amtacctcr, fa.account_id,
           fa.description, fa.bp_value, a2.documentno
    FROM fa_filtered fa
    JOIN A_Depreciation_Entry a2 ON a2.a_depreciation_entry_id = fa.record_id
    WHERE fa.ad_table_id = 53121 AND a2.docstatus IN ('CO','CL')

    UNION ALL
    -- 4. C_AllocationHdr
    SELECT fa.dateacct, fa.amtacctdr, fa.amtacctcr, fa.account_id,
           fa.description, fa.bp_value, a3.documentno
    FROM fa_filtered fa
    JOIN C_AllocationHdr a3 ON a3.c_allocationhdr_id = fa.record_id
    WHERE fa.ad_table_id = 735 AND a3.docstatus IN ('CO','CL')

    UNION ALL
    -- 5. C_BankStatement
    SELECT fa.dateacct, fa.amtacctdr, fa.amtacctcr, fa.account_id,
           fa.description, fa.bp_value, a4.documentno
    FROM fa_filtered fa
    JOIN C_BankStatement a4 ON a4.c_bankstatement_id = fa.record_id
    WHERE fa.ad_table_id = 392 AND a4.docstatus IN ('CO','CL')

    UNION ALL
    -- 6. C_Cash
    SELECT fa.dateacct, fa.amtacctdr, fa.amtacctcr, fa.account_id,
           fa.description, fa.bp_value, a5.documentno
    FROM fa_filtered fa
    JOIN C_Cash a5 ON a5.c_cash_id = fa.record_id
    WHERE fa.ad_table_id = 407 AND a5.docstatus IN ('CO','CL')

    UNION ALL
    -- 7. C_Invoice
    SELECT fa.dateacct, fa.amtacctdr, fa.amtacctcr, fa.account_id,
           fa.description, fa.bp_value, a6.documentno
    FROM fa_filtered fa
    JOIN C_Invoice a6 ON a6.c_invoice_id = fa.record_id
    WHERE fa.ad_table_id = 318 AND a6.docstatus IN ('CO','CL')

    UNION ALL
    -- 8. C_Payment
    SELECT fa.dateacct, fa.amtacctdr, fa.amtacctcr, fa.account_id,
           fa.description, fa.bp_value, a7.documentno
    FROM fa_filtered fa
    JOIN C_Payment a7 ON a7.c_payment_id = fa.record_id
    WHERE fa.ad_table_id = 335 AND a7.docstatus IN ('CO','CL')

    UNION ALL
    -- 9. GL_Journal
    SELECT fa.dateacct, fa.amtacctdr, fa.amtacctcr, fa.account_id,
           fa.description, fa.bp_value, a8.documentno
    FROM fa_filtered fa
    JOIN GL_Journal a8 ON a8.gl_journal_id = fa.record_id
    WHERE fa.ad_table_id = 224 AND a8.docstatus IN ('CO','CL')

    UNION ALL
    -- 10. M_InOut
    SELECT fa.dateacct, fa.amtacctdr, fa.amtacctcr, fa.account_id,
           fa.description, fa.bp_value, a9.documentno
    FROM fa_filtered fa
    JOIN M_InOut a9 ON a9.m_inout_id = fa.record_id
    WHERE fa.ad_table_id = 319 AND a9.docstatus IN ('CO','CL')

    UNION ALL
    -- 11. M_Inventory
    SELECT fa.dateacct, fa.amtacctdr, fa.amtacctcr, fa.account_id,
           fa.description, fa.bp_value, a10.documentno
    FROM fa_filtered fa
    JOIN M_Inventory a10 ON a10.m_inventory_id = fa.record_id
    WHERE fa.ad_table_id = 321 AND a10.docstatus IN ('CO','CL')

    UNION ALL
    -- 12. M_MatchInv
    SELECT fa.dateacct, fa.amtacctdr, fa.amtacctcr, fa.account_id,
           fa.description, fa.bp_value, a11.documentno
    FROM fa_filtered fa
    JOIN M_MatchInv a11 ON a11.m_matchinv_id = fa.record_id
    WHERE fa.ad_table_id = 472

    UNION ALL
    -- 13. M_MatchPO
    SELECT fa.dateacct, fa.amtacctdr, fa.amtacctcr, fa.account_id,
           fa.description, fa.bp_value, a12.documentno
    FROM fa_filtered fa
    JOIN M_MatchPO a12 ON a12.m_matchpo_id = fa.record_id
    WHERE fa.ad_table_id = 473

    UNION ALL
    -- 14. M_Movement
    SELECT fa.dateacct, fa.amtacctdr, fa.amtacctcr, fa.account_id,
           fa.description, fa.bp_value, a13.documentno
    FROM fa_filtered fa
    JOIN M_Movement a13 ON a13.m_movement_id = fa.record_id
    WHERE fa.ad_table_id = 323 AND a13.docstatus IN ('CO','CL')

    UNION ALL
    -- 15. M_Production
    SELECT fa.dateacct, fa.amtacctdr, fa.amtacctcr, fa.account_id,
           fa.description, fa.bp_value, a14.documentno
    FROM fa_filtered fa
    JOIN M_Production a14 ON a14.m_production_id = fa.record_id
    WHERE fa.ad_table_id = 325 AND a14.docstatus IN ('CO','CL')
)
select * 
	from(
	SELECT
	    NULL AS ngay,
	    NULL AS value,
	    'Số dư đầu kỳ:' AS noidung,
	    NULL AS psn,
	    NULL AS psc,
	    SUM(CASE
	        WHEN ev.accountsign='N' THEN b.amtacctdr - b.amtacctcr
	        WHEN ev.accountsign='C' THEN b.amtacctcr - b.amtacctdr
	        else b.amtacctdr-b.amtacctcr
	    END) AS ps,
	    NULL AS soct
	FROM base b
	JOIN c_elementvalue ev ON ev.c_elementvalue_id = b.account_id
	WHERE b.dateacct < $P{StartDate}
	
	UNION ALL
	
	-- Phát sinh trong kỳ
	SELECT
	    b.dateacct AS ngay,
	    b.value,
	    b.description AS noidung,
	    CASE WHEN b.amtacctcr = 0 THEN b.amtacctdr ELSE 0 END AS psn,
	    CASE WHEN b.amtacctdr = 0 THEN b.amtacctcr ELSE 0 END AS psc,
	    CASE
	        WHEN ev.accountsign='N' THEN (CASE WHEN b.amtacctcr=0 THEN b.amtacctdr ELSE -b.amtacctcr END)
	        WHEN ev.accountsign='C' THEN (CASE WHEN b.amtacctdr=0 THEN b.amtacctcr ELSE -b.amtacctdr END)
	        ELSE (CASE WHEN b.amtacctcr=0 THEN b.amtacctdr ELSE -b.amtacctcr END)
	    END AS ps,
	    COALESCE(b.documentno,'') AS soct 
	FROM base b
	JOIN c_elementvalue ev ON ev.c_elementvalue_id = b.account_id
	WHERE b.dateacct BETWEEN $P{StartDate} AND $P{EndDate}
) as k
order by k.ngay NULLS FIRST]]>
