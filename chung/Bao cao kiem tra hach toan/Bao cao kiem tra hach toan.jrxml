	<queryString>
		<![CDATA[select ROW_NUMBER() OVER (PARTITION BY dm ORDER BY dm,ngay ) AS stt,* from
(
	select 'VND' as dm,concat(ce.value ,'-', ce.name) as tk, cp.documentno as so_ct,fa.dateacct as ngay,fa.amtsourcecr  as co , fa.amtsourcedr as "no"  
	from fact_acct fa
	join c_payment cp on cp.c_payment_id = fa.record_id and cp.docstatus in ('CO','CL')
	join c_elementvalue  ce on fa.account_id = ce.c_elementvalue_id and ce.isbankaccount = 'Y'
	join c_bankaccount cb on ce.c_bankaccount_id = cb.c_bankaccount_id and cb.c_currency_id = 100
	where fa.ad_table_id = 335 and fa.ad_client_id =$P{AD_CLIENT_ID} and fa.c_currency_id <>100
	and cp.hsv_kiemtra<>'Y' and fa.dateacct >=$P{StartDate} and fa.dateacct <= $P{EndDate}
	union all 
	select 'USD' as dm,concat(ce.value ,'-', ce.name) as tk, cp.documentno as so_ct,fa.dateacct as ngay,fa.amtsourcecr as co , fa.amtsourcedr as "no"  
	from fact_acct fa
	join c_payment cp on cp.c_payment_id = fa.record_id and cp.docstatus in ('CO','CL')
	join c_elementvalue  ce on fa.account_id = ce.c_elementvalue_id and ce.isbankaccount = 'Y'
	join c_bankaccount cb on ce.c_bankaccount_id = cb.c_bankaccount_id and cb.c_currency_id = 234
	where fa.ad_table_id = 335 and fa.ad_client_id =$P{AD_CLIENT_ID} and fa.c_currency_id <>234
	and cp.hsv_kiemtra<>'Y' and fa.dateacct >=$P{StartDate} and fa.dateacct <= $P{EndDate}
) as k]]>
	</queryString>
