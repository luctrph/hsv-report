	</parameter>
	<parameter name="AD_CLIENT_ID" class="java.lang.Integer"/>
	<parameter name="CURRENT_LANG" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT p.ad_client_id ,
  p.c_conversiontype_id,
  cvt.name,
  cr.c_currency_id,
  cr.c_currency_id_to,
  cr.multiplyrate,
  p.c_payment_id,
  cr.validfrom,
  cr.validto,
  (p.payamt * cr.multiplyrate) AS tienqd,
  cr.dividerate,
  dckh.name AS dckh,

  -- Diễn giải chi phí
  CASE 
    WHEN i.documentno IS NOT NULL THEN 'Thanh toán phân bổ vào hóa đơn'
    WHEN ch.name IS NOT NULL THEN ch.name
    WHEN o.documentno IS NOT NULL THEN CONCAT(' Trả trước cho đơn hàng số : ', o.documentno, ' ngày: ', o.dateordered)
    WHEN dt.docbasetype = 'APP' THEN 'TK- 3317 chi trả tiền NCC'
    ELSE 'TK -1317 thu tiền hàng'
  END AS chiphi,

  u.description AS ten,
  org.name AS bp,
  i.grandtotal AS tienhd,
  i.documentno AS sohd,
  p.processed,
  u.name AS tennd,
  p.documentno AS so,
  UPPER(dt.name) AS lpm,
  ba.value AS manh,
  ba.name AS tennh,
  bp.value AS mkh,
  bp.name AS tenkh,
  cur.iso_code AS tiente,
  p.overunderamt AS cl,
  p.payamt AS tien,
  p.discountamt AS giamgia,
  p.writeoffamt AS bodi,
  p.taxamt AS thue,
  p.description AS ld,
  p.dateacct AS ngay,
  o.documentno AS sdh,
  o.grandtotal AS tiendh,
  act.name AS hd,

  -- Người nhận/nộp
  CASE 
    WHEN dt.docbasetype = 'APP' THEN 'Người nhận'
    ELSE 'Người nộp'
  END AS nguoi,

  -- Trạng thái chứng từ
  CASE p.docstatus
    WHEN 'CO' THEN 'Đã hoàn thành'
    WHEN 'CL' THEN 'Đã đóng'
    WHEN 'RE' THEN 'Không dùng'
    WHEN 'DR' THEN 'Đang nháp'
    WHEN 'VO' THEN 'Đã hủy'
    WHEN 'IN' THEN 'Không hợp lệ'
    WHEN 'AP' THEN 'Phê duyệt'
    WHEN 'IP' THEN 'Đang xử lý'
    WHEN 'NA' THEN 'Chưa phê duyệt'
    WHEN 'WC' THEN 'Chờ Allocation'
    ELSE 'Chờ thanh toán'
  END AS ttct,

  LEFT(tkn.value, 5) AS tkn,
  LEFT(tkc.value, 5) AS tkc,

  CASE 
    WHEN dt.docbasetype = 'APP' THEN tkn.name
    ELSE tkc.name
  END AS tentkdu

FROM c_payment p
LEFT JOIN (
    SELECT a.RECORD_ID, e.name, e.value
    FROM fact_acct a
    JOIN c_elementvalue e ON e.c_elementvalue_id = a.account_id
    WHERE a.amtacctdr <> 0 AND a.ad_table_id = 335
) tkn ON p.c_payment_id = tkn.RECORD_ID AND p.c_payment_id = $P{RECORD_ID}

LEFT JOIN (
    SELECT a.RECORD_ID, e.name, e.value
    FROM fact_acct a
    JOIN c_elementvalue e ON e.c_elementvalue_id = a.account_id
    WHERE a.amtacctcr <> 0 AND a.ad_table_id = 335
) tkc ON p.c_payment_id = tkc.RECORD_ID AND p.c_payment_id = $P{RECORD_ID}

LEFT JOIN c_bankaccount ba ON p.c_bankaccount_id = ba.c_bankaccount_id
JOIN ad_user u ON p.createdby = u.ad_user_id
LEFT JOIN c_bpartner bp ON p.c_bpartner_id = bp.c_bpartner_id
JOIN c_currency cur ON p.c_currency_id = cur.c_currency_id
LEFT JOIN c_charge ch ON p.c_charge_id = ch.c_charge_id
JOIN c_doctype dt ON p.c_doctype_id = dt.c_doctype_id
LEFT JOIN c_invoice i ON p.c_invoice_id = i.c_invoice_id
LEFT JOIN c_bpartner_location dckh ON p.c_bpartner_id = dckh.c_bpartner_id
LEFT JOIN ad_org org ON p.ad_org_id = org.ad_org_id
LEFT JOIN c_order o ON p.c_order_id = o.c_order_id
LEFT JOIN c_activity act ON p.c_activity_id = act.c_activity_id
LEFT JOIN c_conversiontype cvt ON p.c_conversiontype_id = cvt.c_conversiontype_id
LEFT JOIN c_conversion_rate cr ON
  p.c_conversiontype_id = cr.c_conversiontype_id
  AND p.c_currency_id = cr.c_currency_id
  AND p.dateacct BETWEEN cr.validfrom AND cr.validto
  AND cr.ad_client_id = $P{AD_CLIENT_ID}
WHERE p.c_payment_id = $P{RECORD_ID}]]>
