	<queryString language="SQL">
		<![CDATA[select t.kho as tenkho,t.description as t,t.value as mh,t.name as th,t.productionqty as sl,od.documentno as dh 
from 
  (select m_locator.value as kho ,m_attributesetinstance.description ,pl.m_production_id ,m_product.value ,m_product.name ,pl.productionqty ,c_orderline.c_order_id
    from m_productionplan pl
    left join m_locator  on pl.m_locator_id=m_locator.m_locator_id
    left join c_orderline on pl.c_orderline_id=c_orderline.c_orderline_id
    left join  m_attributesetinstance on pl .m_attributesetinstance_id=m_attributesetinstance.m_attributesetinstance_id
    inner join m_product on pl.m_product_id=m_product.m_product_id
    inner join m_production on pl.m_production_id=m_production.m_production_id 
    where pl.m_production_id=$P{RECORD_ID} and pl.ad_client_id = $P{AD_CLIENT_ID}) as t 
left join c_order od on t.c_order_id=od.c_order_id]]>
	</queryString>
