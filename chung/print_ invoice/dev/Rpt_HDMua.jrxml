	<queryString language="SQL">
		<![CDATA[select ad_user.description as ten,ad_org.name as bp,c_doctype.name as tailieu,ci.documentno as sohd,c_order.documentno as soorder,ci.description as ld,ci.dateinvoiced as ngayhd,c_bpartner.value as mkh,c_bpartner.name as tenkh,c_bpartner_location.name as diachi,ci.dateordered as ngayorder,c_currency.iso_code as tiente,ci.totallines as tien,ad_user.name as ddkh,(ci.totallines+sum(c_invoiceline.taxamt)) as tt,sum(c_invoiceline.taxamt) as thue,c_activity.name as hd 
																,case when ci.docstatus in ('CO') then 'Đã hoàn thành' else case when ci.docstatus in ('CL') then 'Đã đóng' else 
																case when ci.docstatus in ('RE') then 'Không dùng' else case when ci.docstatus in ('DR') then ' Đang nháp'else 
																case when ci.docstatus in ('VO') then ' Đã hủy'else Case when ci.docstatus in ('IN') then 'Không hợp lệ' else 
																case when ci.docstatus in ('AP') then ' Phê duyệt'else Case when ci.docstatus in ('IP') then 'Đang xử lý' else 
															    case when ci.docstatus in ('NA') then ' Chưa phê duyệt'else Case when ci.docstatus in ('WC') then 'chờ Allocation' else 'chờ thanh toán'
															    end end 	end end end end end end end end as ttct
from c_invoice ci
left join c_order on c_order.c_order_id=ci.c_order_id
 left join c_bpartner on ci.c_bpartner_id=c_bpartner.c_bpartner_id 
 left join c_bpartner_location on ci.c_bpartner_location_id=c_bpartner_location.c_bpartner_location_id 
 join ad_user on ci.createdby=ad_user.ad_user_id and (ad_user.ad_user_id =$P{AD_User_ID} or $P{AD_User_ID} is null)
 join c_currency on ci.c_currency_id=c_currency.c_currency_id 
 left join ad_org on ci.ad_org_id=ad_org.ad_org_id and (ci.ad_org_id = $P{AD_Org_ID} or $P{AD_Org_ID} is null)  
 join c_doctype on ci.c_doctypetarget_id=c_doctype.c_doctype_id and (ci.c_doctype_id =  $P{C_DocType_ID} or $P{C_DocType_ID} is null)
 join c_invoiceline on ci.c_invoice_id=c_invoiceline.c_invoice_id
 left join c_activity on ci.c_activity_id=c_activity.c_activity_id 
  where  ci.c_invoice_id=$P{RECORD_ID}  and ci.issotrx ='N' and (ci.isprinted = $P{isPrinted} or $P{isPrinted} is null)
group by ad_user.description,ci.docstatus,ad_org.name ,c_doctype.name ,ci.documentno ,c_order.documentno ,ci.description ,ci.dateinvoiced ,c_bpartner.value ,c_bpartner.name ,c_bpartner_location.name ,ci.dateordered ,c_currency.iso_code ,ci.totallines ,ad_user.name,c_activity.name
]]>
