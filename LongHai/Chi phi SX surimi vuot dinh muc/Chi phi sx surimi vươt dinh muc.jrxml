			) as t
			
		
		--Chi tieu 218:14.Nhân Công Tổ bảo quản , cấp đông , khác  SX Surimi
		union all
		
		sELECT 'II' as nhom,'218' AS ct,0 AS sl,COALESCE(sum(t.tt)) AS tt
		FROM (SELECT 0 AS sl,COALESCE(sum(fc.amtacctdr),0)-COALESCE(sum(fc.amtacctcr),0) AS tt
		      FROM fact_acct fc 
