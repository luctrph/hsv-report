		      WHERE  fc.ad_client_id=$P{AD_CLIENT_ID}  AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} and fc.C_BPartner_ID in (1010338, 1010405)
		      
			) as t
			
