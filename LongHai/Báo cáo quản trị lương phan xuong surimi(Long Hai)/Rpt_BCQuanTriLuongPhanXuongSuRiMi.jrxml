			where pl.ad_client_id=$P{AD_CLIENT_ID}
			group by P.movementdate
		) as t
		join hsv_luongsxts dm on t.chitieu=dm.value and dm.ad_client_id=$P{AD_CLIENT_ID}
		group by t.movementdate,dm.hsv_luongsxts_id
		
