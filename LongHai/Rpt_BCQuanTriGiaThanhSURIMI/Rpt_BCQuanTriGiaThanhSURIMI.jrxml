		<defaultValueExpression><![CDATA["dev/"]]></defaultValueExpression>
	</parameter>
	<parameter name="EXPORT_TYPE" class="java.lang.String"/>
	<parameter name="AD_Org_ID" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[select row_number() over() as stt,value,name,b,c,d,e, case when value in ('206','207','208','209','210','211','212','213','214','215','216','217','218','219','220','221','222','223','224','225','226','227','228','229','2291') then e end as a231,
case when value in  ('2041','2042','205') then e end as a2311,
case when value = '202' then d end as a232_1,
case when value = '201' then c end as a232_2,
case when value = '203' then e end as a234

from (
select dm.value,dm.name,dm.hsv_dinhmuc as b
		--so luong
		,case when dm.value in ('200','201','202','203','20','230') then t.sl 
		else case when dm.value in ('2041','206','207','208','209','210','211','212','213','214','215','216','217','218','219','220','221','222','225','226','227','228','229','2291') then sl1.sl
		else case when dm.value in ('205','2042')  then t.sl else sl2.sl  end end end  as c
		--thanh tien
		,case when dm.value in ('200','201','230') then 0 else t.tt end as d
		--Don gia /kg
		,case when dm.value in ('200','201','202','204','230') then 0 
		else case when dm.value in ('205','2041','2042','206','207','208','209','210','211','212','213','214','215','216','217','218','219','220','221','222','224','225','226','227','228','229','2291') and sl1.sl<>0 then t.tt/sl1.sl
		else case when dm.value in ('223','224')and sl2.sl<>0 then t.tt/sl2.sl 
		else case when dm.value in ('203') and t.sl<>0 then t.tt/t.sl  end end end end  as e
				
from(	--Bao cao tinh gia thanh SURIMI ngay 17/9/2020
		--Chi tieu 200:I    THÀNH PHẨM SURIMI
		select 'I' as nhom,'200' as ct,0 as sl,0 as tt from ad_client where ad_client_id =$P{AD_CLIENT_ID}
		
		--Chi tieu 201:1.  Tổng  Surimi SX (Lần 1)(San pham nhap kho - san pham xuat su dung)
		union all
		
		select 'I' as nhom,'201' as ct,sum(t.sl) as sl,0 as tt
		from(	select COALESCE(sum(pl.productionqty),0 ) as sl
				from m_productionplan pl
				join m_product hh on hh.m_product_id =pl.m_product_id and hh.m_attributeset_id  in ('1000088','1000150','1000151')
				join m_production p on p.m_production_id =pl.m_production_id and p.docstatus in ('CO','CL') and p.movementdate >=$P{StartDate}and p.movementdate <=$P{EndDate}
				
				where pl.ad_client_id =$P{AD_CLIENT_ID} 
				
				--Tru di so luong xuat kho su dung
				union all
				
				
				select -1*COALESCE(sum(pl.qtyinternaluse ),0 ) as sl
				from M_Inventoryline pl
				join m_product hh on hh.m_product_id =pl.m_product_id and hh.m_attributeset_id in ('1000088','1000150','1000151')
				join M_Inventory p on p.M_Inventory_id =pl.M_Inventory_id and p.docstatus in ('CO','CL') and p.movementdate >=$P{StartDate}and p.movementdate <=$P{EndDate}
					 
					 and p.C_DocType_ID=1001066
				where pl.ad_client_id =$P{AD_CLIENT_ID} 
				
				
			) as t
			
		--Chi tieu 202:2.   Phụ phẩm thu được: Đầu xương	
		union all
		
		select 'I' as nhom,'202' as ct,sum(t.sl) as sl,sum(t.tt) as tt
		from(	select COALESCE(sum(fc.qty),0 ) as sl ,sum(fc.amtacctdr-fc.amtacctcr) as tt
				from m_productionplan pl
				join m_production p on p.m_production_id =pl.m_production_id and p.docstatus in ('CO','CL') and p.movementdate>=$P{StartDate} and p.movementdate <=$P{EndDate} and p.c_activity_id =1000273
				join fact_acct fc on fc.ad_table_id =325 and  fc.ad_client_id =$P{AD_CLIENT_ID} and p.m_production_id=fc.record_id and fc.amtacctdr<>0 
				where fc.ad_client_id =$P{AD_CLIENT_ID} and fc.M_Product_ID=1015083 and fc.qty = pl.productionqty 
			) as t				
		--Chi tieu 203:3.   Surimi xuất bán (Bình Quân) 
		union all
		SELECT 'I' as nhom,'203' AS ct,COALESCE(sum(t.sl)) AS sl,COALESCE(sum(t.tt)) AS tt
		FROM (SELECT -1*COALESCE(sum(fc.qty),0) AS sl,COALESCE(sum(fc.amtacctcr),0)-COALESCE(sum(fc.amtacctdr),0) AS tt
		      FROM fact_acct fc 
		      JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,2)='51'
		      JOIN M_product hh ON fc.m_product_id=hh.m_product_id AND hh.M_AttributeSet_ID in ('1000088','1000150','1000151') 
		      WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id<>224 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate}
		        		
		      UNION ALL
						
		      SELECT -1*COALESCE(sum(fc.qty),0) AS sl,COALESCE(sum(fc.amtacctcr),0)-COALESCE(sum(fc.amtacctdr),0) AS tt
		      FROM fact_acct fc 
		      JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,2)='51'
		      JOIN gl_journalline gl ON  Fc.record_id=gl.gl_journal_id AND Fc.line_id=gl.gl_journalline_id AND Fc.Account_ID=gl.Account_ID
		                                   AND gl.IsGenerated='N'
		      join m_product hh on hh.m_product_id =fc.m_product_id and hh.M_AttributeSet_ID in ('1000088','1000150','1000151')
		      WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id=224 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate}
		                  
			) AS t       
			
		--Chi tieu 204:I    THÀNH PHẨM SURIMI
		union all
		
		select 'I' as nhom,'204' as ct,0 as sl,0 as tt from ad_client where ad_client_id =$P{AD_CLIENT_ID}
		---------------------------------------------------------------
		union all
	
		SELECT 'I' as nhom,'2041' AS ct,0 AS sl,COALESCE(sum(t.tt)) AS tt
		FROM (SELECT -1*COALESCE(sum(fc.qty),0) AS sl,COALESCE(sum(fc.amtacctdr),0)-COALESCE(sum(fc.amtacctcr),0) AS tt
		      FROM fact_acct fc 
		      JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,4)='6275'
		      WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id<>224 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate}and fc.ad_table_id<>224 and fc.C_Activity_ID=1000273
		
		      union all
		      
		      SELECT 0 AS sl,COALESCE(sum(fc.amtacctdr),0) -COALESCE(sum(fc.amtacctcr),0) AS tt
		      FROM fact_acct fc 
		      join gl_journalline gl on  Fc.record_id=gl.gl_journal_id and Fc.line_id=gl.gl_journalline_id 
		      		and Fc.Account_ID=gl.Account_ID and gl.IsGenerated='N'
		      JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,4) in ('6275')
		      WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} and fc.ad_table_id=224 and fc.C_Activity_ID=1000273
		            
		      
			) as t
			
	union all
	
	
		SELECT 'I' as nhom,'2042' AS ct,sum(sl) AS sl,COALESCE(sum(t.tt)) AS tt
		FROM (SELECT COALESCE(sum(fc.qty),0) AS sl,COALESCE(sum(fc.amtacctdr),0)-COALESCE(sum(fc.amtacctcr),0) AS tt
		      FROM fact_acct fc 
		      JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,4) in ('6211','6329')
		      JOIN M_product hh ON fc.m_product_id=hh.m_product_id  and hh.M_Product_Category_ID in ('1000554')
		      join M_Inventory mi on mi.M_Inventory_ID=fc.record_id and mi.docstatus in ('CO','CL') and mi.C_DocType_ID=1001066
		      --join m_inventoryline miv on mi.m_inventory_id = miv.m_inventory_id and fc.qty = miv.qtyinternaluse 
		      WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id=321 AND fc.dateacct>= $P{StartDate} AND fc.dateacct<= $P{EndDate} and fc.c_activity_id =1000273
		       		                 
		) AS t  
		--Chi tieu 205:1. Nguyên Liệu Cá TK 6211
		union all
		
		SELECT 'I' as nhom,'205' AS ct,sum(sl) AS sl,COALESCE(sum(t.tt)) AS tt
		FROM (
			  SELECT -1*COALESCE(sum(fc.qty),0) AS sl,COALESCE(sum(fc.amtacctcr),0)-COALESCE(sum(fc.amtacctdr),0) AS tt
		      FROM fact_acct fc 
		      JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,2)='15'
		      JOIN M_product hh ON fc.m_product_id=hh.m_product_id  and hh.M_Product_Category_ID in ('1000492','1000493')
		      join M_Inventory mi on mi.M_Inventory_ID=fc.record_id and mi.docstatus in ('CO','CL') and mi.C_DocType_ID=1001066
		      WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id=321 AND fc.dateacct>= $P{StartDate} AND fc.dateacct<= $P{EndDate} and fc.c_activity_id =1000273 		                 
			) AS t  
		
		--Chi tieu 206:2 Nguyên liệu phụ : Đường TK 6213	
		union all
		
		SELECT 'I' as nhom,'206' AS ct,0 AS sl,COALESCE(sum(t.tt)) AS tt
		FROM (SELECT -1*COALESCE(sum(fc.qty),0) AS sl,COALESCE(sum(fc.amtacctdr),0)-COALESCE(sum(fc.amtacctcr),0) AS tt
		      FROM fact_acct fc 
		      JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,4)='6213'
		      JOIN M_product hh ON fc.m_product_id=hh.m_product_id AND hh.M_PartType_ID=1000053  
		      WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id=321 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate}
		       		                 
			) AS t  
		
		--Chi tieu 207:3 Nguyên liệu phụ : Bột Dẻo TK 6213	
		union all
		
		SELECT 'I' as nhom,'207' AS ct,0 AS sl,COALESCE(sum(t.tt)) AS tt
		FROM (SELECT -1*COALESCE(sum(fc.qty),0) AS sl,COALESCE(sum(fc.amtacctdr),0)-COALESCE(sum(fc.amtacctcr),0) AS tt
		      FROM fact_acct fc 
		      JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,4)='6213'
		      JOIN M_product hh ON fc.m_product_id=hh.m_product_id AND hh.M_PartType_ID=1000054  
		      WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id=321 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate}
		       		                 
			) AS t  
				
		--Chi tieu 208:4 Nguyên liệu phụ : TG TK 6213	
		union all
		
		SELECT 'I' as nhom,'208' AS ct,0 AS sl,COALESCE(sum(t.tt)) AS tt
		FROM (SELECT -1*COALESCE(sum(fc.qty),0) AS sl,COALESCE(sum(fc.amtacctdr),0)-COALESCE(sum(fc.amtacctcr),0) AS tt
		      FROM fact_acct fc 
		      JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,4)='6213'
		      JOIN M_product hh ON fc.m_product_id=hh.m_product_id AND hh.M_PartType_ID=1000055  
		      WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id=321 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate}
		       		                 
			) AS t	
			
		--Chi tieu 209:5 Nguyên liệu phụ : Bột trứng    TK 6213	
		union all
		
		SELECT 'I' as nhom,'209' AS ct,0 AS sl,COALESCE(sum(t.tt)) AS tt
		FROM (SELECT -1*COALESCE(sum(fc.qty),0) AS sl,COALESCE(sum(fc.amtacctdr),0)-COALESCE(sum(fc.amtacctcr),0) AS tt
		      FROM fact_acct fc 
		      JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,4)='6213'
		      JOIN M_product hh ON fc.m_product_id=hh.m_product_id AND hh.M_PartType_ID=1000056  
		      WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id=321 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate}
		       		                 
			) AS t		
		
		--Chi tieu 210:6 Nguyên liệu phụ : Muối, Khác.TK 6213.  (PS NO - PSCO  TK 6213 -thành tiền (ID206+ID207+ID208+ID209)	
		union all
		
		SELECT 'I' as nhom,'210' AS ct,0 AS sl,COALESCE(sum(t.tt)) AS tt
		FROM (SELECT -1*COALESCE(sum(fc.qty),0) AS sl,(COALESCE(sum(fc.amtacctdr),0)-COALESCE(sum(fc.amtacctcr),0)) AS tt
		      FROM fact_acct fc 
		      JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,4)='6213'
		      JOIN M_product hh ON fc.m_product_id=hh.m_product_id --AND hh.M_PartType_ID=1000056  
		      WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id=321 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate}
		       		                 
		      --Tru di ID206+ID207+ID208+ID209
		      union all
		      
		      SELECT -1*COALESCE(sum(fc.qty),0) AS sl,-1*(COALESCE(sum(fc.amtacctdr),0)-COALESCE(sum(fc.amtacctcr),0)) AS tt
		      FROM fact_acct fc 
		      JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,4)='6213'
		      JOIN M_product hh ON fc.m_product_id=hh.m_product_id AND hh.M_PartType_ID in ('1000053','1000054','1000055','1000056')
		      WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id=321 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate}
		      
			) AS t			
			
		--Chi tieu 211:7. Bao Bì  TK 6276
		union all
		
		SELECT 'I' as nhom,'211' AS ct,0 AS sl,COALESCE(sum(t.tt)) AS tt
		FROM (SELECT -1*COALESCE(sum(fc.qty),0) AS sl,COALESCE(sum(fc.amtacctdr),0)-COALESCE(sum(fc.amtacctcr),0) AS tt
		      FROM fact_acct fc 
		      JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,4)='6276'
		      WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id<>224 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} and fc.C_Activity_ID=1000273
		      
		      union all
		      
		      SELECT 0 AS sl,COALESCE(sum(fc.amtacctdr),0) -COALESCE(sum(fc.amtacctcr),0) AS tt
		      FROM fact_acct fc 
		      join gl_journalline gl on  Fc.record_id=gl.gl_journal_id and Fc.line_id=gl.gl_journalline_id 
		      		and Fc.Account_ID=gl.Account_ID and gl.IsGenerated='N'
		      JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,4) in ('6276')
		      WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} and fc.ad_table_id=224 and fc.C_Activity_ID=1000273
		
		      
			) as t
			
		--Chi tieu 212:8. Điện sản xuất TK 6275
		union all
		
		SELECT 'I' as nhom,'212' AS ct,0 AS sl,COALESCE(sum(t.tt)) AS tt
		FROM (SELECT -1*COALESCE(sum(fc.qty),0) AS sl,COALESCE(sum(fc.amtacctdr),0)-COALESCE(sum(fc.amtacctcr),0) AS tt
		      FROM fact_acct fc 
		      JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,4)='6277'
		      WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id<>224 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} and fc.C_Activity_ID=1000273
		      
		      union all
		      
		      SELECT 0 AS sl,COALESCE(sum(fc.amtacctdr),0) -COALESCE(sum(fc.amtacctcr),0) AS tt
		      FROM fact_acct fc 
		      join gl_journalline gl on  Fc.record_id=gl.gl_journal_id and Fc.line_id=gl.gl_journalline_id 
		      		and Fc.Account_ID=gl.Account_ID and gl.IsGenerated='N'
		      JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,4) in ('6277')
		      WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} and fc.ad_table_id=224 and fc.C_Activity_ID=1000273
		
		      
			) as t
		
		--Chi tieu 213:9. Nước sạch PV SX TK 
		union all
		
		SELECT 'I' as nhom,'213' AS ct,0 AS sl,COALESCE(sum(t.tt)) AS tt
		FROM (SELECT -1*COALESCE(sum(fc.qty),0) AS sl,COALESCE(sum(fc.amtacctdr),0) AS tt
		      FROM fact_acct fc 
		      JOIN M_product hh ON fc.m_product_id=hh.m_product_id AND hh.M_PartType_ID=1000060  
			  join m_inventory mi on mi.C_DocType_ID=1001066 and mi.m_inventory_id=fc.record_id and mi.docstatus in ('CO','CL')	and mi.C_Activity_ID=1000273      
		      WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id=321 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} and fc.amtacctdr<>0
		       		                 
			) AS t
			
		--Chi tieu 214:10. Sửa chữa Thường Xuyên TK 6273
		union all
		
		SELECT 'I' as nhom,'214' AS ct,0 AS sl,COALESCE(sum(t.tt)) AS tt
		FROM (SELECT -1*COALESCE(sum(fc.qty),0) AS sl,COALESCE(sum(fc.amtacctdr),0)-COALESCE(sum(fc.amtacctcr),0) AS tt
		      FROM fact_acct fc 
		      JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,4)='6273'
		      WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id<>224 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate}and fc.ad_table_id<>224 and fc.C_Activity_ID=1000273
		
		      union all
		      
		      SELECT 0 AS sl,COALESCE(sum(fc.amtacctdr),0) -COALESCE(sum(fc.amtacctcr),0) AS tt
		      FROM fact_acct fc 
		      join gl_journalline gl on  Fc.record_id=gl.gl_journal_id and Fc.line_id=gl.gl_journalline_id 
		      		and Fc.Account_ID=gl.Account_ID and gl.IsGenerated='N'
		      JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,4) in ('6273')
		      WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} and fc.ad_table_id=224 and fc.C_Activity_ID=1000273
		            
		      
			) as t
			
		--Chi tieu 215:11..Nhiên liẹu ,vt khácTK 6272, 6278
		union all
		
		SELECT 'I' as nhom,'215' AS ct,0 AS sl,COALESCE(sum(t.tt)) AS tt
		FROM (SELECT -1*COALESCE(sum(fc.qty),0) AS sl,COALESCE(sum(fc.amtacctdr),0)-COALESCE(sum(fc.amtacctcr),0) AS tt
		      FROM fact_acct fc 
		      JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,4) in ('6272','6278')
		      WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id<>224 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} and fc.C_Activity_ID=1000273
		      
		      union all
		      
		      SELECT 0 AS sl,COALESCE(sum(fc.amtacctdr),0) -COALESCE(sum(fc.amtacctcr),0) AS tt
		      FROM fact_acct fc 
