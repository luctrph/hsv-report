		      JOIN M_product hh ON fc.m_product_id=hh.m_product_id  and hh.M_Product_Category_ID in ('1000493') and hh.m_attributeset_id <>1000086
		      join M_Inventory mi on mi.M_Inventory_ID=fc.record_id and mi.docstatus in ('CO','CL') and mi.C_DocType_ID=1001066
		      WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id=321 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} and fc.c_activity_id =1000272
		             		                 
