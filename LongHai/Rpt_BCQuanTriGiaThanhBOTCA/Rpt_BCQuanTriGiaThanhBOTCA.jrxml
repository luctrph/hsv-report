		      WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id = 321 and fc.C_Activity_ID=1000272 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate}
