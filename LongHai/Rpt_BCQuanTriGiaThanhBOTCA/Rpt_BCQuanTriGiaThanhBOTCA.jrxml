--Lien ket de lay so luong bot ca san xuat 
left join (
		SELECT 'I' as nhom,'241' as ct,sum(k.sl) as sl
		from(	
				  ---241-----
					SELECT COALESCE(sum(civ.productionqty),0 ) as sl
					from m_productionplan civ
					join m_product hh on hh.m_product_id =civ.m_product_id and hh.m_attributeset_id in (1000086,1000152)
					join m_production ci on ci.m_production_id =civ.m_production_id and ci.docstatus in ('CO','CL') and ci.movementdate >=$P{StartDate} and ci.movementdate <=$P{EndDate}
						and ci.c_activity_id =1000272
					where civ.ad_client_id =$P{AD_CLIENT_ID} 
					union all 
					SELECT -1*COALESCE(sum(civ.qtyinternaluse),0 ) as sl
					from m_inventoryline civ
					join m_product hh on hh.m_product_id =civ.m_product_id and hh.m_attributeset_id in (1000086,1000152)
					join m_inventory ci on ci.m_inventory_id =civ.m_inventory_id and ci.docstatus in ('CO','CL') and ci.movementdate >=$P{StartDate} and ci.movementdate <=$P{EndDate}
						and ci.c_activity_id =1000272
					where civ.ad_client_id =$P{AD_CLIENT_ID} 
					union all 
					-----242---
					SELECT COALESCE(sum(pl.qtyinvoiced),0 ) as sl
					from C_Invoiceline pl
					join m_product hh on hh.m_product_id =pl.m_product_id and hh.m_attributeset_id =1000086
					join C_Invoice p on p.C_Invoice_id =pl.C_Invoice_id and p.docstatus in ('CO','CL') and p.dateacct>=$P{StartDate}and p.dateacct <=$P{EndDate} and p.C_DocType_ID=1001045
					where pl.ad_client_id =$P{AD_CLIENT_ID} 
						
			) as k
	) as sl1 on t.nhom=sl1.nhom
) as k]]>
