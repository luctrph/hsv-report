	<queryString language="SQL">
		<![CDATA[select row_number() over() as stt ,value,name,b,c,d,e, 
case when value in ('246','247','248','249','250','251','252','253','254','255','256','257','258','259','260','261','262','263','264','2641') then e end as s_266
,case when value in ('246','247','248','249','250','251','252','253','254','255','256','257','258','259','260','261','262','263','264','2641') then b end as m_266
, case when value in ('2441','242','2421','245') then e end as s_267
, case when value in ('2441','242','2421','245') then b end as m_267

,  case when value in ('243') then e end as s_245
,  case when value in ('243') then b end as m_245
, case when value in ('243') then d end as sl_243
, case when value in ('243') then b end as m_243

from(
SELECT dm.value,dm.name,dm.hsv_dinhmuc as b, 
case when dm.value in ('240','244','2441') then null::int else
case when dm.value in ('241','243','242','2421','245','2421') then t.sl end 
end as c 
, case when dm.value in ('240','244') then null::int else t.tt end as d
,case when dm.value in ('240','244') then null::int else 
case when dm.value  in ('241', '243') and t.sl <> 0 then t.tt/t.sl  else 
case when sl1.sl <> 0 then  t.tt/sl1.sl else 0 end end end as e
from(	--Bao cao tinh gia thanh BOT CA ngay 12/01/2021
		--Chi tieu 240:I    THÀNH PHẨM BOT CA
		SELECT 'I' as nhom,'240' as ct,0 as sl,0 as tt from ad_client where ad_client_id =$P{AD_CLIENT_ID}
		
		--Chi tieu 241:1.  Tổng  bột cá  SX (Lần 1)
		union all
		
		SELECT 'I' as nhom,'241' as ct,sum(t.sl) as sl,0 as tt
		from(	SELECT COALESCE(sum(civ.productionqty),0 ) as sl
				from m_productionplan civ
				join m_product hh on hh.m_product_id =civ.m_product_id and hh.m_attributeset_id in (1000086,1000152)
				join m_production ci on ci.m_production_id =civ.m_production_id and ci.docstatus in ('CO','CL') and ci.movementdate >=$P{StartDate} and ci.movementdate <=$P{EndDate}
					and ci.c_activity_id =1000272
				where civ.ad_client_id =$P{AD_CLIENT_ID} 
				union all 
				SELECT -1*COALESCE(sum(civ.qtyinternaluse),0 ) as sl
				from m_inventoryline civ
				join m_product hh on hh.m_product_id =civ.m_product_id and hh.m_attributeset_id in (1000086,1000152)
				join m_inventory ci on ci.m_inventory_id =civ.m_inventory_id and ci.docstatus in ('CO','CL') and ci.movementdate >=$P{StartDate} and ci.movementdate <=$P{EndDate}
					and ci.c_activity_id =1000272
				where civ.ad_client_id =$P{AD_CLIENT_ID} 
				
			) as t
			
		--Chi tieu 242:2. Bột cá mua 	
		
		union all
		
		SELECT 'I' as nhom,'243' AS ct,COALESCE(sum(t.sl)) AS sl,COALESCE(sum(t.tt)) AS tt
		FROM (SELECT -1*COALESCE(sum(fc.qty),0) AS sl,COALESCE(sum(fc.amtacctcr),0)-COALESCE(sum(fc.amtacctdr),0) AS tt
		      FROM fact_acct fc 
		      JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,2)='51'
		      JOIN M_product hh ON fc.m_product_id=hh.m_product_id AND hh.M_AttributeSet_ID IN ('1000086') 
		      WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id<>224 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate}
		        		
		      UNION ALL
						
		      SELECT -1*COALESCE(sum(fc.qty),0) AS sl,COALESCE(sum(fc.amtacctcr),0)-COALESCE(sum(fc.amtacctdr),0) AS tt
		      FROM fact_acct fc 
		      JOIN c_elementvalue acct ON fc.account_id=acct.c_elementvalue_id AND LEFT(acct.value,2)='51'
		      JOIN gl_journalline gl ON  Fc.record_id=gl.gl_journal_id AND Fc.line_id=gl.gl_journalline_id AND Fc.Account_ID=gl.Account_ID
		                                   AND gl.IsGenerated='N'
		      join m_product hh on hh.m_product_id =fc.m_product_id and hh.M_AttributeSet_ID=1000086
		      WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id=224 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate}
		                  
			) AS t       
			
		--Chi tieu 244:II.   NGUYÊN LIỆU 
