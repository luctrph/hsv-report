		      WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.ad_table_id=321 AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} and fc.c_activity_id =1000272
		      
		      union all
		      
		      SELECT -1*COALESCE(sum(fc.qty),0) AS sl,COALESCE(sum(fc.amtacctcr-fc.amtacctdr),0) AS tt
		      FROM fact_acct fc 
