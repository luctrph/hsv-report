		      WHERE  fc.ad_client_id=$P{AD_CLIENT_ID} AND fc.dateacct>=$P{StartDate} AND fc.dateacct<=$P{EndDate} and fc.ad_table_id=224 and fc.C_Activity_ID=1000272
		      
			) AS t			
		union ALl
		--Chi tieu 245:1. Từ cá nguyên liệu,đầu xương…	
