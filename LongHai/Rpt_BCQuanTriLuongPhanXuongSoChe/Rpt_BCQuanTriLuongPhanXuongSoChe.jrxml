                            join m_product hh on civ.m_product_id=hh.m_product_id  and hh.M_Product_ID=1016237 and  hh.M_AttributeSet_ID=1000089
                            join m_attributesetinstance tt on civ.m_attributesetinstance_id=tt.m_attributesetinstance_id and civ.ad_client_id=tt.ad_client_id
                                   and tt.M_AttributeSet_ID=1000089
                     where civ.ad_client_id=$P{AD_CLIENT_ID} and (civ.HR_Job_ID=$P{HR_Job_ID} or $P{HR_Job_ID} is null)
