                    join hsv_luongnasa l on t.chitieu=l.value and l.ad_client_id=$P{AD_CLIENT_ID} 	
                    join (select tt.m_attributesetinstance_id from m_attributeinstance tt 
                              join m_attributevalue gtt on tt.m_attribute_id=gtt.m_attribute_id and tt.m_attributevalue_id=gtt.m_attributevalue_id and gtt.M_AttributeValue_ID=1000352 and  gtt.ad_client_id=$P{AD_CLIENT_ID} 
                                  where  tt.M_Attribute_ID=1000054 and tt.ad_client_id=$P{AD_CLIENT_ID} 
