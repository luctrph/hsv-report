	<queryString>
		<![CDATA[select nhom, row_number() over (partition by nhom) as stt, to_char(lvs.dateto,'DD-MM-YYYY') as dateto,lvs."name",
sum(a4) as a4,sum(a5) as a5,sum(a51) as a51,sum(a6) as a6,sum(a7) as a7,sum(a8) as a8,sum(a9) as a9, sum(a9)-sum(a6) as a10,
case when (sum(a7)-sum(a51))<>0 then (sum(a4)+ sum(a5))/(sum(a7)-sum(a51)) else 0 end  as a11 

from hsv_lovansay lvs join 
( 
		select  ctl."name" as nhom,lvs.hsv_lovansay_id, 
		sum(case when hh.hsv_loaimat_id not in (1000034) and hh.m_product_category_id in (1000492,1000493) then -1 *fc.qty  else 0 end) as a4, 
		sum(case when hh.m_product_id in (1015083) then -1* fc.qty  else 0 end) as a5,
		0 as a51,
		sum(case when hh.hsv_loaimat_id not in (1000034) and hh.m_product_category_id in (1000492,1000493) then fc.amtacctcr - fc.amtacctdr else 0 end) +
		sum(case when hh.m_product_id in (1015083) then fc.amtacctcr - fc.amtacctdr else 0 end) as a6,
		0 as a7, 0 as a8,0 as a9
from fact_acct fc
join m_inventory  ci on fc.record_id = ci.m_inventory_id and fc.ad_table_id = 321 and ci.docstatus in('Cl','CO')
join m_product hh on fc.m_product_id = hh.m_product_id 
join hsv_lovansay lvs on lvs.hsv_lovansay_id = ci.hsv_lovansay_id 
join c_elementvalue ev on fc.account_id = ev.c_elementvalue_id and  left(ev.value,2) in ('15') 
join m_lotctl ctl on ctl.m_lotctl_id = lvs.m_lotctl_id and (ctl.m_lotctl_id =$P{M_LotCtl_ID} or $P{M_LotCtl_ID} is null)
where fc.ad_client_id = $P{AD_CLIENT_ID} and fc.c_activity_id = 1000272
group by lvs.hsv_lovansay_id,ctl."name" 	
union all

select ctl."name" as nhom,lvs.hsv_lovansay_id, 0 as a4, 0 as a5,0 as a51, 0 as a6, sum(civ.productionqty) as a7,0 as a8,0 as a9-- sum(civ.productionqty)* coalesce(dgl.hsv_dinhmuc,0)  as a9
from m_production ci 
join m_productionplan civ on ci.m_production_id = civ.m_production_id and ci.docstatus in ('CL','CO') 
join m_product hh on hh.m_product_id = civ.m_product_id and hh.m_attributeset_id in (1000088,1000086,1000150,1000151,1000152) 
join hsv_lovansay lvs on lvs.hsv_lovansay_id = ci.hsv_lovansay_id
join m_lotctl ctl on ctl.m_lotctl_id =lvs.m_lotctl_id and (ctl.m_lotctl_id =$P{M_LotCtl_ID} or $P{M_LotCtl_ID} is null)
where ci.ad_client_id = $P{AD_CLIENT_ID} and ci.c_activity_id = 1000272 and (lvs.hsv_lovansay_id = $P{hsv_Lovansay_ID} or $P{hsv_Lovansay_ID} is null)
group by ctl.name,lvs.hsv_lovansay_id
union all 

select ctl."name" as nhom,lvs.hsv_lovansay_id, 0 as a4, 0 as a5,0 as a51, 0 as a6, 0 as a7,0 as a8, sum(civ.productionqty)* coalesce(dgl.hsv_dinhmuc,0)  as a9
from m_production ci 
join m_productionplan civ on ci.m_production_id = civ.m_production_id and ci.docstatus in ('CL','CO') 
join m_product hh on hh.m_product_id = civ.m_product_id and hh.m_attributeset_id in (1000088,1000086,1000150,1000151,1000152) 
left join hsv_nkdongialine dgl on dgl.m_product_id = civ.m_product_id 
left join hsv_dongia dg on dg.hsv_dongia_id = dgl.hsv_dongia_id and dg.hsv_loaidongia_id=1000025
join hsv_lovansay lvs on lvs.hsv_lovansay_id = ci.hsv_lovansay_id
join m_lotctl ctl on ctl.m_lotctl_id =lvs.m_lotctl_id and (ctl.m_lotctl_id =$P{M_LotCtl_ID} or $P{M_LotCtl_ID} is null)
where ci.ad_client_id = $P{AD_CLIENT_ID} and ci.c_activity_id = 1000272 and (lvs.hsv_lovansay_id = $P{hsv_Lovansay_ID} or $P{hsv_Lovansay_ID} is null)
and lvs.dateto>= dg.startdate and lvs.dateto<= dg.enddate
group by ctl.name,dgl.hsv_dinhmuc,lvs.hsv_lovansay_id
union all 
select  ctl."name" as nhom,lvs.hsv_lovansay_id, 
