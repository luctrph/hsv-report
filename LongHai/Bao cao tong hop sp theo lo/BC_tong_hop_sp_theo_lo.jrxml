join m_lotctl ctl on ctl.m_lotctl_id = lvs.m_lotctl_id and (ctl.m_lotctl_id =$P{M_LotCtl_ID} or $P{M_LotCtl_ID} is null)
where fc.ad_client_id = $P{AD_CLIENT_ID} and fc.c_activity_id = 1000273
group by lvs.hsv_lovansay_id,ctl."name" 
