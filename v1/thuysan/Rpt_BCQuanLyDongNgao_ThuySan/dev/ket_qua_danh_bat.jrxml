<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="dau_tu_chua_phan_bo" pageWidth="802" pageHeight="595" orientation="Landscape" columnWidth="802" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0" uuid="110b8325-7714-48c6-a3ff-b70c566164cc">
	<property name="com.jaspersoft.studio.data.defaultdataadapter" value="psql"/>
	<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
	<parameter name="AD_CLIENT_ID" class="java.lang.Integer"/>
	<parameter name="StartDate" class="java.util.Date"/>
	<parameter name="EndDate" class="java.util.Date"/>
	<parameter name="C_BPartner_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select id, 'II. Kết quả  đánh bắt' as dm,concat('2.',row_number() over(order by id)) as stt, noidung, sl, tt,dg,dinhmuc,luyke  
	from(
		select 4 as id,   'Ngao Đánh bắt chuyển kho' as noidung, 
		coalesce(sum(case when ci.movementdate >= $P{StartDate} then civ.productionqty end),0)  as sl ,
		coalesce(sum(case when ci.movementdate >= $P{StartDate} then 10000 * civ.productionqty end),0) as tt,
		10000 as dg, 10000 as dinhmuc,
		coalesce(sum(case when ci.movementdate>= '01-01-2017' then  10000 * civ.productionqty end),0) as luyke
		from m_productionplan civ
			join m_production ci on civ.m_production_id=ci.m_production_id  and ci.docstatus in ('CO','CL') and ci.AD_Org_ID=1000049
			and   ci.movementdate<=$P{EndDate} 
			join m_product hh on civ.m_product_id=hh.m_product_id  and hh.M_Product_ID =1000017
		where civ.ad_client_id=$P{AD_CLIENT_ID} and civ.m_locator_id = 1000122 and (ci.c_bpartner_id = $P{C_BPartner_ID} or $P{C_BPartner_ID} is null) 
		
		
		union all 
		
		select 5 as id, 'Ngao Nuôi xuất bán (511)' as noidung,sl,tt,case when sl <> 0 then tt/sl else 0 end as dg ,null as dinhmuc,luyke
		from(
			select 
			coalesce(sum(case when ci.dateacct>=$P{StartDate} then civ.qtyinvoiced end),0) as sl,
			coalesce(sum(case when ci.dateacct>=$P{StartDate} then civ.linenetamt end),0) as tt ,
			coalesce(sum(case when ci.dateacct>='01-01-2017' then civ.linenetamt end),0) as luyke  
			from c_invoice ci 
			join c_invoiceline civ on ci.c_invoice_id = civ.c_invoice_id  and ci.issotrx = 'Y'
			where civ.m_product_id = 1017197 and ci.docstatus in ('CL','CO')
			and ci.ad_org_id = 1000049 and ci.ad_client_id = $P{AD_CLIENT_ID} and (ci.c_bpartner_id = $P{C_BPartner_ID} or $P{C_BPartner_ID} is null) 
			and ci.dateacct<=$P{EndDate}
		) as t
		union all 
		
		select 6 as id, 'DT sp khác  xuất bán (511)' as noidung,sl,tt,case when sl <> 0 then tt/sl else 0 end as dg ,null as dinhmuc,luyke
		from(
			select 
				coalesce(sum(case when ci.dateacct>=$P{StartDate} then civ.qtyinvoiced end),0) as sl,
				coalesce(sum(case when ci.dateacct>=$P{StartDate} then civ.linenetamt end),0) as tt ,
				coalesce(sum(case when ci.dateacct>='01-01-2017' then civ.linenetamt end),0) as luyke  
			
			from c_invoice ci 
			join c_invoiceline civ on ci.c_invoice_id = civ.c_invoice_id  and ci.issotrx = 'Y'
			where civ.m_product_id <> 1017197 and ci.docstatus in ('CL','CO')
			and ci.ad_org_id = 1000049 and ci.ad_client_id = $P{AD_CLIENT_ID}
			 and ci.dateacct<=$P{EndDate} and (ci.c_bpartner_id = $P{C_BPartner_ID} or $P{C_BPartner_ID} is null) 
		) as t	
		union all
		
		select 7 as id,'thu nhập khác (711)' as noidung, null::int4 as sl, coalesce(sum(case when dateacct>=$P{StartDate} then  amtacctcr-amtacctdr end),0) as tt,null::int4 as dg,
		 null as dinhmuc,coalesce(sum(case when dateacct >= '01-01-2017' then  amtacctcr-amtacctdr end),0) as luyke 
		from(
			SELECT  fc.amtacctdr, fc.amtacctcr, fc.account_id, fc.c_activity_id, fc.dateacct 
			from fact_acct fc
			join gl_journalline gl on fc.record_id = gl.gl_journal_id and fc.line_id = gl.gl_journalline_id and fc.Account_ID = gl.Account_ID and gl.IsGenerated = 'N'
			join gl_journal a on a.gl_journal_id = fc.record_id and a.docstatus in ('CO', 'CL')
			where  fc.ad_client_id = $P{AD_CLIENT_ID} and fc.ad_table_id = 224 and fc.ad_org_id = 1000049
			and (fc.c_bpartner_id = $P{C_BPartner_ID} or $P{C_BPartner_ID} is null) 
			union all
		 	SELECT  fc.amtacctdr, fc.amtacctcr, fc.account_id, fc.c_activity_id, fc.dateacct 
			from fact_acct fc
			where  fc.ad_client_id = $P{AD_CLIENT_ID} and fc.ad_table_id <> 224 and fc.ad_org_id = 1000049
			and (fc.c_bpartner_id = $P{C_BPartner_ID} or $P{C_BPartner_ID} is null) 
		) as t
		join c_elementvalue acct on t.account_id = acct.c_elementvalue_id and LEFT(acct.value, 3) in ('711')	
		where  dateacct<=$P{EndDate}
		
	) as k]]>
	</queryString>
