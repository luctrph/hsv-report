	<queryString>
		<![CDATA[WITH giao_dich AS (
    SELECT 
     	ma.m_attribute_id,
      	mav.hsv_c_thuoctinh_id,
        hh.name,
        hh.value,
        m.movementdate,
        m.movementqty,
        RIGHT(m.movementtype, 1) AS tang_giam
    FROM m_transaction m
    JOIN m_product hh ON hh.m_product_id = m.m_product_id
    JOIN m_product_category hhm ON hh.m_product_category_id = hhm.m_product_category_id
    JOIN m_locator kho ON m.m_locator_id = kho.m_locator_id
    join m_attributeinstance ma on ma.m_attributesetinstance_id = m.m_attributesetinstance_id  
    join m_attributevalue mav on mav.m_attributevalue_id = ma.m_attributevalue_id 
    WHERE m.ad_client_id = $P{AD_CLIENT_ID}
      AND m.movementdate <= $P{EndDate} 
      and hh.m_product_id = 1000017

),
giao_dich_2 as (
	SELECT 
        hh.name,
        hh.value,
        m.movementdate,
        m.movementqty,
        RIGHT(m.movementtype, 1) AS tang_giam
    FROM m_transaction m
    JOIN m_product hh ON hh.m_product_id = m.m_product_id
    JOIN m_product_category hhm ON hh.m_product_category_id = hhm.m_product_category_id
    JOIN m_locator kho ON m.m_locator_id = kho.m_locator_id
    WHERE m.ad_client_id = $P{AD_CLIENT_ID}
      AND m.movementdate <= $P{EndDate} 
      and hh.m_product_id = 1000017
),
chuyen_kho as (
	select coalesce(sum(mmv.movementqty),0) as qty 
	from m_movement mm
	join m_movementline mmv on mm.m_movement_id = mmv.m_movement_id 
	where mmv.ad_client_id = $P{AD_CLIENT_ID} and mm.movementdate >= $P{StartDate} and mm.movementdate <= $P{EndDate} 
	and mm.docstatus in ('CL','CO') and mmv.m_product_id = 1000302 and mmv.m_locator_id = 1000297 and mmv.m_locatorto_id = 1000251
),
nhap_mua_tt as (
	select sum(miv.movementqty) as qty 
	from m_inout mi 
	join m_inoutline miv on mi.m_inout_id = miv.m_inout_id 
	join m_attributeinstance ma on ma.m_attributesetinstance_id = miv.m_attributesetinstance_id
	join m_attributevalue mav on mav.m_attributevalue_id = ma.m_attributevalue_id
	where mi.ad_client_id = $P{AD_CLIENT_ID}
	and miv.m_product_id = 1000017 and ma.m_attribute_id =1000022 and mav.hsv_c_thuoctinh_id = 1000021
	and mi.movementdate BETWEEN $P{StartDate} AND $P{EndDate} and mi.docstatus in ('CL','CO')
	
),
nhap_mua as (
	select sum(miv.movementqty) as qty 
	from m_inout mi 
	join m_inoutline miv on mi.m_inout_id = miv.m_inout_id 
	where mi.ad_client_id = $P{AD_CLIENT_ID}
	and miv.m_product_id = 1000017 
	and mi.movementdate BETWEEN $P{StartDate} AND $P{EndDate} and mi.docstatus in ('CL','CO')
	
),
nhap_kho AS (
    select sum(civ.productionqty) as qty, hh.hsv_loaimat_id, ci.c_campaign_id  
    from m_production ci 
    join m_productionplan civ on ci.m_production_id = civ.m_production_id 
    join m_product hh on hh.m_product_id = civ.m_product_id 
    where ci.ad_client_id = $P{AD_CLIENT_ID}
      and ci.movementdate between $P{StartDate} and $P{EndDate}
      and ci.docstatus in ('CL','CO')
    group by hh.hsv_loaimat_id, ci.c_campaign_id
),
ton_kho AS (
    SELECT
        t.m_attribute_id,
        t.hsv_c_thuoctinh_id,
        SUM(CASE WHEN t.movementdate < $P{StartDate} THEN t.movementqty ELSE 0 END) AS dudau,
        SUM(CASE WHEN t.movementdate BETWEEN $P{StartDate} AND $P{EndDate} AND t.tang_giam = '+' THEN t.movementqty ELSE 0 END) AS sln,
        SUM(CASE WHEN t.movementdate BETWEEN $P{StartDate} AND $P{EndDate} AND t.tang_giam = '-' THEN -t.movementqty ELSE 0 END) AS slx,
        SUM(CASE WHEN t.movementdate <= $P{EndDate} THEN t.movementqty ELSE 0 END) AS ducuoi
    FROM giao_dich t
    GROUP BY t.m_attribute_id, t.hsv_c_thuoctinh_id
),
ton_kho_2 AS (
    SELECT
        SUM(CASE WHEN t.movementdate < $P{StartDate} THEN t.movementqty ELSE 0 END) AS dudau,
        SUM(CASE WHEN t.movementdate BETWEEN $P{StartDate} AND $P{EndDate} AND t.tang_giam = '+' THEN t.movementqty ELSE 0 END) AS sln,
        SUM(CASE WHEN t.movementdate BETWEEN $P{StartDate} AND $P{EndDate} AND t.tang_giam = '-' THEN -t.movementqty ELSE 0 END) AS slx,
        SUM(CASE WHEN t.movementdate <= $P{EndDate} THEN t.movementqty ELSE 0 END) AS ducuoi
    FROM giao_dich_2 t
),

--  Tổng hợp các chỉ tiêu 1–4
kq AS (
    select 1 as id,'Tồn đầu kỳ' as noidung, sum(ngao_nau) as ngao_nau , sum(ngao_nl) as ngao_nl 
    from (
        select sum(t.dudau) FILTER (
             where  t.M_Attribute_ID=1000022 
             and  t.hsv_C_thuoctinh_ID=1000021
        ) as ngao_nau, 0 as ngao_nl
        from ton_kho t
        union all 
        select 0 as ngao_nau , sum(dudau) as ngao_nl
        from ton_kho_2
    ) as k

    union all 

    select 2 as id,'Nhập mua trong kỳ' as noidung, sum(ngao_nau), sum(ngao_nl)
    from (
        select sum(qty) as ngao_nau, 0 as ngao_nl 
        from nhap_mua_tt
        union all 
        select 0 as ngao_nau , sum(qty) as ngao_nl 
        from nhap_mua 
    ) as k

    union all 

    select 3 as id,'Tồn cuối kỳ' as noidung, sum(ngao_nau) as ngao_nau , sum(ngao_nl) as ngao_nl 
    from (
        select sum(t.ducuoi) FILTER (
             where  t.M_Attribute_ID=1000022 
             and  t.hsv_C_thuoctinh_ID=1000021
        ) as ngao_nau, 0 as ngao_nl
        from ton_kho t
        union all 
        select 0 as ngao_nau , sum(ducuoi) as ngao_nl
        from ton_kho_2
    ) as k

    union all 

    select 4 as id,'Ngao sử dụng tại long hải' as noidung,
           sum(ngao_nau) as ngao_nau , sum(ngao_nl) as ngao_nl 
    from (
        select 1.15 * sum(t.qty) FILTER (
            where t.hsv_loaimat_id = 1000020
            and t.c_campaign_id = 1000007
        ) as ngao_nau , 0 as ngao_nl 
        from nhap_kho t
        union all 
        select 0 as ngao_nau , sum(t.qty) as ngao_nl 
        from chuyen_kho t
    ) as k
),
--  Chỉ tiêu 5
kq5 AS (
    SELECT 
        5 AS id,
        'Đưa vào sử dụng trong kỳ' AS noidung,
        SUM(CASE WHEN id = 1 THEN ngao_nau WHEN id = 2 THEN ngao_nau WHEN id = 3 THEN -ngao_nau WHEN id = 4 THEN -ngao_nau END) AS ngao_nau,
        
        SUM(CASE WHEN id = 1 THEN ngao_nl  WHEN id = 2 THEN ngao_nl  WHEN id = 3 THEN -ngao_nl  WHEN id = 4 THEN -ngao_nl  END) AS ngao_nl 
    FROM kq
),

--  Chỉ tiêu 6
kq6 AS (
    SELECT 
        6 AS id,
        'Thành phẩm nhập kho từ nguyên liệu' AS noidung,
        SUM(qty) FILTER (
            WHERE hsv_loaimat_id = 1000020
              AND c_campaign_id IN (1000093,1000094,1000018)
        ) AS ngao_nau,
        SUM(qty) FILTER (
            WHERE hsv_loaimat_id = 1000021
              AND c_campaign_id IN (1000093,1000094,1000018)
        ) AS ngao_trang
    FROM nhap_kho
),


--  Gom tất cả các chỉ tiêu trước (1–4)
kq_all AS (
    SELECT kq.id, kq.noidung,kq.ngao_nau, kq.ngao_nl- kq.ngao_nau as ngao_trang  
    FROM kq
    UNION ALL
    SELECT kq.id, kq.noidung,kq.ngao_nau, kq.ngao_nl- kq.ngao_nau FROM kq5  kq 
    UNION ALL
    SELECT kq.id, kq.noidung,kq.ngao_nau, kq.ngao_trang  FROM kq6 kq
),



-- Chỉ tiêu 7: TP ngao nâu lẫn >10% lô ngao trắng
kq7 AS (
    SELECT
        7 AS id,
        'TP ngao nâu lẫn > 10% lô ngao trắng' AS noidung,
        (
            MAX(CASE WHEN id = 6 THEN ngao_nau END)
            - MAX(CASE WHEN id = 5 THEN ngao_nau END)/1.15
            - 0.1 * MAX(CASE WHEN id = 5 THEN ngao_trang END)/1.15
        ) AS ngao_nau,
        NULL::numeric AS ngao_trang 
    FROM kq_all
)

-- Tổng hợp kết quả cuối
SELECT 
    id,
    noidung,
    ngao_nau,
    ngao_trang
	FROM 
     kq_all
    UNION ALL
    SELECT id,
    noidung,
    ngao_nau,
    ngao_trang
    FROM kq7

]]>
