	<queryString>
		<![CDATA[select 'I' as nhom,1 as id,'Số lượng sản phẩm sản xuất trong  kỳ  (ĐH)' as chitieu,
	0 as cp,
	COALESCE(sum(t.slb),0) as slb,
	COALESCE(sum(t.slb1),0) as slb1,
	COALESCE(sum(t.slb5),0) as slb5,
	coalesce(sum(slnt),0) as slnt,
	coalesce(sum(slnm),0) as slnm,
	0 as cp1
	from (
			select  pl.m_productionplan_id as m_productionline_id,pl.m_product_id,pl.productionqty as slb,0 as slb1,0 as slb5,0 as slnt, 0 as slnm ,pl.m_attributesetinstance_id
			from m_productionplan pl
			join m_product hh on pl.m_product_id=hh.m_product_id and  hh.M_AttributeSet_ID=1000000
			join m_production p on pl.m_production_id=p.m_production_id  and p.docstatus in ('CO','CL') and  (p.AD_org_id=1000027)
			
			join (
				select tt.m_attributesetinstance_id 
				from m_attributeinstance tt
				join m_attributevalue gtt on tt.m_attributevalue_id=gtt.m_attributevalue_id and hsv_c_thuoctinh_id=1000013 and gtt.m_attribute_id=1000001
				WHERE tt.ad_client_id=$P{AD_CLIENT_ID}
			) tt on pl.m_attributesetinstance_id=tt.m_attributesetinstance_id
			where  pl.ad_client_id=$P{AD_CLIENT_ID} and p.movementdate>=$P{StartDate} and p.movementdate<=$P{EndDate}
		
			union all
		
			select  pl.m_productionplan_id as m_productionline_id,pl.m_product_id,0 as slb,pl.productionqty as slb1,0 as slb5,0 as slnt, 0 as slnm ,pl.m_attributesetinstance_id
			from m_productionplan pl
			join m_product hh on pl.m_product_id=hh.m_product_id and  hh.M_AttributeSet_ID=1000000
			join m_production p on pl.m_production_id=p.m_production_id  and p.docstatus in ('CO','CL') and  (p.AD_org_id=1000027)
			join (
				select tt.m_attributesetinstance_id 
				from m_attributeinstance tt
				join m_attributevalue gtt on tt.m_attributevalue_id=gtt.m_attributevalue_id and hsv_c_thuoctinh_id=1000005 and gtt.m_attribute_id=1000001
				WHERE tt.ad_client_id=$P{AD_CLIENT_ID}
			) tt on pl.m_attributesetinstance_id=tt.m_attributesetinstance_id
			where  pl.ad_client_id=$P{AD_CLIENT_ID} and p.movementdate>=$P{StartDate} and p.movementdate<=$P{EndDate}
			
			union all
		
			select  pl.m_productionplan_id as m_productionline_id,pl.m_product_id,0 as slb,0 as slb1,pl.productionqty as slb5,0 as slnt, 0 as slnm ,pl.m_attributesetinstance_id
			from m_productionplan pl
			join m_product hh on pl.m_product_id=hh.m_product_id and  hh.M_AttributeSet_ID=1000000
			join m_production p on pl.m_production_id=p.m_production_id  and p.docstatus in ('CO','CL') and  (p.AD_org_id=1000027)		
			join (
				select tt.m_attributesetinstance_id 
				from m_attributeinstance tt
				join m_attributevalue gtt on tt.m_attributevalue_id=gtt.m_attributevalue_id and hsv_c_thuoctinh_id in(1000006,1000007,1000019) and gtt.m_attribute_id=1000001
				WHERE tt.ad_client_id=$P{AD_CLIENT_ID}
			) tt on pl.m_attributesetinstance_id=tt.m_attributesetinstance_id
			where  pl.ad_client_id=$P{AD_CLIENT_ID} AND p.movementdate>=$P{StartDate} and p.movementdate<=$P{EndDate}
			
			union all 
			
			select  pl.m_productionplan_id as m_productionline_id,pl.m_product_id,0 as slb,0 as slb1,0 as slb5, case when hh.M_AttributeSet_ID=1000062 then pl.productionqty else 0 end as slnt, case when hh.M_AttributeSet_ID=1000018 then pl.productionqty else 0 end as slnm ,pl.m_attributesetinstance_id
			from m_productionplan pl
			join m_product hh on pl.m_product_id=hh.m_product_id and  hh.M_AttributeSet_ID=1000062
			join m_production p on pl.m_production_id=p.m_production_id  and p.docstatus in ('CO','CL') and  (p.AD_org_id=1000027)
			where  pl.ad_client_id=$P{AD_CLIENT_ID} AND p.movementdate>=$P{StartDate} and p.movementdate<=$P{EndDate}
							
	) as t
	
	union all 
	
	SELECT 'I' AS nhom,2 as id,'Số lượng SP đã  bán  trong kỳ  (ĐH)' AS chitieu,0 as cp,
		COALESCE(sum(slb),0) as slb,
		COALESCE(sum(slb1),0) as slb1,
		COALESCE(sum(slb5),0) as slb5,
		coalesce(sum(slnt),0) as slnt,
		coalesce(sum(slnm),0) as slnm,
		0 as cp1
	from(
		
		select 
			(CASE WHEN ta.hsv_c_thuoctinh_id = 1000013 THEN io.movementqty ELSE 0 end) AS slb,
		    (CASE WHEN ta.hsv_c_thuoctinh_id = 1000005 THEN io.movementqty ELSE 0 END) AS slb1,
		    (CASE WHEN ta.hsv_c_thuoctinh_id IN (1000006, 1000007, 1000019) THEN io.movementqty ELSE 0 END) AS slb5,
			0 as slnt,
			0 as slnm
	   
		FROM m_inoutline io
		JOIN  m_inout iol ON io.m_inout_id = iol.m_inout_id  AND iol.docstatus IN ('CO', 'CL')   AND iol.issotrx = 'Y'  AND iol.AD_org_id = 1000027
		JOIN m_product hh ON io.m_product_id = hh.m_product_id  AND hh.M_AttributeSet_ID = 1000000
		JOIN (
			SELECT tt.m_attributesetinstance_id,  gtt.hsv_c_thuoctinh_id
		    FROM  m_attributeinstance tt
		    JOIN m_attributevalue gtt ON tt.m_attributevalue_id = gtt.m_attributevalue_id 
		    WHERE tt.ad_client_id = $P{AD_CLIENT_ID}
		) ta ON io.m_attributesetinstance_id = ta.m_attributesetinstance_id
		WHERE io.ad_client_id = $P{AD_CLIENT_ID} and iol.movementdate >= $P{StartDate} AND iol.movementdate <= $P{EndDate}
		
		union all 
		
		select 	0 AS slb,
		   	 	0 AS slb1,
		    	0 AS slb5,
				case when hh.M_AttributeSet_ID=1000062 then io.movementqty  else 0 end as slnt, 
				case when hh.M_AttributeSet_ID=1000018 then io.movementqty else 0 end as slnm 
		FROM m_inoutline io
		JOIN  m_inout iol ON io.m_inout_id = iol.m_inout_id  AND iol.docstatus IN ('CO', 'CL')   AND iol.issotrx = 'Y' AND iol.AD_org_id = 1000027
		JOIN m_product hh ON io.m_product_id = hh.m_product_id  
		and io.ad_client_id =$P{AD_CLIENT_ID} and iol.dateacct  >= $P{StartDate} AND iol.dateacct  <= $P{EndDate}
	) as k
	union all 
	
	SELECT 'I' AS nhom,3 as id,'Số lượng SP đã bán luỹ kế năm (ĐH)' AS chitieu,0 as cp,
		COALESCE(sum(slb),0) as slb,
		COALESCE(sum(slb1),0) as slb1,
		COALESCE(sum(slb5),0) as slb5,
		coalesce(sum(slnt),0) as slnt,
		coalesce(sum(slnm),0) as slnm,
		0 as cp1
	from(
		
		select 
			(CASE WHEN ta.hsv_c_thuoctinh_id = 1000013 THEN io.movementqty ELSE 0 end) AS slb,
		    (CASE WHEN ta.hsv_c_thuoctinh_id = 1000005 THEN io.movementqty ELSE 0 END) AS slb1,
		    (CASE WHEN ta.hsv_c_thuoctinh_id IN (1000006, 1000007, 1000019) THEN io.movementqty ELSE 0 END) AS slb5,
			0 as slnt,
			0 as slnm
	   
		FROM m_inoutline io
		JOIN  m_inout iol ON io.m_inout_id = iol.m_inout_id  AND iol.docstatus IN ('CO', 'CL')   AND iol.issotrx = 'Y'  AND iol.AD_org_id = 1000027
		JOIN m_product hh ON io.m_product_id = hh.m_product_id  AND hh.M_AttributeSet_ID = 1000000
		JOIN (
			SELECT tt.m_attributesetinstance_id,  gtt.hsv_c_thuoctinh_id
		    FROM  m_attributeinstance tt
		    JOIN m_attributevalue gtt ON tt.m_attributevalue_id = gtt.m_attributevalue_id 
		    WHERE tt.ad_client_id = $P{AD_CLIENT_ID}
		) ta ON io.m_attributesetinstance_id = ta.m_attributesetinstance_id
		WHERE io.ad_client_id = $P{AD_CLIENT_ID} and iol.movementdate >= date_trunc('year', $P{EndDate}::date)::date AND iol.movementdate <= $P{EndDate}
		
		union all 
		
		select 	0 AS slb,
		   	 	0 AS slb1,
		    	0 AS slb5,
				case when hh.M_AttributeSet_ID=1000062 then io.movementqty  else 0 end as slnt, 
				case when hh.M_AttributeSet_ID=1000018 then io.movementqty else 0 end as slnm 
		FROM m_inoutline io
		JOIN  m_inout iol ON io.m_inout_id = iol.m_inout_id  AND iol.docstatus IN ('CO', 'CL')   AND iol.issotrx = 'Y' AND iol.AD_org_id = 1000027
		JOIN m_product hh ON io.m_product_id = hh.m_product_id  
		and io.ad_client_id =$P{AD_CLIENT_ID} and iol.dateacct  >= date_trunc('year', $P{EndDate}::date)::date AND iol.dateacct  <= $P{EndDate}
	) as k]]>
	</queryString>
