	<queryString>
		<![CDATA[SELECT 
    co.c_order_id,
    hh.m_product_id,
    CONCAT(co.documentno,'_',TO_CHAR(co.dateordered,'DD-MM-YYYY')) AS dh,
    hh.name,
    cov.qtyordered,
    cov.priceentered,
    cov.linenetamt,
    COALESCE(k.tygia,1) AS tygia,
    cov.linenetamt * COALESCE(k.tygia,1) AS vnd,
    cov.qtyordered * COALESCE(z.hsv_dinhmuc,0) AS nguyenlieu,
    COALESCE((j.vc * COALESCE(k.tygia,1) * cov.qtyordered)/26000,0) AS cpvc,
    cov.qtyordered * COALESCE(cp_dm.cpdm,0) AS cpdm,
    cov.qtydelivered,
    cc.iso_code 
FROM c_order co
JOIN c_orderline cov ON co.c_order_id = cov.c_order_id
join c_currency cc on cc.c_currency_id = co.c_currency_id 
JOIN m_product hh ON hh.m_product_id = cov.m_product_id AND hh.m_attributeset_id in (1000000,1000199)
JOIN m_attributeinstance ma ON ma.m_attributesetinstance_id = cov.m_attributesetinstance_id AND ma.M_Attribute_ID in (1000001,1000146)

-- Lấy tỷ giá mới nhất cho từng đơn hàng
LEFT JOIN LATERAL (
    SELECT cr.multiplyrate AS tygia
    FROM c_conversion_rate cr
    WHERE cr.ad_client_id = $P{AD_CLIENT_ID}
      AND cr.c_currency_id = co.c_currency_id
      AND cr.c_conversiontype_id = co.c_conversiontype_id
      AND co.dateordered BETWEEN cr.validfrom AND COALESCE(cr.validto, co.dateordered)
    ORDER BY cr.validfrom DESC
    LIMIT 1
) k ON TRUE

-- Lấy đơn giá cước vận chuyển
LEFT JOIN LATERAL (
    SELECT dgl.hsv_dinhmuc1 AS vc
    FROM hsv_dongia dg
    JOIN hsv_nkdongialine dgl ON dg.hsv_dongia_id = dgl.hsv_dongia_id
    WHERE dg.ad_client_id = $P{AD_CLIENT_ID}
      AND dg.hsv_loaidongia_id in (1000056,1000074)
      AND dgl.hsv_cangbien_id = co.hsv_cangbien_id
      AND co.dateordered BETWEEN dg.startdate AND dg.enddate
    LIMIT 1
) j ON TRUE

-- Lấy đơn giá định mức
LEFT JOIN LATERAL (
    SELECT dgl.amountdg1 AS cpdm
    FROM hsv_dongia dg
    JOIN hsv_nkdongialine dgl ON dg.hsv_dongia_id = dgl.hsv_dongia_id
    WHERE dg.ad_client_id = $P{AD_CLIENT_ID}
      AND dg.hsv_loaidongia_id in (1000054,1000072)
      AND dgl.m_attributevalue_id = ma.m_attributevalue_id
      AND co.dateordered BETWEEN dg.startdate AND dg.enddate
    LIMIT 1
) cp_dm ON TRUE

-- Lấy định mức sản phẩm
LEFT JOIN LATERAL (
    SELECT dgl.hsv_dinhmuc
    FROM hsv_dongia dg
    JOIN hsv_nkdongialine dgl ON dg.hsv_dongia_id = dgl.hsv_dongia_id
    WHERE dg.ad_client_id = $P{AD_CLIENT_ID}
      AND dg.hsv_loaidongia_id in (1000055,1000073)
      AND dgl.m_product_id = hh.m_product_id
      AND co.dateordered BETWEEN dg.startdate AND dg.enddate
    LIMIT 1
) z ON TRUE
WHERE co.issotrx = 'Y'
 and co.docstatus in ('CO') 
  AND cov.qtyinvoiced = 0
  AND co.dateordered BETWEEN $P{StartDate} AND $P{EndDate}
  and co.c_currency_id = $P{C_Currency_ID}
ORDER BY co.documentno;]]>
	</queryString>
