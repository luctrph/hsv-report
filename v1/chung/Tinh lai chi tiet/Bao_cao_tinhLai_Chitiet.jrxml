	<queryString>
		<![CDATA[WITH p AS (
    SELECT startdate::date, enddate::date
    FROM c_period
    WHERE c_period_id = $P{C_Period_ID}
      AND ad_client_id = $P{AD_CLIENT_ID}
),

-- 1. Danh sách ngày trong kỳ
days AS (
    SELECT generate_series(p.startdate, p.enddate, interval '1 day')::date AS ngaythang
    FROM p
),

-- 2. Phát sinh theo ngày
ps_ngay AS (
    SELECT
        t.c_bpartner_id,
        t.dateacct::date AS ngay,
        SUM(t.amtacctcr) AS psco,
        SUM(t.amtacctdr) AS psno
    FROM fact_acct t
    CROSS JOIN p
    WHERE t.ad_client_id   = $P{AD_CLIENT_ID}
      AND t.account_id     = $P{Account_ID}
      AND t.c_bpartner_id  = $P{C_BPartner_ID}
      AND t.dateacct <= p.enddate
    GROUP BY t.c_bpartner_id, t.dateacct::date
),

-- 3. Dư đầu kỳ
du_dau AS (
    SELECT
        t.c_bpartner_id,
        SUM(t.amtacctcr - t.amtacctdr) AS sodu_dau
    FROM fact_acct t
    CROSS JOIN p
    WHERE t.ad_client_id   = $P{AD_CLIENT_ID}
      AND t.account_id     = $P{Account_ID}
      AND t.c_bpartner_id  = $P{C_BPartner_ID}
      AND t.dateacct < p.startdate
    GROUP BY t.c_bpartner_id
),

-- 4. Ghép ngày + phát sinh
daily AS (
    SELECT
        d.ngaythang,
        b.c_bpartner_id,
        COALESCE(pn.psco, 0) AS psco,
        COALESCE(pn.psno, 0) AS psno
    FROM days d
    CROSS JOIN (
        SELECT DISTINCT c_bpartner_id
        FROM fact_acct
        WHERE c_bpartner_id = $P{C_BPartner_ID}
    ) b
    LEFT JOIN ps_ngay pn
           ON pn.c_bpartner_id = b.c_bpartner_id
          AND pn.ngay = d.ngaythang
),

-- 5. Tính số dư lũy kế từng ngày
sodu_ngay AS (
    SELECT
        d.ngaythang,
        d.c_bpartner_id,
        COALESCE(dd.sodu_dau, 0)
        + SUM(d.psco - d.psno) OVER (
            PARTITION BY d.c_bpartner_id
            ORDER BY d.ngaythang
          ) AS sodu
    FROM daily d
    LEFT JOIN du_dau dd
           ON dd.c_bpartner_id = d.c_bpartner_id
)

SELECT
    'lx' AS nhom,
    ROW_NUMBER() OVER (ORDER BY s.ngaythang) AS stt,
    s.ngaythang,
    s.sodu,
    s.sodu * rate.hsv_laisuat / 100 / 30 AS ngay
FROM sodu_ngay s
JOIN c_bpartner cb
     ON cb.c_bpartner_id = s.c_bpartner_id
JOIN c_bpartner_rate rate
     ON rate.c_bpartner_id = cb.c_bpartner_id
CROSS JOIN p
WHERE s.ngaythang BETWEEN rate.datefrom AND rate.dateto
ORDER BY s.ngaythang;
]]>
	</queryString>
