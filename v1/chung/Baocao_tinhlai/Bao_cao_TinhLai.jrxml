	<queryString>
		<![CDATA[WITH p AS (
    SELECT startdate, enddate
    FROM c_period
    WHERE c_period_id = $P{C_Period_ID}
      AND ad_client_id = $P{AD_CLIENT_ID}
),

-- danh sách ngày trong kỳ
days AS (
    SELECT generate_series(p.startdate, p.enddate, interval '1 day')::date AS ngay
    FROM p
),

-- phát sinh theo ngày
ps_ngay AS (
    SELECT
        t.c_bpartner_id,
        t.dateacct::date AS ngay,
        SUM(t.amtacctcr - t.amtacctdr) AS ps_ngay
    FROM fact_acct t
    CROSS JOIN p
    WHERE t.ad_client_id = $P{AD_CLIENT_ID}
      AND t.account_id   = $P{Account_ID}
      AND t.dateacct BETWEEN p.startdate AND p.enddate
    GROUP BY t.c_bpartner_id, t.dateacct::date
),

-- dư đầu kỳ
du_dau AS (
    SELECT
        t.c_bpartner_id,
        SUM(t.amtacctcr - t.amtacctdr) AS dudau
    FROM fact_acct t
    CROSS JOIN p
    WHERE t.ad_client_id = $P{AD_CLIENT_ID}
      AND t.account_id   = $P{Account_ID}
      AND t.dateacct < p.startdate
    GROUP BY t.c_bpartner_id
),

-- tạo bảng ngày + đối tác
daily_balance AS (
    SELECT
        d.ngay,
        b.c_bpartner_id,
        COALESCE(ps.ps_ngay, 0) AS ps_ngay
    FROM days d
    CROSS JOIN (
        SELECT DISTINCT c_bpartner_id
        FROM fact_acct
        WHERE ad_client_id = $P{AD_CLIENT_ID}
          AND account_id   = $P{Account_ID}
    ) b
    LEFT JOIN ps_ngay ps
           ON ps.c_bpartner_id = b.c_bpartner_id
          AND ps.ngay = d.ngay
),

-- tính số dư lũy kế theo ngày
so_du_ngay AS (
    SELECT
        db.c_bpartner_id,
        db.ngay,
        COALESCE(dd.dudau, 0)
        + SUM(db.ps_ngay) OVER (
            PARTITION BY db.c_bpartner_id
            ORDER BY db.ngay
          ) AS sodu_ngay
    FROM daily_balance db
    LEFT JOIN du_dau dd
           ON dd.c_bpartner_id = db.c_bpartner_id
)

SELECT
    ROW_NUMBER() OVER (ORDER BY cb.value) AS stt,
    'lx' AS nhom,
    cb.value,
    cb.name,
    rate.hsv_laisuat,
    ROUND(MIN(sdn.sodu_ngay),0) AS dudau,
    ROUND(MAX(sdn.sodu_ngay),0) AS ducuoi,
    SUM(sdn.sodu_ngay * rate.hsv_laisuat / 100 / 30) AS laithang
FROM so_du_ngay sdn
JOIN c_bpartner cb
     ON cb.c_bpartner_id = sdn.c_bpartner_id
JOIN c_bpartner_rate rate
     ON rate.c_bpartner_id = cb.c_bpartner_id
CROSS JOIN p
WHERE ($P{C_BPartner_ID} IS NULL OR cb.c_bpartner_id = $P{C_BPartner_ID})
  AND rate.hsv_laisuat IS NOT null 
  AND sdn.ngay BETWEEN rate.datefrom AND rate.dateto
GROUP BY
    cb.value,
    cb.name,
    rate.hsv_laisuat
having sum(sdn.sodu_ngay) <> 0

]]>
	</queryString>
