	<parameter name="C_Campaign_ID" class="java.lang.Integer"/>
	<parameter name="Account_ID" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[SELECT 
    CASE 
        WHEN $P{AD_Org_ID} IS NOT NULL THEN org.name 
    END AS bophan,
    CASE 
        WHEN $P{Account_ID} IS NOT NULL THEN a.value || '_' || a.name 
    END AS taikhoan,
    CASE 
        WHEN $P{C_BPartner_ID} IS NOT NULL THEN cb.name 
    END AS doituong,
    CASE 
        WHEN $P{C_Campaign_ID} IS NOT NULL THEN cc.name 
    END AS chiendich,
    CASE 
        WHEN $P{C_Activity_ID} IS NOT NULL THEN ac.name 
    END AS hoatdong
FROM c_elementvalue a
LEFT JOIN c_bpartner cb 
       ON cb.c_bpartner_id = $P{C_BPartner_ID}
LEFT JOIN c_campaign cc 
       ON cc.c_campaign_id = $P{C_Campaign_ID}
LEFT JOIN c_activity ac 
       ON ac.c_activity_id = $P{C_Activity_ID}
LEFT JOIN ad_org org 
       ON org.ad_org_id = $P{AD_Org_ID}
WHERE a.ad_client_id = $P{AD_CLIENT_ID}
  AND ($P{Account_ID} IS NULL OR a.c_elementvalue_id = $P{Account_ID})
LIMIT 1;]]>
	</queryString>
