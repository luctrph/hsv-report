	<queryString>
		<![CDATA[SELECT
    value,
    name,
    cc.iso_code,
    SUM(thu) AS thu,
    SUM(ban) AS ban,
    SUM(chi) AS chi,
    SUM(mua) AS mua,
    CASE 
        WHEN (
                -- 1. Thu và Chi cân bằng
                (SUM(thu) = SUM(chi) AND (SUM(thu) <> 0 OR SUM(chi) <> 0))
             OR 
                -- 2. Bán và Mua cân bằng
                (SUM(ban) = SUM(mua) AND (SUM(ban) <> 0 OR SUM(mua) <> 0))
             OR
                -- 3. Tổng phát sinh cân bằng (tổng thu + mua = tổng chi + bán)
                (SUM(thu) + SUM(mua) = SUM(chi) + SUM(ban)
                 AND (SUM(thu) <> 0 OR SUM(chi) <> 0 OR SUM(ban) <> 0 OR SUM(mua) <> 0))
             )
        THEN TRUE
        ELSE FALSE
    END AS note
FROM (
    SELECT cb.value, cb.name,
           SUM(CASE WHEN pay.isreceipt = 'Y' THEN pay.payamt ELSE 0 END) AS thu,
           0 AS ban,
           SUM(CASE WHEN pay.isreceipt = 'N' THEN pay.payamt ELSE 0 END) AS chi,
           0 AS mua
    FROM c_payment pay
    JOIN c_bpartner cb ON cb.c_bpartner_id = pay.c_bpartner_id 
    WHERE pay.ad_client_id = $P{AD_CLIENT_ID}
      AND pay.isallocated = 'N'
      AND pay.docstatus IN ('CL','CO')
      AND pay.c_currency_id = $P{C_Currency_ID}
    GROUP BY cb.value, cb.name
    
    UNION ALL 
    
    SELECT cb.value, cb.name,
           0 AS thu,
           SUM(CASE WHEN ci.issotrx = 'Y' THEN ci.totallines ELSE 0 END) AS ban,
           0 AS chi,
           SUM(CASE WHEN ci.issotrx = 'N' THEN ci.totallines ELSE 0 END) AS mua
    FROM c_invoice ci
    JOIN c_bpartner cb ON cb.c_bpartner_id = ci.c_bpartner_id 
    WHERE ci.ad_client_id = $P{AD_CLIENT_ID}
      AND ci.ispaid = 'N'
      AND ci.c_currency_id = $P{C_Currency_ID}
    GROUP BY cb.value, cb.name
) AS k
LEFT JOIN c_currency cc ON cc.c_currency_id = $P{C_Currency_ID}
GROUP BY value, name, cc.iso_code
HAVING 
       (SUM(thu)<>0 and SUM(ban)<>0)  
       or (SUM(chi)<>0 and SUM(mua)<>0) 
       or (SUM(thu)-SUM(chi)<>0 and SUM(thu)<>0 and SUM(chi) <>0) 
       or (SUM(mua)-SUM(ban)<>0 and SUM(mua)<>0 and SUM(ban)<>0)
ORDER BY name;
]]>
	</queryString>
